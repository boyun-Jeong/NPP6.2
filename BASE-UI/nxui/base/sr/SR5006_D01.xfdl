        };
        Ex.core.alert(this, param);
		return false;
	}
	
	/*if(isReg && !this.grdList01.checkValidate(true) ) return false;
	return true;
	
	if(isReg && !this.grdList02.checkValidate(true) ) return false;
	return true;
	*/
	
	// 보존의뢰서
	var nRowCnt = this.dsSrProcdFileUseSub1.rowcount;
	
	for(i = 0; i < nRowCnt; i++)
	{
