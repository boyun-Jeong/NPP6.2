                    //trace("wfProcCd:[" + wfProcCd + "] objFormDiv.form.vscrollbar.max:[" + objFormDiv.form.vscrollbar.max + "]");

                    //trace("[SR0000_W]-this.wfProcForm() formHeight : " + formHeight + " Height : " + objFormDiv.form.getLayoutInfo("default", "height"));
