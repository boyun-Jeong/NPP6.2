// component onclick event. 
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
    //trace("[SR0000_W]-this.fnCommOnclick obj.id : " + obj.id);
    switch(obj.id)
    {
        case "btnSaveTemp" :    //임시저장 버튼
			this.fnSave(true);
            //trace("[SR0000_W]-this.fnCommOnclick baseWfProcCd : " + this.baseWfProcCd + " dsFormInfo WF_PROC_CD : " + this.dsFormInfo.getColumn(0, "WF_PROC_CD"));
            break;

        case "btnSave" :    //등록 버튼
			this.fnSave(false);
            break;

        case "btnRev" :     //접수(담당자) 버튼
        case "btnScrnRtn" : //반려(담당자) 버튼
			//alert("curWfProcCd : " + curWfProcCd + " curRow : " + curRow + " curScrnType : " + curScrnType);
            if(obj.id == "btnRev")          this.fnRev("REV");
            else if(obj.id == "btnScrnRtn") this.fnProcPop("RTRN"); //this.fnScrnRtn();
            break;

        case "btnApv" :     //승인(결재/검토) 버튼
        case "btnRtn" :     //반려(결재/검토) 버튼
			var gubun = "";
            if(obj.id == "btnApv")  gubun = "APV";
            else                    gubun = "RTRN";
            this.fnApvPop(gubun);
            break;

        case "btnDelete" :  //삭제 버튼
            this.fnDelete();
            break;

        case "btnCancel" :  //회수(취소) 버튼
            this.fnCancel();
            break;

        case "btnComment" :  //회의록 버튼
            this.fnCommentPop("btnComment");
            break;

		case "btnRelationId" :  //연관ID 버튼
            this.fnCommentPop("btnRelationId");
            break;
			
		case "btnChkList" :  //체크리스트 버튼
            this.fnCommentPop("btnChkList");
            break;
			
		case "btnUsrTest" :  //사용자리스트 버튼
            this.fnCommentPop("btnUsrTest");
            break;

		case "btnEnd" :
			this.fnProcPop("END");
			break;

		case "btnReserve" :
			var rtn = true;
			if(this.dsFormInfo.getColumn(0, "RESERV_FLAG") == "N")
			{
				rtn = this.fnReserveRegCheck();
			}
			
			if(rtn)
			{
				Ex.core.popup(
					this,					// <--- 팝업 실행 스코프
					"reservePop",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
					"sr::SR0010_P04.xfdl",	// <--- 팝업창 오픈 Url
					{
						pREQ_ID	: this.getOwnerFrame().pREQ_ID
					},									// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
					"autosize"							// <--- width/height/autosize/modeless(프레임 처리 옵션)
				);
			}
			break;

		case "btnAgreeCompDtChg" :
			var extYn = '';
			var popId	= 'CompDtChgPop';
			var url		= 'chm::CHM0001_P03.xfdl';
			oParam	= {
				title	: '합의완료일시 변경'
			  , data	: extYn	  
			  , pREQ_ID	: this.dsFormInfo.getColumn(0, "REQ_ID")
			  , pMAP_WF_PROC_CD	: this.dsFormInfo.getColumn(0, "MAP_WF_PROC_CD")
			  , pWF_PROC_CD	: this.dsFormInfo.getColumn(0, "WF_PROC_CD")
			};
			
			Ex.core.popup(
				this,
				popId,
				url,           		// <--- 팝업창 오픈 Url
				oParam,
				"autosize"
			);
			break;

        case "btnClose" :
            /* 닫기 전 변경 체크에 대한 로직 작성 필요 */
            Ex.core.pclose(this, this.getOwnerFrame().isSave ? "Y" : "N");
            break;
    }
}

