	this.fnSetchkAutoSaveControll("Y");
	
	if(curWfProcCd != "WF_PROC_9999")
	{
		//alert("reqStatCd : " + reqStatCd + " curWfProcCd : " + curWfProcCd + " gvAdminYn : " + Ex.util.getSession('gvAdminYn') + " PL_YN : " + this.dsFormInfo.getColumn(0,"PL_YN"));

		/* 등록단계일 경우 임시저장/반려 상태가 아니면 등록단계가 아니 경우 반려 상태가 아니면 */
		if( (curWfProcCd == "WF_PROC_1000" && !(reqStatCd == "RQST00" || reqStatCd == "RQST91")) ||
		    (curWfProcCd != "WF_PROC_1000" && reqStatCd != "RQST91") )
		{
			if(Ex.util.getSession('gvAdminYn') == "Y" || this.dsFormInfo.getColumn(0,"PL_YN") == "Y")
