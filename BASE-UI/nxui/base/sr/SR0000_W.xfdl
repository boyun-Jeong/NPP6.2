this.chkAutoSave_onchanged = function(obj:nexacro.CheckBox,e:nexacro.CheckBoxChangedEventInfo)
{
	if(e.postvalue == "Y")
	{
		if(	this.divBottomButton.form.btnSaveTemp.visible && !Ex.isEmpty(this.getOwnerFrame().pREQ_ID) &&
			this.dsAutoSaveSecond.getColumn(0, "USE_YN") == "Y" && this.dsFormInfo.getColumn(0, "WF_PROC_CD") == "WF_PROC_1000" &&
			this.dsFormInfo.getColumn(0, "REQ_STAT_CD") == "RQST00" && Ex.isNvl(this.dsFormInfo.getColumn(0, "RESERV_FLAG"), "N") == "N"
		  )
		{
			this.autoSaveSecond	= nexacro.toNumber(this.dsAutoSaveSecond.getColumn(0, "ITM_VAL_1"), 30);	//자동저장주기
			this.getOwnerFrame().isAutoSave = true;
			this.fnSetchkAutoSaveControll("Y");
		}
		else
		{
			this.getOwnerFrame().isAutoSave = false;
			this.fnSetchkAutoSaveControll();
		}
	}
	else
	{
		this.getOwnerFrame().isAutoSave = false;
		this.fnSetchkAutoSaveControll();
	}
};

this.formOnTimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	if(e.timerid == 1234)
	{
		var remainSec = nexacro.toNumber(this.divAutoInfo.form.stcRemain.text, 0);
		
		if(remainSec > 0)
		{
			remainSec = remainSec - 1;
			this.divAutoInfo.form.stcRemain.set_text(remainSec);
		}
		else
		{
			this.divAutoInfo.form.stcRemain.set_text(remainSec);

			if(	this.divBottomButton.form.btnSaveTemp.visible && !Ex.isEmpty(this.getOwnerFrame().pREQ_ID) &&
				this.dsAutoSaveSecond.getColumn(0, "USE_YN") == "Y" && this.dsFormInfo.getColumn(0, "WF_PROC_CD") == "WF_PROC_1000" &&
				this.dsFormInfo.getColumn(0, "REQ_STAT_CD") == "RQST00" && Ex.isNvl(this.dsFormInfo.getColumn(0, "RESERV_FLAG"), "N") == "N"
			)
			{
				var objComp = this.divForm.form.getFocus();
				
				if(objComp)
				{
					var strType = objComp.toString().toUpperCase();
					trace("strType ====> " + strType + " objComp.id : " + objComp.id);

					if(objComp.updateToDataset)
					{
						trace("[SR0000_W] " + objComp.id + " updateToDataset() value : " + objComp.value);
						var rtn = objComp.updateToDataset();
						trace("[SR0000_W] " + objComp.id + " updateToDataset() rtn : " + rtn);
					}
				}

				var chgCnt = 0;
				for(var i=0; i<this.gubunArr.length; i++)
				{					
					var isUpd = this.divForm.form[this.gubunArr[i][0]].form.isUpdateForm();
					trace("SR0000_W this.fnSave() i=" + i + " divNm : " + this.gubunArr[i][0] + " isUpdateForm() : " + isUpd);
					if(isUpd)
					{
						//trace("this.gubunArr() chgCnt++");
						if(this.gubunArr[i][0] == "divFormTop")
						{
							if( !Ex.isEmpty(this.divForm.form[this.gubunArr[i][0]].form.dsSrBaseInfo.getColumn(0, "TITLE")) )
							{
								chgCnt++;
							}
						}
						else
						{
							chgCnt++;
						}
					}
				}

				if(chgCnt == 0)
				{
					if(this.dsFormInfo.getColumn(0, "ATTACH_USE_YN") == "Y")
					{
						if(this.divForm.form["divFormFile"].form.isUpdateForm())
						{
							//trace("divFormFile chgCnt++");
							chgCnt++;
						}
					}
				}
				
				trace("this.SR0000_W_ontimer() chgCnt : " + chgCnt);
				
				if(chgCnt > 0)
				{
					this.fnSetchkAutoSaveControll();
					this.autoSaveYn = "Y";
					this.dsFormInfo.setColumn(0, "SAVE_GUBUN", "TEMP");
					this.dsFormInfo.setColumn(0, "TEMP_SAVE_YN", "Y");
					this.formDivUpladProc();    //Div Form별 FileUpload진행
				}
				else
				{
					this.fnSetchkAutoSaveControll("Y");
				}
			}
		}
	}
};

