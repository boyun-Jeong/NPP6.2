			{
				this.divBottomButton.form.btnAgreeCompDtChg.set_visible(true);
			}
		}
	}
	
	//예약 등록 버튼 처리
	if(this.dsFormInfo.getColumn(0, "REQ_ID").indexOf("REQ") == -1 && curWfProcCd == "WF_PROC_1000" && reqStatCd == "RQST00" && regBtnYn)
	{
		if(this.dsFormInfo.getColumn(0, "RESERV_PROC_YN") == "N")
		{
			if(this.dsFormInfo.getColumn(0, "RESERV_FLAG") == "Y")
			{
				this.divBottomButton.form.btnReserve.set_text("예약등록 중");
