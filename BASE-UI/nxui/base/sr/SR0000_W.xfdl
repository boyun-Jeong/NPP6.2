this.btn01_onclick = function(obj:nexacro.Button,e:nexacro.ClickEventInfo)
{
	for(var i=0; i<this.gubunArr.length; i++)
    {
		if(this.divForm.form[this.gubunArr[i][0]].border != "1px solid crimson")
		{
			if(!this.divForm.form["stc" + this.gubunArr[i][0]])
			{
				//Static.init( strName, vLeft, vTop , vWidth, vHeight [, vRight, vBottom, [vMinWidth, [vMaxWidth, [vMinHeight, [vMaxHeight]]]]] )
				var wfStatic = new Static("stc" + this.gubunArr[i][0], null, this.divForm.form[this.gubunArr[i][0]].top, 450, 24, 200, null);
				wfStatic.set_textAlign("right");
				var divScope = nexacro.replaceAll(this.gubunArr[i][0], "divForm", "").toUpperCase();
				wfStatic.set_text("단계 : [" + divScope + "]    URL:[" + this.divForm.form[this.gubunArr[i][0]].url + "]");
				this.divForm.addChild("stc" + this.gubunArr[i][0], wfStatic);
				wfStatic.show();
			}
			this.divForm.form["stc" + this.gubunArr[i][0]].set_visible(true);			
			this.divForm.form[this.gubunArr[i][0]].set_border("1px solid crimson");
		}
		else
		{
			this.divForm.form["stc" + this.gubunArr[i][0]].set_visible(false);
			this.divForm.form[this.gubunArr[i][0]].set_border("0px solid crimson");
		}
    }
};

