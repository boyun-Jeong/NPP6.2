                objFormDiv.set_url(wfFormUrl);
                //trace("[SR0000_W]-this.wfProcForm() "+ objFormDiv.id + " wfProcCd : " + wfProcCd + " objFormDiv.wfProcCd : " + objFormDiv.wfProcCd);

                var formHeight = objFormDiv.form.getLayoutInfo("default", "height");
