    var height;
    
    var popOption = "autosize,useCloseButton=false";
    
    /*
    if(gubun == "REV")
    {
        width = this.divForm.form["divForm" + curWfProcCd].form.getLayoutInfo("default", "width");
        height = this.divForm.form["divForm" + curWfProcCd].form.getLayoutInfo("default", "height");
        
        //popOption = "width=" + width + ",height=" + height + ",useCloseButton=false";
    }
    */
    //trace("REV " + curWfProcNm + " " + curWfProcCd + " width : " + width + " height : " + height);

    Ex.core.popup(
        this,                                                       // <--- 팝업 실행 스코프
        "srOpinionPop",                                             // <--- 팝업창 아이디(반드시 영문으로 입력 처리)
