this.fnSetchkAutoSaveControll = function(setValStr)
{
	trace("this.fnSetchkAutoSaveControll() pReqId : " + this.getOwnerFrame().pREQ_ID + "/" +
										 " USE_YN : " + this.dsAutoSaveSecond.getColumn(0, "USE_YN") + "/" +
										 " REQ_STAT_CD : " + this.dsFormInfo.getColumn(0, "REQ_STAT_CD"))
	this.isUseTimer = false;
	this.killTimer(1234);

	if(	this.divBottomButton.form.btnSaveTemp.visible && !Ex.isEmpty(this.getOwnerFrame().pREQ_ID) &&
		this.dsAutoSaveSecond.getColumn(0, "USE_YN") == "Y" && this.dsFormInfo.getColumn(0, "WF_PROC_CD") == "WF_PROC_1000" &&
		this.dsFormInfo.getColumn(0, "REQ_STAT_CD") == "RQST00" && Ex.isNvl(this.dsFormInfo.getColumn(0, "RESERV_FLAG"), "N") == "N"
	  )
	{
		this.divAutoInfo.set_visible(true);
		
		if( Ex.isEmpty(setValStr) )	setValStr = "N";

		if(this.getOwnerFrame().isAutoSave)
		{
			this.divAutoInfo.form.chkAutoSave.set_value(setValStr);

			if(setValStr == "Y")
			{
				this.divAutoInfo.form.stcRemain.set_text(this.autoSaveSecond||'');
				this.isUseTimer = true;
				this.setTimer(1234, 1000);
			}
		}
	}
	else
	{
		this.divAutoInfo.set_visible(false);
	}
	
	if(setValStr != "Y")
	{
		this.divAutoInfo.form.stcRemain.set_text("");
		this.divAutoInfo.form.chkAutoSave.set_value("N");
	}
};

