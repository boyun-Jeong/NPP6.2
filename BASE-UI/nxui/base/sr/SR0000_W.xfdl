    var fRow        = this.dsWfProc.findRow("WF_PROC_CD", curWfProcCd);
    var scrnPath    = "";
    var isEss       = false;

    var scrnPath    = "sr::SR0030_P01.xfdl";
    
    var isSaveTempBtn   = false;
    var saveTempBtnText = "";

    var isRevBtn        = false;
    var revBtnText      = "";
    
    var isRtrnBtn       = false;
    var rtrnBtnText = "";
	
	var isEndBtn		= false;

    if(gubun == "TEMP")
    {
        scrnPath    = Ex.isNvl(this.dsWfProc.getColumn(fRow, "URL")||'', "sr::SR0010_D02.xfdl");
        
        isRevBtn    = false;
        revBtnText  = "";

