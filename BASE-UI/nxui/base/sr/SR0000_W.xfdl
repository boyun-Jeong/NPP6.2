            }
            //trace("SR0000_W this.fnSave() btnSave i= " + i + " this.gubunArr : " + this.gubunArr[i][0] + " this.gubunArr[i][0].form.fn" rtn::: " + rtn);
        }
    }

//     if(isWfProcFocus)
//     {
//         for(var i=this.gubunArr.length-1; i>=0; i--)
