	if(fRow != -1)
	{
		var isExist = this.divForm.form.isValidObject("_printBtn");

		if(!isExist)
		{
			//var btnRComp = new Button("_printBtn", null, 0, 100, 24, 20, null);				
			var btnRComp = new Button("_printBtn", null, 0, 100, 24, 20, null);				
			this.divForm.form.addChild("_printBtn", btnRComp);
			btnRComp.set_cssclass("btn_WF_fileDown");
			//btnRComp.set_cssclass("btn_WF_primary_save_ex");
