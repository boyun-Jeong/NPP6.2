        saveTempBtnText = this.divBottomButton.form.btnSaveTemp.text;
    }   
    else if(gubun == "REV")
    {
        scrnPath    = Ex.isNvl(this.dsWfProc.getColumn(fRow, "URL")||'', "sr::SR0010_D02.xfdl");

        isRevBtn    = true;
        isEss       = Ex.isNvl(this.dsWfProc.getColumn(fRow, "SCREEN_APV_ESS_YN"), "N") == "Y" ? true : false;
        revBtnText  = this.divBottomButton.form.btnRev.text;

        isSaveTempBtn   = Ex.isNvl(this.dsFormInfo.getColumn(0,"SAVE_BTN_YN"), "N") == "Y" ? true : false;      
        saveTempBtnText = this.divBottomButton.form.btnSaveTemp.text;
    }
    else if(gubun == "RTRN")
