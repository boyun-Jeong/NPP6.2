{
	if(this.parent.editYn == "Y")
	{
		if(this.reqInfo.WF_FUNC_CD == "WF_FUNC_04" && (this.reqInfo.SAVE_BTN_YN == "Y" || this.reqInfo.REV_BTN_YN == "Y") )
		{
			Ex.util.setCompPropAll(this, this.reqInfo.PARENT_FORM.divForm.form["divFormWF_PROC_1000"], false);
			this.reqInfo.PARENT_FORM.divForm.form["divFormWF_PROC_1000"].form.divWebEditor.form.initCreate(true);
			
			if(this.reqInfo.PARENT_FORM.divForm.form["divFormWF_PROC_1000"].form.cboUpmu3)
			{
				this.reqInfo.PARENT_FORM.divForm.form["divFormWF_PROC_1000"].form.cboUpmu3.set_enable(true);
				this.reqInfo.PARENT_FORM.divForm.form["divFormWF_PROC_1000"].form.cboUpmu3.set_readonly(false);
			}
		}
	}
}

