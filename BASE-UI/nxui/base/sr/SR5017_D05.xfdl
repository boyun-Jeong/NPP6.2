this.fnSaveCheck = function(isReg)
{
	if( !Ex.util.checkValidate(this, "input01") ) return false;
	
	var selecToday = nexacro.toNumber(this.dsWfSrm.getColumn(0,"AGREE_COMP_DT"));
	
  	//if(this.reqInfo.WF_PROC_CD == "WF_PROC_2020"){
	if(this.reqInfo.WF_PROC_NM == "요청접수")	// 요청서마다 WF_PROC_CD 다를수있어서 이름으로 체크
	{
		if(selecToday <  nexacro.toNumber(Ex.util.today()))
		{
			var param = {
				  id : "calPlanDt"
				, msg	: "합의완료일시는 오늘날짜보다 크거나 같아야합니다."	//메시지 내용
				, arrparam : []						//메시지의 변수에 들어갈 실제값
				, msgtype : "E"						//메시지 타입 '' : [No icon]아이콘 없는 기본 alert, 'I' : [Info]일반 alert, 'W' : [Warning]경고 Alert, 'S' : [Success]성공 alert(저장 등에 사용)&#13;
			};
			Ex.core.alert(this, param);
			return false;
		}
		
		strValue = this.dsWfSrm.getColumn(0, "AGREE_COMP_TIME");
		if(strValue < '0000' || strValue > '2459')
		{
			var param = {
				  id : "mskPlanStime"
				, msg	: "합의완료시간을 확인하시기 바랍니다."	//메시지 내용
				, arrparam : []						//메시지의 변수에 들어갈 실제값
				, msgtype : "E"						//메시지 타입 '' : [No icon]아이콘 없는 기본 alert, 'I' : [Info]일반 alert, 'W' : [Warning]경고 Alert, 'S' : [Success]성공 alert(저장 등에 사용)&#13;
			};
			Ex.core.alert(this, param);
			return false;
		}
		
		if( Ex.isEmpty(this.dsWfSrm.getColumn(0,"WORK_USER_ID") ))
		{
			var param = {
				  id : "cboWorkUserNm"
				, msg	: "담당자를 확인하시기 바랍니다."	//메시지 내용
				, arrparam : []						//메시지의 변수에 들어갈 실제값
				, msgtype : "E"						//메시지 타입 '' : [No icon]아이콘 없는 기본 alert, 'I' : [Info]일반 alert, 'W' : [Warning]경고 Alert, 'S' : [Success]성공 alert(저장 등에 사용)&#13;
			};
			Ex.core.alert(this, param);
			return false;
		};

		// 외부직원은 현장대리인 승인단계 필수, 요청접수단계에서만 체크
		if(this.reqInfo.WF_PROC_NM == "요청접수" && this.divContent.form.cboSiteUserNm.enable && Ex.isEmpty(this.dsWfSrm.getColumn(0, "SITE_USER_ID")) )
		{
			var param = {
				  id : "cboSiteUserNm"
				, msg	: "현장대리인을 확인하시기 바랍니다."	//메시지 내용
				, arrparam : []						//메시지의 변수에 들어갈 실제값
				, msgtype : "E"						//메시지 타입 '' : [No icon]아이콘 없는 기본 alert, 'I' : [Info]일반 alert, 'W' : [Warning]경고 Alert, 'S' : [Success]성공 alert(저장 등에 사용)&#13;
			};
			Ex.core.alert(this, param);
			return false;
		}

		if( Ex.isEmpty(this.dsWfSrm.getColumn(0,"BA2_EXAM_OPIN")) )
		{
			var param = {
				  id : "txaOpinion"
				, msg	: "접수의견 및 수정방법을 작성하시기 바랍니다."	//메시지 내용
				, arrparam : []						//메시지의 변수에 들어갈 실제값
				, msgtype : "E"						//메시지 타입 '' : [No icon]아이콘 없는 기본 alert, 'I' : [Info]일반 alert, 'W' : [Warning]경고 Alert, 'S' : [Success]성공 alert(저장 등에 사용)&#13;
				};
			Ex.core.alert(this, param);
			return false;
		}
	}

	if(this.dsRevProc.rowcount == 0)
	{
		this.dsRevProc.setAddRow();
		this.dsRevProc.setColumn(0, "REQ_ID", this.reqInfo.REQ_ID);
		this.dsRevProc.setColumn(0, "WF_PROC_CD", this.reqInfo.WF_PROC_CD);
	}
	return true;
}

this.fnSetWorkUserId = function(workUserId)
{
	this.dsWfSrm.set_enableevent(false);
	this.dsWfSrm.setColumn(0, "WORK_USER_ID", workUserId);	// 담당자ID
	this.dsWfSrm.set_enableevent(true);

	if( Ex.isEmpty(workUserId) ) workUserId = "";

	if(workUserId.startsWith("P") || workUserId.startsWith("e"))
	{
		this.divContent.form.stcProcUserNm01.set_cssclass("sta_WF_inputTitle_E");
		this.divContent.form.cboSiteUserNm.set_enable(true);
	}
	else
	{
		this.divContent.form.stcProcUserNm01.set_cssclass("sta_WF_inputTitle");
		this.divContent.form.cboSiteUserNm.set_enable(false);
		this.dsWfSrm.setColumn(0, "SITE_USER_ID", undefined);
		this.divContent.form.cboSiteUserNm.set_index(0);
	}
}

this.fnSetSiteUserComp = function(siteUserId)
{
	this.dsWfSrm.set_enableevent(false);
	this.dsWfSrm.setColumn(0, "SITE_USER_ID", siteUserId);	// 현장대리인ID
	this.dsWfSrm.set_enableevent(true);
}

