        <Static id="stcBg01_00" taborder="7" left="20" top="87" height="34" cssclass="sta_WF_inputBg" right="20"/>
        <Static id="stcReqId01" taborder="8" left="20" top="87" width="130" height="34" cssclass="sta_WF_inputTitle_E" text="언어코드"/>
