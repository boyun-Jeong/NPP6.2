      <BindItem id="item1" compid="edtUserNm" propid="value" datasetid="dsFommUserMt" columnid="USER_NM"/>
      <BindItem id="item2" compid="cboLangCd" propid="value" datasetid="dsFommUserMt" columnid="LANG_CD"/>
    </Bind>
    <Objects>
