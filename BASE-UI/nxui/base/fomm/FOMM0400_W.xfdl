// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		upCmmCd : ['USE_CD',		'USE_CD'			]	// 조회할 상위코드
		,codeDiv: ['CMM_CD',		'CMM_CD'			]	// 조회할 코드의 업무구분
		,optStr	: ['ALL',			'SEL'				]	// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn	: ['Y',				'Y'					]	// 사용여부
		,filter	: ['',				''					]	// filterStr
		,objDs	: [this.dsUseYn,	this.dsUseYnGrid	]	// copy dataset
	};
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util
	
	var aRow = this.dsCommCd.addRow();
	this.dsCommCd.setColumn(aRow, "CODE", "01");
	this.dsCommCd.setColumn(aRow, "NAME", Ex.core.word("일반항목"));

	aRow = this.dsCommCd.addRow();
	this.dsCommCd.setColumn(aRow, "CODE", "02");
	this.dsCommCd.setColumn(aRow, "NAME", Ex.core.word("종합의견"));
};

this.fnCheckValidate = function()
{
	
	return true;
};

/***********************************************************************************
 * Component Event
 ***********************************************************************************/
// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	var pDivId = obj.parent.parent.parent.parent.id;

	switch(obj.name)
	{
		case 'btnSearch' :	// 조회
			this.fnSearchM();
			break;
			
		case 'btnSearchReset' :	// 조회조건 초기화
			this.fnSearchReset();
			break;
		
		case 'btnAdd' :
			if(pDivId == "divUp")
			{
				this.dsSatiQstItem.clearData();
				this.divDown.form.grdMainList.setCellProperty("Head", this.divDown.form.grdMainList.getBindCellIndex('Body', 'CHK'), "text", "0");
				var rowPos = this.dsSatiqstMng.setInsertRow(this.dsSatiqstMng.rowposition+1);
				this.divUp.form.grdMainList.showEditor(true);			// 그리드 편집모드로 변경
				this.divUp.form.grdMainList.setCellPos(3, rowPos);	// 그리드 셀 포커스 이동
				this.dsSatiqstMng.setColumn(nRow, "APP_DATE", Ex.util.today());
				this.dsSatiqstMng.setColumn(nRow, "USE_YN", 'Y');
			}
			else if(pDivId == "divDown")
			{
				var strSatiQstId = this.dsSatiqstMng.getColumn(this.dsSatiqstMng.rowposition, "SATI_QST_ID");
				
				var nRow = this.dsSatiQstItem.setInsertRow(this.dsSatiQstItem.rowposition+1);
				
				this.dsSatiQstItem.setColumn(nRow, "SATI_QST_ID", strSatiQstId);
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_TYPE", "01");
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_RSP1", Ex.core.word("매우만족"));
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_RSP2", Ex.core.word("만족"));
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_RSP3", Ex.core.word("보통"));
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_RSP4", Ex.core.word("불만족"));
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_RSP5", Ex.core.word("매우불만족"));
				
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_GRD1", "5");
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_GRD2", "4");
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_GRD3", "3");
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_GRD4", "2");
				this.dsSatiQstItem.setColumn(nRow, "QST_ITEM_GRD5", "1");

				this.divDown.form.grdMainList.showEditor(true);			// 그리드 편집모드로 변경
				this.divDown.form.grdMainList.setCellPos(3, nRow);	// 그리드 셀 포커스 이동
			}
			break;
			
		case 'btnDelete' :
			if(pDivId == "divUp")
			{
				this.fnDelete(this.dsSatiqstMng);
			}
			else if(pDivId == "divDown")
			{
				this.fnDelete(this.dsSatiQstItem);
			}
			break;

		case 'btnSave' : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
					  id : "saveConfirm"
					, msg : "10003"
					, arrparam : ['']
					, msgtype : "I"
				};
				Ex.core.confirm(this, param);
			}
			break;
	}
};

// 공통 onkeydown event : component
this.fnCommOnkeydown = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == '13')	this.fnSearch();
};

this.dsCanRowposChange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if( Ex.util.isUpdated(this.dsSatiQstItem) )
	{
		if( !this.rowChangeFlag ) {
			this.nSelRow = e.newrow;		// 이동하려는 행 저장
			var param = {	id : "rowposChangeConfirm"
					  , msg : "10001"
					  , arrparam : []
					  , msgtype : "I"};
			Ex.core.confirm(this, param);
			return false;
		}
	}
	return true;
};

this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	obj.dropdownCombo();
	obj.dropdownCalendar();
	obj.showEditor();
};

this.dsOnRowposChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	this.dsSatiQstItem.clearData();	

	var satiQstId = obj.getColumn(e.newrow, 'SATI_QST_ID') || '';

	// Grid title 처리
	//this.stcGrdTitle.set_text('[' + msgNo + '] ' + Ex.core.word("언어코드별 단어"));

	var rowFlag = obj.getColumn(e.newrow, "ROWFLAG");
	//trace("obj.id : " + obj.id + " e.newrow : " + e.newrow + " rowFlag : " + rowFlag);

	if(rowFlag != "I")
	{
		this.dsCond.setColumn(0, 'SEL_SATI_QST_ID', satiQstId);
		if(obj.getColumn(e.newrow, "ROWFLAG") != "D")	this.fnSearch02();
	}

	this.rowChangeFlag = false;		// 행변경 flag 초기화
	this.nSelRow = -1;				// 이동하려는 행 초기화
};

// 공통 oncloumnchanged event : dataset
this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(obj.name == "dsSatiQstItem") {
		if(e.columnid.indexOf("QST_ITEM_TYPE")!=-1) {		
			if(e.newvalue == '02') {
				for(var i = 5; i > 0;i--){				
					obj.setColumn(e.row, "QST_ITEM_GRD"+i, null);
					obj.setColumn(e.row, "QST_ITEM_RSP"+i, null);
				}
			}
		}
		
		if(e.columnid.indexOf("QST_ITEM_GRD")!=-1) {		
			if(e.newvalue == 'N') {							
				//obj.setColumn(e.row, e.columnid, null);				
			}
		}
	}

	obj.setUpdateRow(e);
};]]></Script>
