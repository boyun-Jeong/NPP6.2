this.fnPopupAfter = function(pID,varValue)
{
	switch(pID)
	{
		case "" :
			break;
	}
};

// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID,varValue)
{
	if(!varValue) return;
	
	switch(mID)
	{	
		case "searchConfirmM" :
			this.fnSearch01();
			break;

		case "searchConfirmD" :
			this.fnSearch02();
			break;
			
		case "saveConfirm" :
			this.fnSave();
			break;

		case "rowposChangeConfirm" :		// 행변경 confirm
			this.rowChangeFlag  = true;
			this.dsSatiqstMng.set_rowposition(this.nSelRow);
			break;
	}
};

