		case "btnDelete" : 
		
			if(Ex.isEmpty(this.dsAgency.getColumn(0,"USER_ID"))) {
				var param = {
					id : "failMsg"
				  , msg : "삭제할 업무위임 정보가 없습니다.."
				  , arrparam : ['']
				  , msgtype : "W"};
				Ex.core.alert(this, param);	
				return false;				
			}
		
			var param = {
 					id : "saveDelete"
 				  , msg : "업무위임 정보를 삭제하시겠습니까?"
 				  , arrparam : ['']
 				  , msgtype : "I"
 			};
 			Ex.core.confirm(this, param);
			break;
			
		case "btnStop" : 
