		this.dsAgency.copyRow(0, this.dsMain, e.row);
		
		this.divChange.form.divOutChangeUser.enable = false;
		this.divChange.form.divOutChangeAgencyUser.enable = false;
		this.divChange.form.calBeginDate.enable = false;
