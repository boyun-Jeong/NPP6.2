            <Layout>
              <Static id="Static00" taborder="3" text="이름" cssclass="sta_WF_schTitle" left="20" top="13" width="79" height="24" visible="false"/>
              <Edit id="edtSearchStr" taborder="0" left="67" top="13" width="140" height="24" autoselect="true" imemode="hangul" cssclass="edt_WF_normal" onkeyup="onKeyUp" visible="false"/>
              <Static id="Static06" taborder="4" text="W20" cssclass="Guide_Color" visible="false" left="0" top="13" width="20" height="24" background="#e5dbfa"/>
              <Button id="btnSearch" taborder="1" top="13" width="75" height="24" text="조회" right="20" cssclass="btn_WF_search" onclick="fnCommOnclick"/>
              <Button id="btnSearchReset" taborder="2" top="13" width="90" height="24" text="초기화" right="btnSearch:5" cssclass="btn_WF_searchReset" onclick="fnCommOnclick" visible="false"/>
            </Layout>
          </Layouts>
        </Div>
        <Static id="Static00_00" taborder="8" left="0" top="114" height="10" background="#e5dbfa" text="10" textAlign="center" verticalAlign="middle" font="normal 12pt/normal &quot;Arial&quot;" visible="false" right="0"/>
        <Grid id="grdUser" taborder="1" useinputpanel="false" left="20" top="Static00_00:0" binddataset="dsAttnd" autofittype="col" bottom="80" right="15" cssclass="grd_WF_normal">
          <Formats>
            <Format id="default">
              <Columns>
                <Column size="50" band="left"/>
                <Column size="120"/>
                <Column size="220"/>
                <Column size="48"/>
                <Column size="125"/>
              </Columns>
              <Rows>
                <Row size="32" band="head"/>
                <Row size="32"/>
              </Rows>
              <Band id="head">
                <Cell text="No"/>
                <Cell col="1" text="이름"/>
                <Cell col="2" text="부서"/>
