<?xml version="1.0" encoding="utf-8"?>
<FDL version="2.1">
  <Form id="FOMM0250_P01" left="0" top="0" width="600" height="514" titletext="회의 참석자 조회" onload="form_onload">
    <Layouts>
      <Layout width="600" height="514">
        <Div id="divSearch" taborder="0" left="20" top="20" height="50" scrollbars="none" cssclass="div_WF_divSearchArea" right="20">
