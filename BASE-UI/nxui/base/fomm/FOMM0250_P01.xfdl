			//trace("bfdate.getMinutes():" + bfdate.getMinutes().toString().padLeft(2,"0")  );
			
			var hourStr = bfdate.getHours().toString().padLeft(2,"0") ;
			var minStr = bfdate.getMinutes().toString().padLeft(2,"0") ;
			
			//trace("hourStr:" + hourStr + " minStr:" + minStr );
