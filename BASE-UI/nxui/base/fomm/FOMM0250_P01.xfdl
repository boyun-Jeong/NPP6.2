			this.divContents.form.grdMeetAttnd.setCellPos(cellIdx, this.grdValidRow);						// 그리드 셀 포커스 이동
			break;

		case "alertMeetTimeChkI" :
			this.divContents.form.mskMeetStime.setFocus();
			break;
			
		case "saveAlert":
			Ex.core.pclose(this, this.saveGbn);
			break;
	}
}

