/* 
 * sta_POP_msg_btnbg
 * popup 하단 button 영역 backgroun static css
 */
.Static.sta_POP_msg_btnbg
{
	background		: #ffffff ;
	-nexa-border		: 0px;
}

/* 
 * sta_POP_msg_conts
 * alert/confirm pop 메시지 내용 static css
 */
.Static.sta_POP_msg_conts
{
	background		: transparent;
	font			: bold 14px '돋움';
	color			: #637195;
}
/* 
 * txa_POP_msg_conts
 * alert/confirm pop 메시지 내용 text area css
 */
.TextArea.txa_POP_msg_conts,
.TextArea.txa_POP_msg_conts[status=readonly]
{
	background		: transparent;
	-nexa-border 		: 0px;
	-nexa-padding 		: 0px;
	-nexa-text-align 	: left;
	font			: bold 14px '돋움';
	color			: #637195;
}
/****************************************************************************************************************************/
/*****************************************************   STATIC END	*****************************************************/
/****************************************************************************************************************************/


/****************************************************************************************************************************/
/*****************************************************   DIV START   ********************************************************/
/****************************************************************************************************************************/
/* 
 * div_POP_msg_btnbg
 * SPP Popup bottom (divBottom) background css
 */
.Div.div_POP_msg_btnbg
{
	background		: #ffffff ;
	-nexa-border		: 1px solid #d0d1d6, 0px, 0px, 0px;
}

/****************************************************************************************************************************/
/*****************************************************   DIV END   **********************************************************/
/****************************************************************************************************************************/



/****************************************************************************************************************************/
/*****************************************************   CHILDFRAME START   *************************************************/
/****************************************************************************************************************************/
/* 
 * cf_FOMM_normal
 * SPP Popup titleBar css
 */
.ChildFrame.cf_FOMM_normal .titlebar
{
	font			: bold 14px '돋움';
	color 			: #ffffff;
	background 		: #3697F8;
	-nexa-padding 		: 0px 0px 0px 10px;
}

.ChildFrame.cf_FOMM_normal .titlebar .titleicon
{
	-nexa-icon		: url('theme://SPP_IMAGE_SET/FOMM/ico_FOMM_title.png');
	-nexa-text-padding 	: 0px 0px 0px 10px;
}

.ChildFrame.cf_FOMM_normal .titlebar .normalbutton
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleNormalN.png');
	cursor 			: pointer;
}

.ChildFrame.cf_FOMM_normal .titlebar .normalbutton[status=mouseover]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleNormalM.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .normalbutton[userstatus=pushed]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleNormalP.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .normalbutton[status=disabled]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleNormalD.png');
}

.ChildFrame.cf_FOMM_normal .titlebar .minbutton
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMinN.png');
	cursor 			: pointer;
}
.ChildFrame.cf_FOMM_normal .titlebar .minbutton[status=mouseover]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMinM.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .minbutton[userstatus=pushed]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMinP.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .minbutton[status=disabled]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMinD.png');
}

.ChildFrame.cf_FOMM_normal .titlebar .maxbutton
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMaxD.png');
	cursor 			: pointer;
}
.ChildFrame.cf_FOMM_normal .titlebar .maxbutton[status=mouseover]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMaxD.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .maxbutton[userstatus=pushed]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMaxD.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .maxbutton[status=disabled]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleMaxD.png');
}

.ChildFrame.cf_FOMM_normal .titlebar .closebutton
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleCloseN.png');
	cursor 			: pointer;
}
.ChildFrame.cf_FOMM_normal .titlebar .closebutton[status=mouseover]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleCloseM.png');
}
.ChildFrame.cf_FOMM_normal .titlebar .closebutton[userstatus=pushed]
{
	-nexa-icon 		: URL('theme://SPP_IMAGE_SET/FOMM/btn_FOMM_titleCloseP.png');
