/* 
 * sta_TOAST_normal
 * toast message Type normal
 */
.Static.sta_TOAST_normal
{
	background		: #ffffff URL('theme://SPP_IMAGE_SET/FOMM/bg_FOMM_toastInfo.png') no-repeat 10px center;
	-nexa-border		: 1px solid #7db1f1;
	-nexa-padding		: 0px 0px 0px 30px;
	font			: 14px '돋움';
	box-shadow		: 0px 2px 4px #808080;
}
/* 
 * sta_TOAST_success
 * toast message Type success
 */
.Static.sta_TOAST_success
{
	background		: #ffffff URL('theme://SPP_IMAGE_SET/FOMM/bg_FOMM_toastSuccess.png') no-repeat 10px center;
	-nexa-border		: 1px solid #7ef2a1;
	-nexa-padding		: 0px 0px 0px 30px;
	font			: 14px '돋움';
	box-shadow		: 0px 2px 4px #808080;
}
/* 
 * sta_TOAST_error
 * toast message Type error
 */
