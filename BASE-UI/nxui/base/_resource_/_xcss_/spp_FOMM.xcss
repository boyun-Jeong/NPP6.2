	-nexa-padding		: 0px 0px 0px 30px;
	font			: 14px '돋움';
	box-shadow		: 0px 2px 4px #808080;
}


/* 
 * sta_POP_title
 * popup title background type none
 */
.Static.sta_POP_title
{
	background		: #ffffff;
	-nexa-border		: 0px;
	-nexa-padding		: 2px 0px 0px 20px;
	font			: bold 18px '돋움';
	color			: #3556ba;
}
/* 
 * sta_POP_msg_title
 * message popup title background type none
 */
.Static.sta_POP_msg_title
{
	background		: #ffffff;
	-nexa-border		: 0px;
	-nexa-padding		: 2px 0px 0px 20px;
	font			: bold 18px '돋움';
	color			: #3556ba;
}
/* 
