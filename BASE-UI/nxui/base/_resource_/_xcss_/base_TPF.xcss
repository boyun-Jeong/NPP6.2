 * Top frame 메시지 버튼 (기본)
 */
.Button.btn_TPF_message,
.Button.btn_TPF_message[status=focused],
.Button.btn_TPF_message[status=mouseover],
.Button.btn_TPF_message[userstatus=pushed],
.Button.btn_TPF_message[userstatus=selected]
{
	background		: url('theme://SPP_IMAGE_SET/TPF/ico_TPF_Message.png') no-repeat center center;
	-nexa-border		: 0px;
	cursor			: pointer; 
	-nexa-padding		: 0px 0px 0px 0px;
}


/* 
 * btn_TPF_messageWR
 * Top frame 메시지 버튼 (쓰기)
 */
.Button.btn_TPF_messageWR,
.Button.btn_TPF_messageWR[status=focused],
.Button.btn_TPF_messageWR[status=mouseover],
