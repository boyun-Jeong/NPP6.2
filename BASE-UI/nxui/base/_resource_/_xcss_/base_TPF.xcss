}
.Button.btn_TPF_hyperlink[status=mouseover],
.Button.btn_TPF_hyperlink[userstatus=pushed],
.Button.btn_TPF_hyperlink[userstatus=selected]
{
	background		: url('theme://SPP_IMAGE_SET/TPF/ico_TPF_hyperlink.png') no-repeat right center;
	-nexa-border		: 0px;
	-nexa-text-align	: left;
	-nexa-vertical-align	: middle;
	color			: #444444;
	cursor			: pointer;
	font 			: 14px "KoPubL";
	-nexa-padding		: 0px 25px 0px 0px;
	-nexa-text-decoration 	: underline;
}

/* 
