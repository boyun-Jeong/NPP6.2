    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ CH0600_D02
* 화면(명)		︰ 검수 요청서 - 운영
* 메뉴(경로)		︰ 사업계획/계약 > 계약관리 > 계약검수등록(CT0140_W)
* 화면 설명		︰ 검수 요청서
* 작성자			︰ WEMB
* 작성일			︰ 2024.06.28 
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2024.06.28 	 WEMB			최초작성
*------------------------------------------------------------------
***********************************************************************************/
/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_WorkFlow.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.pPopType = (this.getOwnerFrame().pPopType||'').toUpperCase();    // WF: 요청서 / R: 조회
this.PRE_REQ_ID = this.reqInfo.PRE_REQ_ID;
this.PAY_RPOS = 0;
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{		
	// 전처리 SELECT 를 위한 dsCond 설정		
	this.dsCond.setColumn(0, "PRE_REQ_ID", this.PRE_REQ_ID);	
	this.gfnFormOnLoad(this, this.fnInit);
			
	this.dsCond.setColumn(0, "REQ_ID", this.reqInfo.REQ_ID);
	
	if( Ex.isEmpty(this.wfDsReqWfProc) )
	{
		this.wfdsReqWfProc = new Dataset();
	}
	this.wfdsReqWfProc.loadXML(this.dsWfProc.saveXML());	
}

this.fnInit = function()
{
	// 폼 로드시 필요한 기술사항은 this.fnInit에서 기술할것	
	
	this.fnCompInit();		// 컴포넌트 초기화
	this.fn_initDataset();
}
// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		upCmmCd : [		'CURR_CD', 		'PAY_CYCLE_CD', 	]	// 조회할 상위코드
		,codeDiv: [		'CNTR_CD',		'CNTR_CD',			]			// 조회할 코드의 업무구분
		,optStr	: [		'SEL',			'SEL',				]				// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn	: [		'Y',			'Y',				]					// 사용여부
		,filter	: [		'', 			'', 				]						// filterStr
		,objDs	: [		this.dsCurrCd,	this.dsPayCycleCd,	]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);

	//this.dsAttatchPayCd.assign(this.dsAttatchCntcCd);
	//this.dsAttatchCntcCd.set_filterstr(" ITM_VAL_1=='CNTC' ");
	//this.dsAttatchPayCd.set_filterstr("ITM_VAL_1=='PAY'");
}

this.fn_initDataset = function()
{	

}
