/***********************************************************************************
 * User Function
 ***********************************************************************************/
this.fnCompInit = function()
{
	this.grdList.displayRowType();
	
	// 공통코드 조회
	this.fnGetCmmCd();
	
	// 사업년도 ( -10년/ +6년 )
	var CNTR_YY_ST = Ex.util.today().substring(0, 4);
	for(var i=0, j=6; i<6; i++){
		this.dsCntrYY.addRow();
		this.dsCntrYY.setColumn(i,"cntrYY",nexacro.toNumber(CNTR_YY_ST)+(j--));
	}
	for(var i=0, j=this.dsCntrYY.rowcount;i<=10;i++){
		this.dsCntrYY.addRow();
		this.dsCntrYY.setColumn((j++),"cntrYY",nexacro.toNumber(CNTR_YY_ST)-i);
	}
	for(var i=0; i<12; i++){
		this.dsCntrMM.addRow();
		if(i <= 10 )
			this.dsCntrMM.setColumn(i,"cntrMM",'0'+(i+1));
		else 	this.dsCntrMM.setColumn(i,"cntrMM",i+1);
	}
	
	if(this.pPopType == 'WF')
	{	
		// 요청서 기본정보 세팅
		var strTitle = '계약서 검수확인 ( ' + this.dsCtCntrMt.getColumn(0,'TITLE') +')';
		this.reqInfo.PARENT_FORM.topForm.dsSrBaseInfo.setColumn(0,'TITLE',strTitle );
		this.reqInfo.PARENT_FORM.topForm.stcReqTitle.set_text("투자예산 검수");
		
		// 대금 지급 정보 (dsSrReqCtCntrPay) 진행 값 세팅
		this.dsSrReqCtCntrPay.setColumn(0,"BUSI_MNG_NO", this.dsCtCntrMt.getColumn(0, 'BUSI_NO'));
	}
	
	var oParam = {
		 sSvcId		: 'fileSvc01'		// divFile callback 후처리용 서비스 ID
		,title		: '검수확인서'
		,limitSize	: 100				// 첨부파일 개당 용량 제한(단위 MB); default 100 MB
		,folderName	: 'ct'				// 폴더명(업무명)
		,tableName	: 'SR_REQ_CT_PAY_INSPT_OP'	// 구분명(ex. 화면명 + "_" + 첨부파일 Div ID)
		,primaryKey1: this.reqInfo.REQ_ID||''	// 테이블 PK1(ex. 요청서ID 등)
		,primaryKey2: ''				// 테이블 PK2 (복합 키인 경우 사용)
		,primaryKey3: ''				// 테이블 PK3 (복합 키인 경우 사용)
		,extGrid	: ''				// 확장컬럼 입력을 위한 format용 grid
		,fileDownYn	: 'Y'				// 파일다운권한; 	공백 시 메뉴의 조회 권한
		,fileUpYn	: 'Y'				// 파일업로드권한; 	공백 시 메뉴의 추가 권한
		,deleteYn	: 'Y'				// 파일삭제권한; 	공백 시 메뉴의 삭제 권한
	};
	this.divDown.form.divFile1.form.setConfig(this, oParam);

	Ex.util.callParentSizeChangeComplete(this, "fnLoadComplete", this.reqInfo.SCRN_WF_PROC_CD);
}

