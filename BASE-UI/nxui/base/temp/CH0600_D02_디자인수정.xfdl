/***********************************************************************************
 * Common Function
 ***********************************************************************************/

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");

	for(var i = objDs.rowcount-1; i >= 0; i--) objDs.setDeleteRow(i);

	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
	
	switch (sSvcId)
	{
		case "select01" :
						
			break;
		
		case "preSelect" :
			if( Ex.isEmpty(this.dsCond.getColumn(0, "REQ_ID")) )
			{
				this.dsSrReqCtPayInsptOp.setInsertRow();
				// 검수확인 (dsSrReqCtPayInsptOp) 기본값 세팅
				this.dsSrReqCtPayInsptOp.setColumn(0,'CNTC_NO',this.dsCtCntrMt.getColumn(0,'CNTC_NO'));
				trace(this.dsSrReqCtPayInsptOp.getColumn(0,'CNTC_NO')+">>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>>");
				this.dsSrReqCtPayInsptOp.setColumn(0,'PA_SEQ',0);
				this.dsSrReqCtPayInsptOp.setColumn(0,"BUSI_MNG_NO", this.dsCtCntrMt.getColumn(0, 'BUSI_NO'));
				this.dsSrReqCtPayInsptOp.setColumn(0,'PAY_DATE',Ex.util.today());
				
				this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_CHRG_ID',Ex.util.getSession('gvUserId')); //검수담당자
				this.dsSrReqCtPayInsptOp.addColumn('CONF_CHRG_NM','STRING');
				this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_CHRG_NM',Ex.util.getSession('gvUserNm')); 
				this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_CHRG_DEPT_ID',Ex.util.getSession('gvDeptCd')); //검수담당자부서
				this.dsSrReqCtPayInsptOp.addColumn('CONF_CHRG_DEPT_NM','STRING');
				this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_CHRG_DEPT_NM',Ex.util.getSession('gvDeptNm')); 
				this.dsSrReqCtPayInsptOp.setColumn(0,'CURR_CD',"KRW");				
				this.dsSrReqCtPayInsptOp.addColumn('CONF_START_Y','STRING');
				this.dsSrReqCtPayInsptOp.addColumn('CONF_START_M','STRING');
				this.dsSrReqCtPayInsptOp.addColumn('CONF_END_Y','STRING');
				this.dsSrReqCtPayInsptOp.addColumn('CONF_END_M','STRING');
				
				if (Ex.isEmpty(this.dsSrReqCtPayInsptOp.getColumn(0,'CONF_START_YM')))
				{
					this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_START_Y',this.dsCtCntrMt.getColumn(0,'CNTC_START_DATE').substring(0,4));
					this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_END_Y',this.dsCtCntrMt.getColumn(0,'CNTC_END_DATE').substring(0,4));
					this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_START_M',this.dsCtCntrMt.getColumn(0,'CNTC_START_DATE').substring(4,6));
					this.dsSrReqCtPayInsptOp.setColumn(0,'CONF_END_M',this.dsCtCntrMt.getColumn(0,'CNTC_END_DATE').substring(4,6));
				}	
			}
			break;		
			
	}
}

// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID,varValue)
{
	// TO DO
	// 개발자가 지정한 pID(팝업창ID) , varValue(팝업창 반환값)
