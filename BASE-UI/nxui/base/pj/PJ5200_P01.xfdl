// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd	: ['DFT_TYPE_CD',		'DFT_REV_STS_CD',	'DFT_PRC_STS_CD']	// 조회할 상위코드
		,codeDiv	: ['PJT_MNG_CD',		'PJT_MNG_CD',		'PJT_MNG_CD']		// 조회할 코드의 업무구분
		,optStr		: ['SEL',				'SEL',				'SEL']				// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn		: ['Y',					'Y',				'Y']				// 사용여부
		,filter		: ['',					'',					'']					// filterStr
		,objDs		: [this.dsUtdftTypeCd,	this.dsUtdftRcvCd,	this.dsUtdftActCd]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsUtDefect) && !this.divContents.form.divFiles.form.isUpdateForm())
	{
		return false;
	}

	if( this.dsUtDefect.getColumn(0, "UTDFT_RCV_CD") == "DFT_REV_STS_CD02" &&
		Ex.isEmpty(this.dsUtDefect.getColumn(0, "UTDFT_RCV_DESC")) )
	{
		var oParam = {id : "alertChkRcvCd", msg : "접수상태가 미수용인경우 접수의견은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	
	var msgNo;

	//성명 입력 체크
	var rtnVal = this.divContents.form.divDftUser.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divDevUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertDftUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcDftUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	
	/*
	if( Ex.isEmpty(this.dsUtDefect.getColumn(0, "UTEST_ID")) )
	{
		var oParam = {id : "alertEssUtestIdI", msg : "11001", arrparam : [Ex.core.word(this.divContents.form.stcUtest.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	*/
	
	return true;
} 

