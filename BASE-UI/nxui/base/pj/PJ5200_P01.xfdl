    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ5200_P01
* 화면(명)		︰ 단위결함 상세
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.05
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.05	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/
/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_PjComm.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ5200_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ5200_P01] this.fnSearch()");
	var sTranId = "select01";									// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ5200P/select01";							// 서비스명
    var sInDs = "dsCond=dsCond";								// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsUtDefect=dsUtDefect";						// 서버에서 수신할 데이타셋
    var sArg = "";												// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ5200_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ5200P/save01";
    var sInDs = "dsCond=dsCond dsUtDefect=dsUtDefect:U";
	var sOutDs = "dsUtDefect=dsUtDefect dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ5200_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);

	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "fileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			
			var strDftUserId    = this.dsUtDefect.getColumn(0, "DFT_USER_ID");
			var strDftUserNm    = this.dsUtDefect.getColumn(0, "DFT_USER_NM");
			var strDftDept      = this.dsUtDefect.getColumn(0, "DFT_USER_DEPT");
			this.divContents.form.divDftUser.form.setValue(strDftUserId, strDftUserNm, strDftDept);
			
			var strActUserId    = this.dsUtDefect.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsUtDefect.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsUtDefect.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			var strConfUserId    = this.dsUtDefect.getColumn(0, "CONF_USER_ID");
			var strConfUserNm    = this.dsUtDefect.getColumn(0, "CONF_USER_NM");
			var strConfDept      = this.dsUtDefect.getColumn(0, "CONF_USER_DEPT");
			this.divContents.form.divConfUser.form.setValue(strConfUserId, strConfUserNm, strConfDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "UTDFT_ID")) )
			{
				this.getOwnerFrame().pUtdftId	= this.dsCond.getColumn(0, "UTDFT_ID");
				this.getOwnerFrame().pMode = "SEARCH";
				this.divContents.form.divFiles.form.fnReplaceTempkeyAndSearch(this.dsCond.getColumn(0, "UTDFT_ID"));
			}
			
			var strDftUserId    = this.dsUtDefect.getColumn(0, "DFT_USER_ID");
			var strDftUserNm    = this.dsUtDefect.getColumn(0, "DFT_USER_NM");
			var strDftDept      = this.dsUtDefect.getColumn(0, "DFT_USER_DEPT");
			this.divContents.form.divDftUser.form.setValue(strDftUserId, strDftUserNm, strDftDept);
			
			var strActUserId    = this.dsUtDefect.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsUtDefect.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsUtDefect.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			var strConfUserId    = this.dsUtDefect.getColumn(0, "CONF_USER_ID");
			var strConfUserNm    = this.dsUtDefect.getColumn(0, "CONF_USER_NM");
			var strConfDept      = this.dsUtDefect.getColumn(0, "CONF_USER_DEPT");
			this.divContents.form.divConfUser.form.setValue(strConfUserId, strConfUserNm, strConfDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;
	}
}
