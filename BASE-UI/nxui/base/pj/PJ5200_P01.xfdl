// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchUtest" :	//단위테스트 돋보기 버튼
			if( Ex.isEmpty((this.dsUtDefect.getColumn(0, "P_PJT_ID"))) )
			{
				var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}

			if( Ex.isEmpty((this.dsUtDefect.getColumn(0, "PJT_ID"))) )
			{
				var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if( Ex.isEmpty((this.dsUtDefect.getColumn(0, "BIZ_ID"))) )
			{
				var oParam = {id : "alertNoSelBizIdI", msg : "업무를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			var strPjtId    = this.dsUtDefect.getColumn(0, "P_PJT_ID");
			var strPjtNm	= this.dsUtDefect.getColumn(0, "P_PJT_NAME");
			var strSubPjtId	= this.dsUtDefect.getColumn(0, "PJT_ID");
			var strSubPjtNm	= this.dsUtDefect.getColumn(0, "PJT_NAME");
			var strBizId	= this.dsUtDefect.getColumn(0, "BIZ_ID");
			var strRefNo	= this.dsUtDefect.getColumn(0, "UTEST_ID");
			
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ5020_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ5020_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title			: Ex.core.word("단위 테스트 검색"),
					pSelType		: "S",
					pPjtId			: strPjtId,
					pPjtNm			: strPjtNm,
					pSubPjtId		: strSubPjtId,
					pBizId			: strBizId,
					pRefNo			: strRefNo,
					pPjtEnable		: false,
					pSubPjtEnable	: false,
					pBizEnable		: false
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
			break;

		case "btnSave" : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

