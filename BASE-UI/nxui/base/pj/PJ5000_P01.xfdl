/**
 * 테스트대상 팝업 오픈
 * @param  {string} 대상 ID
 * @param  {string} 대상 URL
 * @param  {dataset} result dataset
 * @return : N/A
 */
this.fn_openPopupPrg = function(strId)
{
	var strTitle = "";
	var popUrl = "";

	if(strId == "searchScreen")			strTitle = Ex.core.word("화면 목록 검색");
	else if(strId == "searchReport")	strTitle = Ex.core.word("보고서 목록 검색");
	else if(strId == "searchInterface")	strTitle = Ex.core.word("인터페이스 목록 검색");
	else if(strId == "searchBatchPrg")	strTitle = Ex.core.word("배치프로그램 목록 검색");
	else if(strId == "searchProgram")	strTitle = Ex.core.word("프로그램 목록 검색");

	if(strId == "searchScreen")			popUrl = "pj::PJ4220_P01.xfdl";
	else if(strId == "searchReport")	popUrl = "pj::PJ4720_P01.xfdl";
	else if(strId == "searchInterface")	popUrl = "pj::PJ4320_P01.xfdl";
	else if(strId == "searchBatchPrg")	popUrl = "pj::PJ4520_P01.xfdl";
	else if(strId == "searchProgram")	popUrl = "pj::PJ4620_P01.xfdl";

	if( !Ex.isEmpty(popUrl) )
	{
		var strPjtId    = this.dsUnitTest.getColumn(0, "P_PJT_ID");
		var strPjtNm	= this.dsUnitTest.getColumn(0, "P_PJT_NAME");
		var strSubPjtId	= this.dsUnitTest.getColumn(0, "PJT_ID");
		var strSubPjtNm	= this.dsUnitTest.getColumn(0, "PJT_NAME");
		var strBizId	= this.dsUnitTest.getColumn(0, "BIZ_ID");
		var strRefNo	= this.dsUnitTest.getColumn(0, "PRG_REF_NO");

		Ex.core.popup(
			this,							// <--- 팝업 실행 스코프
			strId,							// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			popUrl,							// <--- 팝업창 오픈 Url
			{
				title			: strTitle,
				pSelType		: "S",
				pPjtId			: strPjtId,
				pPjtNm			: strPjtNm,
				pSubPjtId		: strSubPjtId,
				pBizId			: strBizId,
				pRefNo			: strRefNo,
				pPjtEnable		: false,
				pSubPjtEnable	: false
			},								// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)		
			"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
}

this.fn_setPrgRef = function (colPrefix:String)
{
	if(this.dsPopRtn.rowcount > 0 )
	{
		trace("this.fn_setPrgRef()\n" + this.dsPopRtn.saveXML());
		
		// 대상 프로그램 설정
		var prgRefId   = this.dsPopRtn.getColumn(0, (colPrefix + "_ID"));
		var prgRefNo   = this.dsPopRtn.getColumn(0, (colPrefix + "_NO"));
		var prgRefName = this.dsPopRtn.getColumn(0, (colPrefix + "_NAME"));

		trace(colPrefix + "/" + prgRefId + "/" + prgRefNo + "/" + prgRefName);

		this.dsUnitTest.setColumn(0, "PRG_REF_ID",   prgRefId);
		this.dsUnitTest.setColumn(0, "PRG_REF_NO",   prgRefNo);
		this.dsUnitTest.setColumn(0, "PRG_REF_NAME", prgRefName);

		// 대상 프로그램 개발자 설정
		var strDevUserId   = this.dsPopRtn.getColumn(0, "DEV_USER_ID");
		var strDevUserNm   = this.dsPopRtn.getColumn(0, "DEV_USER_NM");
		var strDevUserDept = this.dsPopRtn.getColumn(0, "DEV_USER_DEPT");

		this.dsUnitTest.setColumn(0, "DEV_USER_ID", strDevUserId);
		this.dsUnitTest.setColumn(0, "DEV_USER_NM", strDevUserNm);
		this.dsUnitTest.setColumn(0, "DEV_USER_DEPT", strDevUserDept);
		
		this.divContents.form.divDevUser.form.setValue(strDevUserId, strDevUserNm, strDevUserDept);

		this.dsPopRtn.clearData();
	}
}

