    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ5000_P01
* 화면(명)		︰ 단위테스트 상세
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.11.07
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.11.07	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_PjComm.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ5000_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ5000_P01] this.fnSearch()");
	var sTranId = "select01";									// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ5000P/select01";							// 서비스명
    var sInDs = "dsCond=dsCond";								// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsUnitTest=dsUnitTest dsUtDefect=dsUtDefect";		// 서버에서 수신할 데이타셋
    var sArg = "";												// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ5000_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ5000P/save01";
    var sInDs = "dsCond=dsCond dsUnitTest=dsUnitTest:U dsUtDefect=dsUtDefect:U";
	var sOutDs = "dsUnitTest=dsUnitTest dsUtDefect=dsUtDefect dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ5000_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);
	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "srFileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");

			var strDevUserId    = this.dsUnitTest.getColumn(0, "DEV_USER_ID");
			var strDevUserNm    = this.dsUnitTest.getColumn(0, "DEV_USER_NM");
			var strDevDept      = this.dsUnitTest.getColumn(0, "DEV_USER_DEPT");
			this.divContents.form.divDevUser.form.setValue(strDevUserId, strDevUserNm, strDevDept);

			var strTesterId    = this.dsUnitTest.getColumn(0, "TESTER_ID");
			var strTesterNm    = this.dsUnitTest.getColumn(0, "TESTER_NM");
			var strTesterDept      = this.dsUnitTest.getColumn(0, "TESTER_DEPT");
			this.divContents.form.divTester.form.setValue(strTesterId, strTesterNm, strTesterDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "UTEST_ID")) )
			{
				this.getOwnerFrame().pUtestId	= this.dsCond.getColumn(0, "UTEST_ID");
				this.getOwnerFrame().pMode = "SEARCH";
			}
			
			this.divContents.form.btnSearchPjtPop.set_enable(false);
			this.divContents.form.cboSubProject.set_enable(false);
			
			var strDevUserId    = this.dsUnitTest.getColumn(0, "DEV_USER_ID");
			var strDevUserNm    = this.dsUnitTest.getColumn(0, "DEV_USER_NM");
			var strDevDept      = this.dsUnitTest.getColumn(0, "DEV_USER_DEPT");
			this.divContents.form.divDevUser.form.setValue(strDevUserId, strDevUserNm, strDevDept);

			var strTesterId    = this.dsUnitTest.getColumn(0, "TESTER_ID");
			var strTesterNm    = this.dsUnitTest.getColumn(0, "TESTER_NM");
			var strTesterDept      = this.dsUnitTest.getColumn(0, "TESTER_DEPT");
			this.divContents.form.divTester.form.setValue(strTesterId, strTesterNm, strTesterDept);

			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;
	}
}
// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "PJ0100_P01" :
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)	this.dsPopRtn.loadXML(varValue);
			this.dsUnitTest.setColumn(0, "P_PJT_ID", this.dsPopRtn.getColumn(0, "PJT_ID"));
			this.dsUnitTest.setColumn(0, "P_PJT_NAME", this.dsPopRtn.getColumn(0, "PJT_NAME"));
			this.fnSetSubPjtCombo();
			break;

		case "searchScreen" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.fn_setPrgRef("SCR");
			}
			break;

		case "searchBatchPrg" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.fn_setPrgRef("BTPR");
			}
			break;

		case "searchInterface" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.fn_setPrgRef("INF");
			}
			break;

		case "searchReport" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.fn_setPrgRef("RPPR");
			}
			break;

		case "searchProgram" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.fn_setPrgRef("PRG");
			}
			break;
	}
}
