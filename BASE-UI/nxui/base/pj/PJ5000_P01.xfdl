this.fn_prgRefOnClick = function()
{
	var sRefType = this.dsUnitTest.getColumn(0, "PRG_REF_CD");

	if( Ex.isEmpty(sRefType) )
	{
		var oParam = {id : "alertNoSelPrgRefCdI", msg : "테스트 대상을 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	
	//trace(sRefType);
	switch(sRefType)
	{
  		case "TEST_TARG_CD01" :
			this.fn_openPopupPrg("searchScreen");
 			break;
 		case "TEST_TARG_CD02" :
			this.fn_openPopupPrg("searchReport");
 			break;
 		case "TEST_TARG_CD03" :
			this.fn_openPopupPrg("searchInterface");
 			break;
 		case "TEST_TARG_CD04" :
			this.fn_openPopupPrg("searchBatchPrg");
 			break;
 		case "TEST_TARG_CD05" :
			this.fn_openPopupPrg("searchProgram");
 			break; 			
		default :
