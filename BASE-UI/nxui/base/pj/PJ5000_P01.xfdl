// component init
this.fnCompInit = function()
{
	this.dsCond.setColumn(0, "UTEST_ID", this.getOwnerFrame().pUtestId);

	// Grid 행상태(ROWFLAG) 이미지 처리 util 호출
	this.divContents.form.grdUtDefect.displayRowType();

	this.divContents.form.edtPrgRefNo.set_displaynulltext(Ex.core.word("프로그램ID"));
	this.divContents.form.edtPrgRefName.set_displaynulltext(Ex.core.word("프로그램명"));

	// 공통코드 조회
	this.fnGetCmmCd();

	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divBtnBottom.form.btnSave];	// 저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);

	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divBtnBottom, 'right', true);

	if(this.getOwnerFrame().pMode == "NEW")
	{
		var nRow = this.dsUnitTest.setInsertRow();
		this.dsUnitTest.setColumn(nRow, "P_PJT_ID",   this.getOwnerFrame().pPjtId);
		this.dsUnitTest.setColumn(nRow, "P_PJT_NAME", this.getOwnerFrame().pPjtNm);
		this.dsUnitTest.setColumn(nRow, "PJT_ID", this.getOwnerFrame().pSubPjtId);
		this.divContents.form.cboBizId.setFocus();
		this.fnSetSubPjtCombo();	//업무 콤보 셋팅
	}
	else if(this.getOwnerFrame().pMode == "SEARCH")
	{
		this.divContents.form.btnSearchPjtPop.set_enable(false);
		this.divContents.form.cboSubProject.set_enable(false);

		this.fnSearch();
	}
}

// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd	: ['TEST_TYP_CD',		'TEST_TARG_CD',		'TEST_RST_CD',		'DFT_REV_STS_CD',	'DFT_PRC_STS_CD']	// 조회할 상위코드
		,codeDiv	: ['PJT_MNG_CD',		'PJT_MNG_CD',		'PJT_MNG_CD',		'PJT_MNG_CD',		'PJT_MNG_CD']		// 조회할 코드의 업무구분
		,optStr		: ['SEL',				'SEL',				'SEL',				'',					'']					// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn		: ['Y',					'Y',				'Y',				'Y',				'Y']				// 사용여부
		,filter		: ['',					'',					'',					'',					'']					// filterStr
		,objDs		: [this.dsUtestTypeCd,	this.dsPrgRefCd,	this.dsResultCd,	this.dsUtdftRcvCd,	this.dsUtdftActCd]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	if( Ex.isEmpty((this.dsUnitTest.getColumn(0, "P_PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}

	if( Ex.isEmpty((this.dsUnitTest.getColumn(0, "PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsUnitTest) )
	{
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;

	var msgNo;

	//성명 입력 체크
	var rtnVal = this.divContents.form.divDevUser.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divDevUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertDevUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcDevUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	rtnVal = this.divContents.form.divTester.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divTester rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertTesterI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcTester.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	return true;
} 

