    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ3820_P01
* 화면(명)		︰ 요구사항 조회
* 메뉴(경로)	 : 요구사항을 검색한다.
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.31
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.31	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/
/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_PjComm.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/

/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
}

this.fnInit = function()
{	
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	// transaction
	var sTranId = "select01";					// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ3820P/select01";			// 서비스명
    var sInDs = "dsCond=dsCond";				// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsReqment=dsReqment";	// 서버에서 수신할 데이타셋
    var sArg = "";								// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ3800_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ3800P/save01";
    var sInDs = "dsCond=dsCond dsReqment=dsReqment:U";
	var sOutDs = "dsReqment=dsReqment";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnImportExcelCallback = function(obj,  e : nexacro.ExcelImportEventInfo)
{
	this.dsReqment.clearData();

	trace("this.fnImportExcelCallback()");
	
	for(var i=0; i<this.dsImport.rowcount; i++)
	{
		var aRow = this.dsReqment.setAddRow();
		
		this.dsReqment.setColumn(aRow, "P_PJT_ID", this.dsCond.getColumn(0, "PJT_ID"));
		this.dsReqment.setColumn(aRow, "PJT_ID", this.dsCond.getColumn(0, "SUB_PJT_ID"));
		this.dsReqment.setColumn(aRow, "P_PJT_NAME", this.dsCond.getColumn(0, "PJT_NM"));
		this.dsReqment.setColumn(aRow, "PJT_NAME", this.divSearch.form.cboSubProject.text);
		this.dsReqment.setColumn(aRow, "BIZ_ID", this.dsCond.getColumn(0, "BIZ_ID"));
		this.dsReqment.setColumn(aRow, "BIZ_NAME", this.divSearch.form.cboBizId.text);
		this.dsReqment.setColumn(aRow, "CHG_ST_CD", "REQ_STS_CD01");

		for(var j=0; j<this.dsImport.colcount; j++)
		{
			if(this.dsReqment.colinfos[this.dsImport.getColID(j)] == "[object DSColumnInfo]")
			{
				if(this.dsImport.getColID(j) == "REQ_TYPE_CD")
				{
					var fRow = this.dsReqTypeCd.findRowExpr("String(CMM_CD_NM).indexOf('" + this.dsImport.getColumn(i, this.dsImport.getColID(j)) + "') >= 0");

					if(fRow > -1)
					{
						this.dsReqment.setColumn(aRow, this.dsImport.getColID(j), this.dsReqTypeCd.getColumn(fRow, "CMM_CD"));
					}
					else
					{
						this.dsReqment.setColumn(aRow, this.dsImport.getColID(j), undefined);
					}
				}
				else if(this.dsImport.getColID(j) == "ACCEPT_YN" || this.dsImport.getColID(j) == "CONF_YN")
				{
					var fRow = this.dsYnCd.findRow("CMM_CD_NM", this.dsImport.getColumn(i, this.dsImport.getColID(j)));

					if(fRow > -1)
					{
						this.dsReqment.setColumn(aRow, this.dsImport.getColID(j), this.dsYnCd.getColumn(fRow, "CMM_CD"));
					}
					else
					{
						this.dsReqment.setColumn(aRow, this.dsImport.getColID(j), "N");
					}
				}
				else
				{
					this.dsReqment.setColumn(aRow, this.dsImport.getColID(j), this.dsImport.getColumn(i, this.dsImport.getColID(j)));
				}
			}
		}
	}
}

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			break;

		case "save01" :
			Ex.core.pclose(this, "Y");
			break;
	}
}

// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "" :
			break;
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "alertNoSelSubPjtI" :
			this.divSearch.form.cboSubProject.setFocus();
			break;

		case "alertNoSelBizIdI" :
			this.divSearch.form.cboBizId.setFocus();
			break;

		case "resetConfirm" :
			this.dsReqment.clearData();
			break;

		case "saveConfirm" :
			this.fnSave();
			break;
	}
}

