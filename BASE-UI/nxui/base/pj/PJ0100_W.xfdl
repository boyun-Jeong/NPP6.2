// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnTreeExpand" :			//트리 전체 펼치기
			this.grdMainList.set_treeinitstatus("expand,all");
			break;

		case "btnTreeCollapse" :		//트리 전체 접기
			this.grdMainList.set_treeinitstatus("collapse,all");
			
			var minLvl = this.grdMainList.getBindDataset().getMin("LVL");
	
			for(var i=0; i<this.grdMainList.getBindDataset().rowcount; i++)
			{
				if(this.grdMainList.getBindDataset().getColumn(i, "LVL") < 1)
				{
					trace("i=" + i);
					this.grdMainList.setTreeStatus(i, true);
				}
			}
			break;

		case "btnSearchPjtPop" :
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "프로젝트 검색"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnSearch" :			// 검색
			this.fnSearch();
			break;
		
		case "btnSearchReset" :		// 검색조건 초기화
			this.fnSearchReset();
			break;

		case "btnExcelTemplate" :
			Ex.util.downloadTemplateFiles(this, null, "WBS_UPLOAD");
			break;

		case "btnWbsDay" :
			if( Ex.isEmpty(this.dsCond.getColumn(0, "SEL_SUB_PJT_ID")) )
			{
				var oParam = {id : "alertNoSelectSubProjectI", msg : "예외일정 관리는 서브프로젝트 조회시에서만 가능합니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}

			var fRow = this.divSearch.form.cboSubPjt.getInnerDataset().findRow("PJT_ID", this.dsCond.getColumn(0, "SEL_SUB_PJT_ID"));
			
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P04",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P04.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "WBS 예외 일정 관리 ",
					pPjtId	: this.dsCond.getColumn(0, "SEL_SUB_PJT_ID"),
					pPjtNm	: this.divSearch.form.cboSubPjt.getInnerDataset().getColumn(fRow, "PJT_NAME")
					
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnUpLvl" :		//←
			var strMsg = "[" + this.dsPjWbs.getColumn(this.dsPjWbs.rowposition, "WBS_CODE") + "] - " +
						 this.dsPjWbs.getColumn(this.dsPjWbs.rowposition, "WBS_NAME") +
						 "\n를 상위로 이동하시겠습니까?";

			var param = {
						id : "moveConfirm"
					  , msg : strMsg
					  , arrparam : ['']
					  , msgtype : "I"
			};
			Ex.core.confirm(this, param);
			break;

		case "btnUpSort" :		//↑
			this.fnSetRowMove("UP", this.grdMainList, this.dsPjWbsTmp);
			break;

		case "btnDownSort" :	//↓
			this.fnSetRowMove("DOWN", this.grdMainList, this.dsPjWbsTmp);
			break;

		case "btnExcelDown" :	// 엑셀다운
			var oInfo = {
   				Grid	: [this.grdMainList],
   				Sheet	: [this.getOwnerFrame().info.menuNm],
   				//FileNm	: ["WBS_" + this.divSearch.form.edtPjtNm.text + (!Ex.isEmpty(this.divSearch.form.cboSubPjt.text) ? "_" + this.divSearch.form.cboSubPjt.text : "")],
				FileNm : ["WBS"],
   				ExportType : "XLSX",
   				ExportId : "excel01"
   			};
			this.gfnExportExcel(this, oInfo);
			break;

		case "btnChart" :
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P02",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P02.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "WBS CHART"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnAddWbs" :			// 추가
			if( Ex.isEmpty(this.dsCond.getColumn(0, "SEL_PJT_ID")) )
			{
				var oParam = {id : "alertNoSelectProjectI", msg : "선택된 프로젝트가 없습니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if( Ex.isEmpty(this.dsCond.getColumn(0, "SEL_SUB_PJT_ID")) )
			{
				var oParam = {id : "alertNoSelectSubProjectI", msg : "일정 추가/삭제 및 WBS레벨 변경은 서브프로젝트 조회시에서만 가능합니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if(this.dsPjWbs.getColumn(this.dsPjWbs.rowposition, "LVL") == 0)
			{
				var oParam = {id : "alertProjectlVLI", msg : "프로젝트 레벨에는 추가할 수 없습니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}

			var strPrWbsCode = this.dsPjWbs.getColumn(this.dsPjWbs.rowposition, "WBS_CODE");
			var nCuLvl       = this.dsPjWbs.getColumn(this.dsPjWbs.rowposition, "LVL");

			var aRow = this.fnGetLastChildRow(this.grdMainList, this.grdMainList.getTreeParentRow(this.dsPjWbs.rowposition));			
			var nRow = this.dsPjWbs.setInsertRow(aRow + 1);

			var nPntIdx = strPrWbsCode.lastIndexOf(".");

			var strCuWbsCode = "";
			var nNxtCd;
			var strTmpCode = "";

			if( Ex.isEmpty(nPntIdx) )
			{
				strCuWbsCode = new Number(strPrWbsCode).valueOf() + 1;
			}
			else
			{
				nNxtCd  = new Number(strPrWbsCode.substr(nPntIdx + 1)) + 1;
				strTmpCode = strPrWbsCode.substr(0, nPntIdx + 1);
				strCuWbsCode = strTmpCode + nNxtCd;
			}

			this.dsPjWbs.setColumn(nRow, "WBS_CODE", strCuWbsCode);

			if (this.dsPjWbsTot.rowcount == 0)
			{
				this.dsPjWbsTot.setInsertRow();
				this.dsPjWbsTot.setColumn(0, "PJT_ID", this.dsCond.getColumn(0, "SEL_SUB_PJT_ID"));
			}

			this.dsPjWbs.setColumn(nRow, "PJT_ID",      this.dsCond.getColumn(0, "SEL_SUB_PJT_ID"));
			
			this.dsPjWbs.setColumn(nRow, "PARENT_ID",   this.dsCond.getColumn(0, "SEL_PJT_ID"));
			this.dsPjWbs.setColumn(nRow, "PLAN_SDATE",  Ex.util.today());
			this.dsPjWbs.setColumn(nRow, "PLAN_EDATE",  Ex.util.today());
			this.dsPjWbs.setColumn(nRow, "LVL",         0);
			this.dsPjWbs.setColumn(nRow, "IS_LAST_LVL", "Y");
			this.dsPjWbs.setColumn(nRow, "IS_CALCU",    "N");

			this.dsPjWbs.setColumn(nRow, "LVL", nCuLvl);

			this.fnSetWbsCodeRow(this.grdMainList, this.dsPjWbsTmp);
			break;

		case "btnDelWbs" :			// 삭제
			break;

		case "btnBatchAddRep" :
			if(Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")))
			{
				var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P05",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P05.xfdl",	// <--- 팝업창 오픈 Url
				{
					title		: "WBS 일괄등록",
					pPjtId		: this.dsCond.getColumn(0, "PJT_ID"),
					pPjtNm		: this.dsCond.getColumn(0, "PJT_NM"),
					pSubPjtId	: this.dsCond.getColumn(0, "SUB_PJT_ID")					
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
			break;

		case "btnSaveWbs" :			// 계획기준저장
		case "btnSaveWbs2" :		// 조정계획기준저장
			if (this.dsPjWbs.rowcount < 1) return;
			
			if( this.fnSaveChk() )
			{
				var param = {
						id : (obj.id == "btnSaveWbs" ? "saveConfirm" : "saveConfirm2")
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			else
			{
				Ex.core.popup(
					this,								// <--- 팝업 실행 스코프
					"comTextareaPop",				// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
					"comm::comTextareaPop.xfdl",	// <--- 팝업창 오픈 Url
					{
						title		: "WBS 입력 체크",
						subTitle	: "[계획 기준 저장]입력 체크",
						pMsg		: this.chkMsg
					},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
					"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
				);
			}
			break;
	}
}

this.grdOnDrag = function(obj:nexacro.Grid, e:nexacro.GridDragEventInfo)
{
	if(e.row <= 0 || Ex.isEmpty(this.dsCond.getColumn(0, "SEL_SUB_PJT_ID")) )
	{
		this.stcSelectInfo.set_visible(false);
		this.stcDropInfo.set_visible(false);
		return false;
	}
	
	this.dragRow = e.row;
	trace("this.grdOnDrag() this.dragRow : " + this.dragRow);
	trace("dataset Row : " + obj.getTreeRow(this.dragRow));
	return true;
};

this.grdOnDragMove = function(obj:nexacro.Grid, e:nexacro.GridDragEventInfo)
{
	if(e.col == obj.getBindCellIndex("Body", "WBS_CODE"))
	{
		//trace("this.grdOnDragMove() this.dragRow : " + this.dragRow + " this.dropRow : " + e.row);
		
		if(this.dragRow == e.row)
		{
			var strVal = "[" + this.dsPjWbs.getColumn(this.dragRow, "WBS_CODE") + "] - \"" + this.dsPjWbs.getColumn(e.row, "WBS_NAME") + "\" 항목";
			this.stcSelectInfo.set_text(strVal);
			this.stcDropInfo.set_visible(false);
			this.stcSelectInfo.set_border("2px dashed crimson");
		}
		else
		{
			var movePRow = this.grdMainList.getTreeParentRow(e.row, true);	//Drop 대상이 되는 Row의 부모 Row정보
			var dragPRow = this.grdMainList.getTreeParentRow(this.dragRow, true);	//Drag된 Row의 부모 Row정보

			var isDropPoss = true;

			if(dragPRow == e.row)
			{
				isDropPoss = false;
			}
			else
			{
				while(movePRow > -1)
				{
					if(this.dragRow == movePRow)
					{
						isDropPoss = false;
						break;
					}
					else
					{
						movePRow = this.grdMainList.getTreeParentRow(movePRow, true);
					}
				}
			}

			var moveStrVal = "[" + this.dsPjWbs.getColumn(this.dragRow, "WBS_CODE") + "] - \"" + this.dsPjWbs.getColumn(this.dragRow, "WBS_NAME") + "\"";
			var targStrVal = "[" + this.dsPjWbs.getColumn(e.row, "WBS_CODE") + "] - \"" + this.dsPjWbs.getColumn(e.row, "WBS_NAME") + "\"";
			
			if(isDropPoss)
			{
				this.stcSelectInfo.set_text(moveStrVal + " 항목을");
				this.stcDropInfo.set_text(targStrVal + " 항목의 하위로 <b v='true'><fc v='blue'>이동 가능</fc></b>");
			}
			else
			{
				this.stcSelectInfo.set_text(moveStrVal + " 항목");
				this.stcDropInfo.set_text(targStrVal + " 항목의 하위로 <b v='true'><fc v='red'>이동 불가</fc></b>");
			}

			this.stcSelectInfo.set_border("2px dashed crimson, 2px dashed crimson, 2px none crimson");
			this.stcDropInfo.set_border("2px none crimson, 2px dashed crimson, 2px dashed crimson");

			this.stcDropInfo.set_visible(true);
		}
		this.stcSelectInfo.set_visible(true);
	}

	//trace("this.stcSelectInfo.text : " + this.stcSelectInfo.getDisplayText());
	//trace("this.stcDropInfo.text : " + this.stcDropInfo.getDisplayText());
	
	var objSizeSel = nexacro.getTextSize(this.stcSelectInfo.getDisplayText(), 'bold 14px/normal "NanumSquareR"');
	var objSizeDrop = nexacro.getTextSize(this.stcDropInfo.getDisplayText(), 'bold 14px/normal "NanumSquareR"');
	
	var objSizeW;
	var objSizeSelW = parseInt(objSizeSel.nx);
	var objSizeDropW = parseInt(objSizeDrop.nx);

	if(this.stcDropInfo.visible)
	{
		if(objSizeSelW >= objSizeDropW)	objSizeW = objSizeSelW;
		else 							objSizeW = objSizeDropW;
	}
	else
	{
		objSizeW	= objSizeSelW;
	}
	objSizeW = objSizeW + 20;
	
	//trace("objSizeW : " + objSizeW + " objSizeSelW : " + objSizeSelW + " objSizeDropW : " + objSizeDropW);

	this.stcSelectInfo.set_width(objSizeW);
	this.stcDropInfo.set_width(objSizeW);

	this.stcSelectInfo.move(e.clientx, e.clienty + obj.getOffsetTop());
	this.stcDropInfo.move(e.clientx, e.clienty + obj.getOffsetTop() + 24);
};

this.grdOnDrop = function(obj:nexacro.Grid,e:nexacro.GridDragEventInfo)
{
	this.dropRow = e.row;
	
	this.stcSelectInfo.set_visible(false);
	this.stcDropInfo.set_visible(false);

	trace("this.grdOnDrop() this.dragRow : " + this.dragRow + " this.dropRow : " + this.dropRow);

	if( !this.isEscKeyDropCancel )
	{		
		if(this.dragRow != this.dropRow)
		{
			var dragPWbsId = this.dsPjWbs.getColumn(this.dragRow, "P_WBS_CODE");	//Drag가 시작된 row의 부모 WBS_CODE
			var dropWbsId = this.dsPjWbs.getColumn(this.dropRow, "WBS_CODE");	//Drop된 row의 WBS_CODE

			if(dragPWbsId != dropWbsId && e.col == obj.getBindCellIndex("Body", "WBS_CODE"))
			{
				var parentRow = this.grdMainList.getTreeParentRow(this.dropRow, true);
				var dragPRow = this.grdMainList.getTreeParentRow(this.dragRow, true);	//Drag된 Row의 부모 Row정보
		
				var isDropPoss = true;
				
				if(dragPRow == e.row)
				{
					isDropPoss = false;
				}
				else
				{
					while(parentRow > -1)
					{
						if(this.dragRow == parentRow)
						{
							isDropPoss = false;
							break;
						}
						else
						{
							parentRow = this.grdMainList.getTreeParentRow(parentRow, true);
						}
					}
					
					if(isDropPoss)
					{
						this.setWaitCursor(true, true);
						this.setTimer(1, 100);
					}
				}
			}
		}
	}
	else
	{
		this.isEscKeyDropCancel = false;
	}
};

this.grdOnDragLeave = function(obj:nexacro.Grid,e:nexacro.DragEventInfo)
{
	this.stcSelectInfo.set_visible(false);
	this.stcDropInfo.set_visible(false);
};

this.grdOnKeyUp = function(obj:nexacro.Grid,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 27)
	{
		var dragEvt = nexacro.GridDragEventInfo;
		dragEvt.row = -1
		this.grdOnDrag(this.grdMainList, dragEvt);

		var dropEvt = nexacro.GridDragEventInfo;
		dropEvt.col = -1;
		this.grdOnDrop(this.grdMainList, dropEvt);

		this.isEscKeyDropCancel = true;
	}
};

this.formOnTimer = function(obj:nexacro.Form,e:nexacro.TimerEventInfo)
{
	if(e.timerid == 1)
	{
		this.fnSetTempDragDataList(this.grdMainList, this.dsPjWbsTmp, this.dragRow, this.dropRow);
		this.killTimer(1);
		this.setTimer(2, 100);
	}
	else if(e.timerid == 2)
	{
		this.killTimer(2);
		this.setWaitCursor(false, true);
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	//trace("this.dsOnColumnChanged()");
	//trace("obj.id : " + obj.id + " e.columnid : " + e.columnid);
	obj.setUpdateRow(e);
};]]></Script>
