// 조회
this.fnSearch = function()
{
	// transaction
	var sTranId = "select01";								// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ0100W/select01";						// 서비스명
    var sInDs = "dsCond=dsCond";							// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjWbs=dsPjWbs dsPjWbsTot=dsPjWbsTot dsCond=dsCond";	// 서버에서 수신할 데이타셋
    var sArg = "";											// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

/*저장 : 최하위 레벨 계획시작일, 계획종료일, 총작업량, 계획작업량 자동 계산 저장*/
this.fnSave = function()
{
	trace("this.calCalcDate.value ::: " + this.calCalcDate.value);
	this.dsCond.setColumn(0, "CALC_DATE", this.calCalcDate.value);
	this.dsPjWbs.set_enableevent(false);
	this.dsPjWbs.set_updatecontrol(false);

	for(var i=0; i<this.dsPjWbs.rowcount; i++)
	{
		if(this.dsPjWbs.getColumn(i, "CREATE_YN") == "Y")
		{
			this.dsPjWbs.setRowType(i, Dataset.ROWTYPE_INSERT);
			this.dsPjWbs.setColumn(i, "ROWFLAG", "I");
		}
	}

	this.dsPjWbs.set_enableevent(true);
	this.dsPjWbs.set_updatecontrol(true);
	
	// transaction
	var sTranId = "save01";													// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ0100W/save01";										// 서비스명
    var sInDs = "dsCond=dsCond dsPjWbs=dsPjWbs:U";							// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨	dsPjWbsTot=dsPjWbsTot:U
    var sOutDs = "dsPjWbs=dsPjWbs dsPjWbsTot=dsPjWbsTot dsCond=dsCond";		// 서버에서 수신할 데이타셋
    var sArg = "";															// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::" + nErrorCode+"::" + sErrorMsg};
		Ex.core.error(this, param);
		return;
	}

	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			this.grdMainList.set_treeinitstatus("expand,all");
			this.fnSetButtonControl();
			
			if(this.dsPjWbsTot.rowcount == 0)
			{
				this.dsPjWbsTot.setInsertRow(0);
				this.dsPjWbsTot.setColumn(0, "CALC_DATE", Ex.util.today());
			}
			break;

		case "save01" :
			Ex.core.toast(this, "10004");
			this.grdMainList.set_treeinitstatus("expand,all");
			this.fnSetButtonControl();
			break;
	}
}

// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "PJ0100_P01" :
			trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)	this.dsPopRtn.loadXML(varValue);
			this.dsCond.setColumn(0, "PJT_ID", this.dsPopRtn.getColumn(0, "PJT_ID"));
			this.dsCond.setColumn(0, "PJT_NM", this.dsPopRtn.getColumn(0, "PJT_NAME"));
			this.fnSetCombo();
			break;

		case "PJ0100_P05" :
			if(varValue == "Y")
			{
				this.fnSearch();
			}
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "alertCalcDateI" :
			this.calCalcDate.setFocus();
			break;

		case "alertPlanDateI" :		
			this.grdMainList.getBindDataset().set_rowposition(this.dateChkRow);		// 데이터셋 ROW 포커스 이동
			this.grdMainList.showEditor(true);										// 그리드 편집모드로 변경
			var cellIdx = this.grdMainList.getBindCellIndex("Body", "PLAN_STIME");
			this.grdMainList.setCellPos(cellIdx, this.dateChkRow);					// 그리드 셀 포커스 이동
			break;

		case "moveConfirm" :
			var pRow = this.grdMainList.getTreeParentRow(this.grdMainList.getTreeRow(this.dsPjWbs.rowposition), false);
				pRow = this.grdMainList.getTreeParentRow(pRow, true);

			trace("btnUpLvl pRow : " + pRow);
			
			if(pRow > -1)
			{
				var pLvl = this.dsPjWbs.getColumn(pRow, "LVL");

				trace("btnUpLvl pRow : " + pRow + " pLvl : " + pLvl + " pWBS_CODE : " + this.dsPjWbs.getColumn(pRow, "WBS_CODE"));
				
				if(pLvl >= 0)
				{
					trace("btnUpLvl dragRow : " + this.grdMainList.getTreeRow(this.dsPjWbs.rowposition) + " dropRow : " + pRow);
					this.fnSetTempDragDataList(this.grdMainList, this.dsPjWbsTmp, this.grdMainList.getTreeRow(this.dsPjWbs.rowposition), pRow);
				}
			}
			break;

		case "saveConfirm" :
			this.dsCond.setColumn(0, "GUBUN", "1");
			this.fnSave();
			break;

		case "saveConfirm2" :
			this.dsCond.setColumn(0, "GUBUN", "2");
			this.fnSave();
			break;
	}
}

