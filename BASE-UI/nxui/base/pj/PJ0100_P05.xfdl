this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(obj.id == "dsPjWbs")
	{
		if(obj.getColumn(e.row, "IS_LAST_LVL") == "Y")
		{		
			if(e.columnid == "PLAN_SDATE")
			{
				if(obj.getMax("PLAN_SDATE") != obj.getColumn(0, "PLAN_SDATE"))
				{
					obj.setColumn(0, "PLAN_SDATE", obj.getMax("PLAN_SDATE"));
				}
			}
			else if(e.columnid == "PLAN_EDATE")
			{
				if(obj.getMax("PLAN_EDATE") != obj.getColumn(0, "PLAN_EDATE"))
				{
					obj.setColumn(0, "PLAN_EDATE", obj.getMax("PLAN_EDATE"));
				}
			}
		}
	}
	obj.setUpdateRow(e);
