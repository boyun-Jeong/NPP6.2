	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

//Sub Project Combo 조회
this.fnSetSubPjtCombo = function()
{
	this.setSubPjtList(this, this.dsCond.getColumn(0, "PJT_ID"), this.divSearch.form.cboSubProject, this.searchSubPjtInfoCallBack);
}

//Sub Project Combo 조회 CallBack
this.searchSubPjtInfoCallBack = function()
{
	var aRow = this.divSearch.form.cboSubProject.getInnerDataset().insertRow(0);
	this.divSearch.form.cboSubProject.getInnerDataset().setColumn(0, "PJT_NAME", Ex.core.word("- 선택 -"));

	var fRow = -1;
	this.divSearch.form.cboSubProject.set_displaynulltext("");
	
	if(this.divSearch.form.cboSubProject.getInnerDataset().rowcount > 1)
	{
		fRow = this.divSearch.form.cboSubProject.getInnerDataset().findRow("PJT_ID", this.dsCond.getColumn(0, "SUB_PJT_ID"));
	}
	else
	{
		fRow = 0;
	}

	if(fRow > -1)	this.divSearch.form.cboSubProject.set_index(fRow);
	else			this.divSearch.form.cboSubProject.set_index(0);

	//trace("[PJ0100_P06] this.searchSubPjtInfoCallBack() fRow : " + fRow + " this.divSearch.form.cboSubProject.value : " + this.divSearch.form.cboSubProject.value);

	this.dsCond.setColumn(0, "SUB_PJT_ID", this.divSearch.form.cboSubProject.value);

	//trace("AF this.searchProjectBizCallBack() DS_COND BIZ_ID : " + this.dsCond.getColumn(0, "BIZ_ID"));
	//trace("AF this.searchProjectBizCallBack() COMBO BIZ_ID : " + this.divSearch.form.cboBizId.value);
	
	var calcDate = this.divSearch.form.cboSubProject.getInnerDataset().getColumn(fRow, "CALC_DATE");
	
	if( !Ex.isEmpty(calcDate) )
	{
		this.dsCond.setColumn(0, "CALC_DATE", calcDate);
	}
	else
	{
		this.dsCond.setColumn(0, "CALC_DATE", "");
	}
	
	if( !Ex.isEmpty(this.dsCond.getColumn(0, "SUB_PJT_ID")) )
	{
		var version = this.divSearch.form.cboSubProject.getInnerDataset().getColumn(fRow, "VERSION");
		trace("version : " + version);

		if(version == "0.0")
		{
			var oParam = {
				 upCmmCd : ['WBS_VER_CD']	// 조회할 상위코드
				,codeDiv: ['PJT_MNG_CD']	// 조회할 코드의 업무구분
				,optStr	: ['']				// ALL: '- 전체 -'; SEL: '- 선택 -'
				,useYn	: ['Y']				// 사용여부
				,filter	: ['']				// filterStr
				,objDs	: [this.dsWbsVerCd]	// copy dataset
			}
			Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);

			this.cboWbsVerCd.set_value(version);
		}
		else
		{
			var verNext = nexacro.toNumber(version);
			verNext = verNext + 0.1;
			this.dsWbsVerCd.clearData();
			var aRow = this.dsWbsVerCd.addRow();
			this.dsWbsVerCd.setColumn(aRow, "CMM_CD", version);
			this.dsWbsVerCd.setColumn(aRow, "CMM_CD_NM", version);

			aRow = this.dsWbsVerCd.addRow();
			this.dsWbsVerCd.setColumn(aRow, "CMM_CD", verNext);
			this.dsWbsVerCd.setColumn(aRow, "CMM_CD_NM", verNext);
			
			this.cboWbsVerCd.set_value(version);
		}
		this.cboWbsVerCd.set_visible(true);
	}
	else
	{
		this.dsWbsVerCd.clearData();
		this.cboWbsVerCd.set_visible(false);
	}
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsPjWbs)) return false;
	
	if(Ex.isEmpty(this.dsCond.getColumn(0, "CALC_DATE")))
	{
		var oParam = {id : "alertCalcDateI", msg : "기준일자를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !this.grdMainList.checkValidate() ) return false;

	if(this.fnSaveChk())
	{
		return true;
	}
	else
	{
		Ex.core.popup(
			this,							// <--- 팝업 실행 스코프
			"comTextareaPop",				// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			"comm::comTextareaPop.xfdl",	// <--- 팝업창 오픈 Url
			{
				title		: "WBS 입력 체크",
				subTitle	: "Excel 일괄 등록 체크 내용",
				pMsg		: this.chkMsg
			},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
		);
		return false;
	}
} 

this.fnSaveChk = function()
{
	this.chkMsg = "";
	for(var i=0; i<this.dsPjWbs.rowcount; i++)
	{
		var wbsCode = this.dsPjWbs.getColumn(i, "WBS_CODE");
		var wbsName = this.dsPjWbs.getColumn(i, "WBS_NAME");

		if( this.grdMainList.isTreeLeafRow(i, true) )
		{
			if( Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_SDATE")) && Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_ㄸDATE")) )
			{
				this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[화면 [" + (i+1) + "행" + "] ";
				this.chkMsg += "계획시작일/종료일이 입력되지 않았습니다.";
			}
			else
			{
				if( Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_SDATE")) )
				{
					this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[화면 [" + (i+1) + "행" + "] ";
					this.chkMsg += "계획시작일이 입력되지 않았습니다.";
				}
				else if( Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_EDATE")) )
				{
					this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[화면 [" + (i+1) + "행" + "] ";
					this.chkMsg += "계획종료일이 입력되지 않았습니다.";
				}
			}
		}

		if( !Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_SDATE")) && !Ex.isEmpty(this.dsPjWbs.getColumn(i, "PLAN_EDATE")) )
		{
			if(this.dsPjWbs.getColumn(i, "PLAN_SDATE") > this.dsPjWbs.getColumn(i, "PLAN_EDATE"))
			{
				this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[화면 [" + (i+1) + "행" + "] ";
				this.chkMsg += "계획종료일은 계획시작일보다 커야 합니다.";
			}
		}
		
		var actlCmplRate = nexacro.toNumber(this.dsPjWbs.getColumn(i, "ACTL_CMPL_RATE"), 0);
		if(actlCmplRate > 100)
		{
			this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[화면 [" + (i+1) + "행" + "] ";
			this.chkMsg += "실적은 100보다 크게 입력할 수 없습니다.";
		}

		if(this.grdMainList.getTreeChildCount(i) > 0)
		{
			var grpTot = 0;
			for(var j=0; j<this.grdMainList.getTreeChildCount(i); j++)
			{
				var cRow = this.grdMainList.getTreeChildRow(i, j);
				var weight	= nexacro.toNumber(this.dsPjWbs.getColumn(cRow, "WEIGHT"), 0);
				//trace("cRow : " + cRow + " wbsCode : " + wbsCode + " wbsName : " + wbsName + " weight : " + weight);
				grpTot = grpTot + weight;					
			}
			
			/* 임시 주석
			if(grpTot != 100)
			{
				this.chkMsg += ((Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[" + wbsCode + "] " + wbsName);

				if(grpTot > 100)
				{
					 this.chkMsg += " - 하위의 가중치 합이 100을 초과하였습니다.";
				}
				else
				{
					this.chkMsg += " - 하위의 가중치 합은 100이 되도록 입력해야 합니다.";
				}
			}
			임시주석 */
		}
		/*
		else
		{
			var weight	= nexacro.toNumber(this.dsPjWbs.getColumn(i, "WEIGHT"), 0);
			
			this.chkMsg += ((Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[" + wbsCode + "] " + wbsName);

			if(weight > 100)
			{
				 this.chkMsg += " - 하위의 가중치 합이 100을 초과하였습니다.";
			}
			else
			{
				this.chkMsg += " - 하위의 가중치 합은 100이 되도록 입력해야 합니다.";
			}
		}
		*/
		if(this.dsPjWbs.getColumn(i, "LVL") == 0)
		{
			var weight	= nexacro.toNumber(this.dsPjWbs.getColumn(i, "WEIGHT"), 0);
			
			/* 임시 주석
			if( Ex.isEmpty(this.dsPjWbs.getColumn(i, "WEIGHT")) )
			{
				this.chkMsg += ((Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[" + wbsCode + "] " + wbsName);
				this.chkMsg += " - 가중치가 입력되지 않았습니다.";
			}
			else
			{
				if(weight > 100)
				{
					this.chkMsg += ((Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[" + wbsCode + "] " + wbsName);
					this.chkMsg += " - 가중치는 100을 초과하여 입력할 수 없습니다.";
				}
			}
			임시주석 */
		}
	}
	trace("this.chkMsg:\n" + this.chkMsg);
	return Ex.isEmpty(this.chkMsg) ? true : false;
}

