    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ0100_P06
* 화면(명)		︰ 요구사항 조회
* 메뉴(경로)	 : 요구사항을 검색한다.
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.31
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.31	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/
/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_PjComm.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.chkMsg;
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
}

this.fnInit = function()
{	
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	// transaction
	var sTranId = "select01";					// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ3820P/select01";			// 서비스명
    var sInDs = "dsCond=dsCond";				// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjWbs=dsPjWbs";	// 서버에서 수신할 데이타셋
    var sArg = "";								// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ3800_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ0100P/save02";
    var sInDs = "dsCond=dsCond dsPjWbs=dsPjWbs:U";
	var sOutDs = "dsPjWbs=dsPjWbs";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnImportExcelCallback = function(obj,  e : nexacro.ExcelImportEventInfo)
{
	if(this.dsImport.rowcount == 0)
	{
		var oParam = {id : "alertNoDataI", msg : "EXCEL 파일에 읽어올 DATA가 없습니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	trace("this.fnImportExcelCallback()");
	
	this.grdMainList.set_binddataset("");

	this.chkMsg = "";

	trace("this.dsImport.rowcount ::::::::::::::> " + this.dsImport.rowcount);

	var excelHeadRow = 1;	//Excel 양식의 HeadRow 수

	var minWbsCode = String(this.dsImport.getMin("Column0") || '');
	var staLvl = minWbsCode.split(".");
	var staIdx;
	
	if(staLvl.length == 1)
	{
		if( Ex.isEmpty(staLvl) ) return;
		else staIdx = staLvl[0];
	}
	trace("minWbsCode : " + minWbsCode);

	for(var i=0; i<this.dsImport.rowcount; i++)
	{
		var wbsCode = this.dsImport.getColumn(i, this.dsImport.getColID(0)) || '';
		var wbsName = this.dsImport.getColumn(i, this.dsImport.getColID(1)) || '';
		
		if( !Ex.isEmpty(wbsCode) )
		{
			wbsCode = String(wbsCode);
			var lvl = wbsCode.split('.').length;
			trace("i = " + i + " WBS_CODE : " + wbsCode + " lvl : " + lvl);
			
			if(lvl > staLvl.length)
			{
				var endIdx = wbsCode.lastIndexOf(".");
				var pWbsCode = wbsCode.substring(0, endIdx);
				
				trace("pWbsCode : " + pWbsCode);
				
				if(this.dsImport.findRow("Column0", pWbsCode) == -1)
				{
					//메시지
					this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") + "[EXCEL Line No. " + (i+1+excelHeadRow) + "] WBS코드 : [" + wbsCode + "]";
					this.chkMsg += "에 해당하는 상위 WBS 코드를 찾을 수 없습니다.";
				}
			}
		}
		else
		{
			this.chkMsg += (Ex.isEmpty(this.chkMsg) ? "" : "\n") +
							"[EXCEL Line No. " + (i+1+excelHeadRow) + "] WBS코드가 입력되지 않았습니다.";
		}
	}
	
	trace("this.chkMsg:\n" + this.chkMsg);
	
	if( !Ex.isEmpty(this.chkMsg) )
	{
		Ex.core.popup(
			this,					// <--- 팝업 실행 스코프
			"comTextareaPop",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			"comm::comTextareaPop.xfdl",	// <--- 팝업창 오픈 Url
			{
				title		: "WBS 입력 체크",
				subTitle	: "Excel 일괄 등록 체크 내용",
				pMsg		: this.chkMsg
			},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
		);
		return;
	}

	this.dsImport.set_keystring("S:+Column0");

	this.addRowData();
}

this.addRowData = function(staIdx)
{
	var excelColArr = [
		"WBS_CODE", "WBS_NAME", "PLAN_SDATE", "PLAN_EDATE", "EXE_USER_NM",
		"CHG_USER_NM", "WBS_DESC", "WEIGHT", "ACTL_CMPL_RATE"
	];

	for(var i=0; i<this.dsImport.rowcount; i++)
	{
		var aRow = this.dsPjWbs.setAddRow();
			
		this.dsPjWbs.setColumn(aRow, "PARENT_ID", this.dsCond.getColumn(0, "PJT_ID"));
		this.dsPjWbs.setColumn(aRow, "PJT_ID", this.dsCond.getColumn(0, "SUB_PJT_ID"));

		for(var j=0; j<excelColArr.length; j++)
		{
			//trace("j=" + j + " excelCol : " + excelColArr[j] + " dsImport ColNm : " + this.dsImport.getColID(j));
			if(excelColArr[j] == "WEIGHT")
			{
				var weight = this.dsImport.getColumn(i, this.dsImport.getColID(j));
				if( !Ex.isEmpty(weight) )
				{
					//trace("i=" + i + (weight * 100));
					this.dsPjWbs.setColumn(aRow, excelColArr[j], weight * 100);
				}
			}
			else if(excelColArr[j] == "PLAN_SDATE" || excelColArr[j] == "PLAN_EDATE")
			{
				this.dsPjWbs.setColumn(aRow, excelColArr[j], nexacro.replaceAll(this.dsImport.getColumn(i, this.dsImport.getColID(j)), "-", ""));
			}
			else if(excelColArr[j] == "WBS_CODE")
			{
				var wbsCode = String(this.dsImport.getColumn(i, this.dsImport.getColID(j))||'');
				var lvl = wbsCode.split('.').length;
				this.dsPjWbs.setColumn(aRow, "LVL", lvl);
				this.dsPjWbs.setColumn(aRow, "EX_WBS_CODE", wbsCode);

				if( !Ex.isEmpty(staIdx) )
				{
					if(lvl == 1)
					{
						this.dsPjWbs.setColumn(aRow, "P_WBS_CODE", staIdx);
						this.dsPjWbs.setColumn(aRow, "WBS_CODE", staIdx + "." + wbsCode);
					}
					else
					{
						var endIdx = wbsCode.lastIndexOf(".");
						var pWbsCode = lvlStartNum + "." + wbsCode.substring(0, endIdx);
						this.dsPjWbs.setColumn(aRow, "P_WBS_CODE", pWbsCode);
						this.dsPjWbs.setColumn(aRow, "WBS_CODE", staIdx + "." + wbsCode);
					}
				}
			}
			else
			{
				this.dsPjWbs.setColumn(aRow, excelColArr[j], this.dsImport.getColumn(i, this.dsImport.getColID(j)));
			}
		}
	}

	this.grdMainList.set_treeinitstatus("expand,all");
	this.grdMainList.set_binddataset(this.dsPjWbs);

	var subPjtDs = this.divSearch.form.cboSubProject.getInnerDataset();
	var fRow = subPjtDs.findRow("PJT_ID", this.divSearch.form.cboSubProject.value);
	
	//trace("fRow : " + fRow);

	this.dsPjWbs.setInsertRow(0);
	this.dsPjWbs.setColumn(0, "PARENT_ID", subPjtDs.getColumn(fRow, "PARENT_ID"));
	this.dsPjWbs.setColumn(0, "PJT_ID", subPjtDs.getColumn(fRow, "PJT_ID"));
	this.dsPjWbs.setColumn(0, "WBS_CODE", subPjtDs.getColumn(fRow, "WBS_CODE"));
	this.dsPjWbs.setColumn(0, "WBS_NAME", subPjtDs.getColumn(fRow, "WBS_NAME"));
	this.dsPjWbs.setColumn(0, "PLAN_SDATE", this.dsPjWbs.getMax("PLAN_SDATE"));
	this.dsPjWbs.setColumn(0, "PLAN_EDATE", this.dsPjWbs.getMax("PLAN_EDATE"));
	this.dsPjWbs.setColumn(0, "EXE_USER_NM", subPjtDs.getColumn(fRow, "EXE_USER_NM"));
	this.dsPjWbs.setColumn(0, "CHG_USER_NM", subPjtDs.getColumn(fRow, "CHG_USER_NM"));
	this.dsPjWbs.setColumn(0, "WBS_DESC", subPjtDs.getColumn(fRow, "WBS_DESC"));
	this.dsPjWbs.setColumn(0, "WEIGHT", subPjtDs.getColumn(fRow, "WEIGHT"));
	this.dsPjWbs.setColumn(0, "LVL", subPjtDs.getColumn(fRow, "LVL"));
	this.dsPjWbs.setColumn(0, "ACTL_CMPL_RATE", subPjtDs.getColumn(fRow, "ACTL_CMPL_RATE"));

	//this.fnSetWbsCodeRow(this.grdMainList, this.dsPjWbsTmp, staIdx);
	this.fnSetWbsCodeRow(this.grdMainList, this.dsPjWbsTmp);
	this.grdMainList.vscrollbar.set_pos(0);
	
	
}

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			break;

		case "save01" :
			Ex.core.pclose(this, "Y");
			break;
	}
}

// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "" :
			break;
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "alertNoSelSubPjtI" :
			this.divSearch.form.cboSubProject.setFocus();
			break;

		case "alertNoSelBizIdI" :
			this.divSearch.form.cboBizId.setFocus();
			break;

		case "resetConfirm" :
			this.dsPjWbs.clearData();
			break;

		case "saveConfirm" :
			this.fnSave();
			break;

		case "alertCalcDateI" :
			this.divSearch.form.calCalcDate.setFocus();
			break;

		case "restConfirm" :
			this.dsPjWbs.clearData();
			break;
	}
}

