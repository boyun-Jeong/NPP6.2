// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchPjtPop" :
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "프로젝트 검색"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnFileSelect" :		// 파일선택
			if(Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")))
			{
				var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if(Ex.isEmpty(this.dsCond.getColumn(0, "SUB_PJT_ID")))
			{
				var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			this.dsPjWbs.clearData();

			this.gfnImportExcel(this, "head=;body=!A2;", "dsImport");
			break;

		case "btnDelete" :
			this.fnDelete(this.dsPjWbs);
			break;

		case "btnReset" :
			var param = {
						id : "restConfirm"
					  , msg : "초기화 하시겠습니까?"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			break;

		case "btnSave" :
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : ("saveConfirm")
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnChangeFmt" :
			if(this.grdMainList.formatid == "default")
			{
				this.grdMainList.set_formatid("default_org");
				this.divGrdMtTopBtn.form.btnChangeFmt.set_text("양식 값 보기");
			}
			else
			{
				this.grdMainList.set_formatid("default");
				this.divGrdMtTopBtn.form.btnChangeFmt.set_text("실제 값 보기");
			}
			break;

		case "btnClose" :
			Ex.core.pclose(this);
			break;
	}
}

