    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ3400_W
* 화면(명)		︰ 투입인력관리
* 메뉴(경로)	︰ 프로젝트관리 > 인력관리 >  투입인력관리
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.20
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.20	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.fv_grdFormats;
this.fv_grdExcelFormats;
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
}

this.fnInit = function()
{	
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 검색조건 초기화
this.fnSearchReset = function()
{
// 	this.dsCond.setColumn(0, "PJT_ID", "");
// 	this.dsCond.setColumn(0, "PJT_NM", "");
	this.dsCond.setColumn(0, "USER_NM", undefined);
	this.dsCond.setColumn(0, "PARTIN_CD", "PJT_PRT_DIV_CD01");
	this.divSearch.form.edtActUserNm.setFocus();
}

// 조회
this.fnSearch = function()
{
	if(Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")))
	{
		var oParam = {id : "alertNoSearchProjectI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	
	// transaction
	var sTranId = "select01";							// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ3400W/select01";					// 서비스명
    var sInDs = "dsCond=dsCond";						// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjMonth=dsPjMonth dsUser=dsUser";	// 서버에서 수신할 데이타셋
    var sArg = "";										// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	this.dsUserMm.clearData();
	this.dsUserMm.set_updatecontrol(false);
	for(var i=0; i<this.dsUser.rowcount; i++)
	{
		if( this.dsUser.getColumn(i, "ROWFLAG") == "U")
		{
			//db에서 조회한 project 기간을 추가한다.
			for(var j=0; j<this.dsPjMonth.rowcount; j++)
			{
				// pivot column 정의
				var nRow = this.dsUserMm.addRow();
				planCol = "MM" + String(j+1).padLeft(3, "0") + "_PLAN_MM";
				actCol  = "MM" + String(j+1).padLeft(3, "0") + "_ACT_MM";

				//trace ( " planCol : " + planCol  + "  actCol : " + actCol );

				var yyyymm = this.dsPjMonth.getColumn(j , "PJT_MONTH");
				this.dsUserMm.setColumn( nRow, "USER_ID",  this.dsUser.getColumn(i, "USER_ID"));
				this.dsUserMm.setColumn( nRow, "PJT_ID",   this.dsUser.getColumn(i, "PJT_ID"));
				this.dsUserMm.setColumn( nRow, "YYYYMM",   yyyymm);
				this.dsUserMm.setColumn( nRow, "PLAN_MM",  this.dsUser.getColumn(i, planCol));
				this.dsUserMm.setColumn( nRow, "ACT_MM",   this.dsUser.getColumn(i, actCol));
				this.dsUserMm.setRowType(nRow, Dataset.ROWTYPE_UPDATE);
				this.dsUserMm.setColumn(nRow, "ROWFLAG", "U");
			}
		}
	}
	this.dsUserMm.set_updatecontrol(false);
		
	var sTranId = "save01";
    var sService = "PJ3400W/save01";
    var sInDs = "dsCond=dsCond dsUser=dsUser:U dsUserMm=dsUserMm:U";
	var sOutDs = "dsPjMonth=dsPjMonth dsUser=dsUser";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
	
	this.setAddRepButton();
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::" + nErrorCode+"::" + sErrorMsg};
		Ex.core.error(this, param);
		return;
	}

	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			this.setGridFormat();
			this.setAddRepButton();
			break;

		case "save01" :
			Ex.core.toast(this, "10004");
			this.setGridFormat();
			this.setAddRepButton();
			break;
	}
}

// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	trace("this.fnPopupAfter() pID ::: " + pID + " varValue ::: " + varValue);
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "PJ0100_P01" :
			trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)	this.dsPopRtn.loadXML(varValue);
			this.dsCond.setColumn(0, "PJT_ID", this.dsPopRtn.getColumn(0, "PJT_ID"));
			this.dsCond.setColumn(0, "PJT_NM", this.dsPopRtn.getColumn(0, "PJT_NAME"));
			this.fnSearch();
			break;

		case "PJ3400_P01" :
			trace("varValue ::: " + varValue);
			if(varValue == "Y") this.fnSearch();
			break;
	}
}

// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "saveConfirm" :
			this.fnSave();
			break;
	}
}

