 * User Function
 ***********************************************************************************/
// component init
this.fnCompInit = function()
{
	// 공통코드 조회
	this.fnGetCmmCd();

	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divGrdTopBtn.form.btnDelete];	//삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);
	
	arrBtn = [this.divGrdTopBtn.form.btnExcelDown];		//엑셀
	Ex.util.setBtnVisible(this, 'exclDown', arrBtn);

	arrBtn = [this.divGrdBotBtn.form.btnAddRep];		//등록
	Ex.util.setBtnVisible(this, 'save', arrBtn);
	
	arrBtn = [this.divGrdBotBtn.form.btnSave];			//저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);

	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divGrdTopBtn, 'right', true);
	Ex.util.setBtnAlign(this.divGrdBotBtn, 'right', true);

	this.fv_grdFormats = this.grdMainList.formats;
	
	this.fnSearchReset();
}

this.setAddRepButton = function()
{
	var cnt = this.dsUser.getCaseCount("ROWFLAG=='D' || ROWFLAG=='U'");
	trace("this.dsOnColumnDataChanged() cnt : " + cnt);
	if(cnt > 0)
	{
		if(this.divGrdBotBtn.form.btnAddRep.visible)
		{
			this.divGrdBotBtn.form.btnAddRep.set_enable(false);
		}
	}
	else
	{
		if(this.divGrdBotBtn.form.btnAddRep.visible)
		{
			this.divGrdBotBtn.form.btnAddRep.set_enable(true);
		}
	}
}

/**
 * 선택된 프로젝트의 기간 정보를 조회해서 월정보 만큼 
 * 그리드 컬럼을 추가한다.
 * @param  
 * @return : N/A
 */
this.setGridFormat = function()
{
	this.grdMainList.set_enableredraw(false);

	//화면 로드시점에서 저장한 최초 format을 사용한다.
	this.grdMainList.set_formats(this.fv_grdFormats);

	var planCol = "";
	var actCol = "";
	var align = "right";
	var mask = "0.##";
	var displaytype = "number";
	var edittype = "mask";
	var colSize = 80;
	var totSumIdx = 0;
	fv_mmInfo = "";
	
	var formats = ["default_excel", "default"];

	for(var x=0; x<formats.length ; x++)
	{
		var headColIdx = new Array();

		this.grdMainList.set_formatid(formats[x]);
		//db에서 조회한 project 기간을 추가한다.
		for(var i=0; i<this.dsPjMonth.rowcount; i++)
		{
			// pivot column 정의
			planCol = "MM" + String(i+1).padLeft(3, "0") + "_PLAN_MM";
			actCol  = "MM" + String(i+1).padLeft(3, "0") + "_ACT_MM";

			var iCol1 = this.grdMainList.appendContentsCol("Head", false);
			//trace("i=" + i + " iCol1 : " + iCol1);
			this.grdMainList.setFormatColProperty(iCol1, "size", colSize);
			this.grdMainList.setCellProperty("head", iCol1, "text", this.dsPjMonth.getColumn(i,"PJT_FMONTH").substr(2) );
			
			this.grdMainList.setCellProperty("body", iCol1, "text", "bind:" + planCol);
			this.grdMainList.setCellProperty("body", iCol1, "displaytype", displaytype);
			this.grdMainList.setCellProperty("body", iCol1, "align", align);
			this.grdMainList.setCellProperty("body", iCol1, "maskeditformat", mask); 		
			this.grdMainList.setCellProperty("body", iCol1, "edittype", edittype);
			this.grdMainList.setCellProperty("body", iCol1, "editlimit", "0");
			this.grdMainList.setCellProperty("body", iCol1, "maskeditlimitbymask", "both");
			this.grdMainList.setCellProperty("body", iCol1, "maskeditautoselect", true);
			this.grdMainList.setCellProperty("body", iCol1, "validation", this.dsPjMonth.getColumn(i,"PJT_FMONTH").substr(2) + "[" + Ex.core.word("계획") + "];MAX:1");

			totSumIdx = this.grdMainList.getCellCount("Summ")-1;
			this.grdMainList.setCellProperty("Summ", totSumIdx, "text", "expr:dataset.getSum('"+planCol+"')");
			this.grdMainList.setCellProperty("Summ", totSumIdx, "displaytype", displaytype);
			this.grdMainList.setCellProperty("Summ", totSumIdx, "mask", mask);

			var iCol2 = this.grdMainList.appendContentsCol("Head", false);
			//trace("i=" + i + " iCol2 : " + iCol2);
			headColIdx.push(iCol2);
			this.grdMainList.setFormatColProperty(iCol2, "size", colSize);
			this.grdMainList.setCellProperty("head", iCol2, "text", this.dsPjMonth.getColumn(i, "PJT_FMONTH").substr(2));

			this.grdMainList.setCellProperty("body", iCol2, "text", "bind:" + actCol);
			this.grdMainList.setCellProperty("body", iCol2, "displaytype", displaytype);
			this.grdMainList.setCellProperty("body", iCol2, "align", align);
			this.grdMainList.setCellProperty("body", iCol2, "maskeditformat", mask);
			this.grdMainList.setCellProperty("body", iCol2, "edittype", edittype);
			this.grdMainList.setCellProperty("body", iCol2, "editlimit", "0");
			this.grdMainList.setCellProperty("body", iCol2, "maskeditlimitbymask", "both");
			this.grdMainList.setCellProperty("body", iCol2, "maskeditautoselect", true);
			this.grdMainList.setCellProperty("body", iCol2, "validation", this.dsPjMonth.getColumn(i,"PJT_FMONTH").substr(2) + "[" + Ex.core.word("실적") + "];MAX:1");
			
			totSumIdx = this.grdMainList.getCellCount("Summ")-1;
			this.grdMainList.setCellProperty("Summ", totSumIdx, "text", "expr:dataset.getSum('"+actCol+"')");
			this.grdMainList.setCellProperty("Summ", totSumIdx, "displaytype", displaytype);
			//this.grdMainList.setCellProperty("Summ", totSumIdx, "align", align);
			this.grdMainList.setCellProperty("Summ", totSumIdx, "mask", mask);
			
			var totHeadIdx = this.grdMainList.getCellCount("Head")-1;
			
			var iPlan = totHeadIdx-1;
			var iAct = totHeadIdx;
			this.grdMainList.setCellProperty("head", iPlan, "text", Ex.core.word("계획"));
			this.grdMainList.setCellProperty("head", iAct, "text", Ex.core.word("실행"));
		}

		//왜 역순으로 해야 가능한지는 모름
		for(var i=(headColIdx.length-1); i >= 0; i--)
		{
			//trace("headColIdx : " + headColIdx[i]);
			this.grdMainList.mergeContentsCell("head", 0, headColIdx[i]-1, 0, headColIdx[i], headColIdx[i]-1, false);
		}
	}
	this.grdMainList.set_enableredraw(true);

	// Grid 행상태(ROWFLAG) 이미지 처리 util 호출
	this.grdMainList.displayRowType();
}

// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd : ['PJT_PRT_DIV_CD',	'PJT_ACT_CD']	// 조회할 상위코드
		,codeDiv: ['PJT_MNG_CD',		'PJT_MNG_CD']	// 조회할 코드의 업무구분
		,optStr	: ['ALL',				'']				// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn	: ['Y',					'Y']			// 사용여부
		,filter	: ['',					'']				// filterStr
		,objDs	: [this.dsPartinCd,		this.dsRoleCd]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsUser) )	return false;

	// Grid Validation
	if( !this.grdMainList.checkValidate() ) return false;
	return true;
} 

/***********************************************************************************
 * Component Event
 ***********************************************************************************/
// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchPjtPop" :			
			Ex.core.popup(
				this,						// <--- 팝업 실행 스코프
				"PJ0100_P01",				// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P01.xfdl",		// <--- 팝업창 오픈 Url
				{
					title : "프로젝트 검색"
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnSearch" :			// 검색
			this.fnSearch();
			break;
		
		case "btnSearchReset" :		// 검색조건 초기화
			this.fnSearchReset();
			break;

		case "btnDelete" :			// 삭제
			this.fnDelete(this.dsUser);
			break;

		case "btnExcelDown" :		// 엑셀다운
			var oInfo = {
   				Grid	: [this.grdMainList],
   				Sheet	: [this.getOwnerFrame().info.menuNm],
   				FileNm	: [this.getOwnerFrame().info.menuNm],
   				ExportType : "XLSX",
   				ExportId : "excel01"
   			};
			this.gfnExportExcel(this, oInfo);
			break;

		case "btnAddRep" : 			// 등록
			if(Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")))
			{
				var oParam = {id : "alertNoSelectProjectI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
		
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ3400_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ3400_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title	: "투입 인력 등록",
					pPjtId	: this.dsCond.getColumn(0, "PJT_ID"),
					pPjtNm	: this.dsCond.getColumn(0, "PJT_NM"),
					pUserId	: "",
					pMode	: 'NEW'
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
			break;

		case "btnSave" :			//저장
			// validation
			if( this.fnCheckValidate() )
			{	
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;
	}
}

this.onKeyUp = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13) this.fnSearch();
};

this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(e.row < 0) return;

	// PJT_NAME 프로젝트/서브프로젝트명을 누를 때 마다 POPUP화면 호출
	if (e.cell == obj.getBindCellIndex("body","USER_ID") || e.cell == obj.getBindCellIndex("body","USER_NM"))
	{
		var strPjtId = this.dsUser.getColumn(obj.getDatasetRow(e.row), "PJT_ID");

		if(Ex.isEmpty(strPjtId))
		{
			var oParam = {id : "alertNoSelectProjectI", msg : "선택된 프로젝트가 없습니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return;
		}
		
		var strPjtNm	= this.dsUser.getColumn(obj.getDatasetRow(e.row), "PJT_NAME");
		var strUserId	= this.dsUser.getColumn(obj.getDatasetRow(e.row), "USER_ID");

		Ex.core.popup(
			this,					// <--- 팝업 실행 스코프
			"PJ3400_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			"pj::PJ3400_P01.xfdl",	// <--- 팝업창 오픈 Url
			{
				title	: "투입 인력 상세",
				pPjtId	: strPjtId,
				pPjtNm 	: strPjtNm,
				pUserId	: strUserId,
				pMode	: 'SEARCH'
			},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize"				// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	obj.setUpdateRow(e);
	
	this.setAddRepButton();
}
