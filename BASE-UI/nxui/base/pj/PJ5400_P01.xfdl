this.fnCheckValidate = function()
{
	if( Ex.isEmpty((this.dsScnrTest.getColumn(0, "P_PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}

	if( Ex.isEmpty((this.dsScnrTest.getColumn(0, "PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsScnrTest) && !Ex.util.isUpdated(this.dsScrMap) &&
		!Ex.util.isUpdated(this.dsBatchjobMap) && !Ex.util.isUpdated(this.dsInfMap) )
	{
		return false;
	}

	var findRow = this.dsScrMap.findRowExpr("PJT_ID != '" + this.dsScnrTest.getColumn(0, "PJT_ID") + "'");
	if(findRow > -1)
	{
		var oParam = {id : "alertChkPjtIdI", msg : "선택된 서브프로젝트 정보와 화면 기능의 서브프로젝트 정보가 다릅니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	
	findRow = this.dsBatchjobMap.findRowExpr("PJT_ID != '" + this.dsScnrTest.getColumn(0, "PJT_ID") + "'");
	if(findRow > -1)
	{
		var oParam = {id : "alertChkPjtIdI", msg : "선택된 서브프로젝트 정보와 배치 JOB의 서브프로젝트 정보가 다릅니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	
	findRow = this.dsInfMap.findRowExpr("PJT_ID != '" + this.dsScnrTest.getColumn(0, "PJT_ID") + "'");
	if(findRow > -1)
	{
		var oParam = {id : "alertChkPjtIdI", msg : "선택된 서브프로젝트 정보와 요구사항의 서브프로젝트 정보가 다릅니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	
	var msgNo;

	//성명 입력 체크
	var rtnVal = this.divContents.form.divTester.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divTester rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertDevUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcTester.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	return true;
} 

