    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ5400_P01
* 화면(명)		︰ 시나리오 테스트 상세
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.11.13
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.11.13	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/
include "lib::lib_PjComm.xjs";
/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ5400_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ5400_P01] this.fnSearch()");
	var sTranId = "select01";																
    var sService = "PJ5400P/select01";																								// 서비스명
    var sInDs = "dsCond=dsCond";																									// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsScnrTest=dsScnrTest dsStDefect=dsStDefect dsScrMap=dsScrMap dsBatchjobMap=dsBatchjobMap dsInfMap=dsInfMap";		// 서버에서 수신할 데이타셋
    var sArg = "";																													// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ5400_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ5400P/save01";
    var sInDs = "dsCond=dsCond dsScnrTest=dsScnrTest:U dsScrMap=dsScrMap:U dsBatchjobMap=dsBatchjobMap:U dsInfMap=dsInfMap:U";
	var sOutDs = "dsScnrTest=dsScnrTest dsStDefect=dsStDefect dsScrMap=dsScrMap dsBatchjobMap=dsBatchjobMap dsInfMap=dsInfMap dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ5400_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);
	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "srFileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");

			var strTesterId    = this.dsScnrTest.getColumn(0, "TESTER_ID");
			var strTesterNm    = this.dsScnrTest.getColumn(0, "TESTER_NM");
			var strTesterDept      = this.dsScnrTest.getColumn(0, "TESTER_DEPT");
			this.divContents.form.divTester.form.setValue(strTesterId, strTesterNm, strTesterDept);

			var strUpmuUserId    = this.dsScnrTest.getColumn(0, "UPMU_USER_ID");
			var strUpmuUserNm    = this.dsScnrTest.getColumn(0, "UPMU_USER_NM");
			var strUpmuDept      = this.dsScnrTest.getColumn(0, "UPMU_USER_DEPT");
			this.divContents.form.divUpmuUser.form.setValue(strUpmuUserId, strUpmuUserNm, strUpmuDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "STEST_ID")) )
			{
				this.getOwnerFrame().pStestId	= this.dsCond.getColumn(0, "STEST_ID");
				this.getOwnerFrame().pMode = "SEARCH";
			}

			this.divContents.form.btnSearchPjtPop.set_enable(false);
			this.divContents.form.cboSubProject.set_enable(false);
			
			var strTesterId    = this.dsScnrTest.getColumn(0, "TESTER_ID");
			var strTesterNm    = this.dsScnrTest.getColumn(0, "TESTER_NM");
			var strTesterDept      = this.dsScnrTest.getColumn(0, "TESTER_DEPT");
			this.divContents.form.divTester.form.setValue(strTesterId, strTesterNm, strTesterDept);

			var strUpmuUserId    = this.dsScnrTest.getColumn(0, "UPMU_USER_ID");
			var strUpmuUserNm    = this.dsScnrTest.getColumn(0, "UPMU_USER_NM");
			var strUpmuDept      = this.dsScnrTest.getColumn(0, "UPMU_USER_DEPT");
			this.divContents.form.divUpmuUser.form.setValue(strUpmuUserId, strUpmuUserNm, strUpmuDept);

			this.fnSetSubPjtCombo();	//업무 콤보 셋팅
			break;
	}
}
// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "PJ0100_P01" :	//프로젝트 검색
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)	this.dsPopRtn.loadXML(varValue);
			this.dsScnrTest.setColumn(0, "P_PJT_ID", this.dsPopRtn.getColumn(0, "PJT_ID"));
			this.dsScnrTest.setColumn(0, "P_PJT_NAME", this.dsPopRtn.getColumn(0, "PJT_NAME"));
			this.fnSetSubPjtCombo();
			break;

		case "PJ4220_P01" :	//화면 기능 검색
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				for(var i=0; i<this.dsPopRtn.rowcount; i++)
				{
					var aRow = this.dsScrMap.setAddRow();
					this.dsScrMap.setColumn(aRow, "SCR_ID",    this.dsPopRtn.getColumn(i,"SCR_ID"));
					this.dsScrMap.setColumn(aRow, "SCR_NO",    this.dsPopRtn.getColumn(i,"SCR_NO"));
					this.dsScrMap.setColumn(aRow, "SCR_NAME",  this.dsPopRtn.getColumn(i,"SCR_NAME"));
					this.dsScrMap.setColumn(aRow, "PJT_ID",    this.dsPopRtn.getColumn(i,"PJT_ID"));
					this.dsScrMap.setColumn(aRow, "REF_ID",    this.dsScnrTest.getColumn(0,"STEST_ID"));
				}
			}
			break;

		case "PJ4420_P01" :	//배치 JOB 검색
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				for(var i=0; i<this.dsPopRtn.rowcount; i++)
				{
					var aRow = this.dsBatchjobMap.setAddRow();
					this.dsBatchjobMap.setColumn(aRow, "BTJB_ID",    this.dsPopRtn.getColumn(i,"BTJB_ID"));
					this.dsBatchjobMap.setColumn(aRow, "BTJB_NO",    this.dsPopRtn.getColumn(i,"BTJB_NO"));
					this.dsBatchjobMap.setColumn(aRow, "BTJB_NAME",  this.dsPopRtn.getColumn(i,"BTJB_NAME"));
					this.dsBatchjobMap.setColumn(aRow, "PJT_ID",    this.dsPopRtn.getColumn(i,"PJT_ID"));
					this.dsBatchjobMap.setColumn(aRow, "REF_ID",    this.dsScnrTest.getColumn(0,"STEST_ID"));
				}
			}
			break;

		case "PJ4320_P01" :	//인터페이스 기능 검색
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				for(var i=0; i<this.dsPopRtn.rowcount; i++)
				{
					var aRow = this.dsInfMap.setAddRow();
					this.dsInfMap.setColumn(aRow, "INF_ID", this.dsPopRtn.getColumn(i, "INF_ID"));
					this.dsInfMap.setColumn(aRow, "INF_NO", this.dsPopRtn.getColumn(i, "INF_NO"));
					this.dsInfMap.setColumn(aRow, "INF_NAME", this.dsPopRtn.getColumn(i, "INF_NAME"));
					this.dsInfMap.setColumn(aRow, "PJT_ID", this.dsPopRtn.getColumn(i, "PJT_ID"));
					this.dsInfMap.setColumn(aRow, "REF_ID", this.dsScnrTest.getColumn(0, "STEST_ID"));
				}
			}
			break;
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "saveConfirm" :
			this.fnSave();
			break;

		case "alertChkPlanDtI" :
			this.divContents.form.calPlanEdate.setFocus();
			break;

		case "alertChkActlDtI" :
			this.divContents.form.calActlEdate.setFocus();
			break;

		case "alertEssDevUserDtI" :
			this.divContents.form.divDevUser.form.edtUserNm.setFocus();
			break;
	}
}

/***********************************************************************************
 * User Function
 ***********************************************************************************/
// component init
this.fnCompInit = function()
{
	this.dsCond.setColumn(0, "STEST_ID", this.getOwnerFrame().pStestId);

	// Grid 행상태(ROWFLAG) 이미지 처리 util 호출
	this.divContents.form.grdStDefect.displayRowType();
	this.divContents.form.grdScrMap.displayRowType();
	this.divContents.form.grdBatchjobMap.displayRowType();
	this.divContents.form.grdInfMap.displayRowType();

	// 공통코드 조회
	this.fnGetCmmCd();
	
	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divContents.form.divGrdTopBtn01.form.btnAdd];	// 쓰기
	Ex.util.setBtnVisible(this, 'write', arrBtn);

	arrBtn = [this.divContents.form.divGrdTopBtn01.form.btnDelete];	// 삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);
	
	arrBtn = [this.divContents.form.divGrdTopBtn02.form.btnAdd];	// 쓰기
	Ex.util.setBtnVisible(this, 'write', arrBtn);

	arrBtn = [this.divContents.form.divGrdTopBtn02.form.btnDelete];	// 삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);
	
	arrBtn = [this.divContents.form.divGrdTopBtn03.form.btnAdd];	// 쓰기
	Ex.util.setBtnVisible(this, 'write', arrBtn);

	arrBtn = [this.divContents.form.divGrdTopBtn03.form.btnDelete];	// 삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);

	arrBtn = [this.divBtnBottom.form.btnSave];	// 저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);

	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divBtnBottom, 'right', true);

	if(this.getOwnerFrame().pMode == "NEW")
	{
		var nRow = this.dsScnrTest.setInsertRow();
		this.dsScnrTest.setColumn(nRow, "P_PJT_ID",   this.getOwnerFrame().pPjtId);
		this.dsScnrTest.setColumn(nRow, "P_PJT_NAME", this.getOwnerFrame().pPjtNm);
		this.dsScnrTest.setColumn(nRow, "PJT_ID", this.getOwnerFrame().pSubPjtId);
		this.dsScnrTest.setColumn(nRow, "STEST_VERSION", undefined);
		this.dsScnrTest.setColumn(nRow, "STEP_SEQ", 1);
		this.divContents.form.cboBizId.setFocus();
		this.fnSetSubPjtCombo();	//업무 콤보 셋팅
	}
	else if(this.getOwnerFrame().pMode == "SEARCH")
	{
		this.divContents.form.btnSearchPjtPop.set_enable(false);
		this.divContents.form.cboSubProject.set_enable(false);

		this.fnSearch();
	}
}

