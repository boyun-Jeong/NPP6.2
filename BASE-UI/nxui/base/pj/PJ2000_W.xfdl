    <Objects>
      <Dataset id="dsPjMilestone" oncolumnchanged="dsOnColumnChanged"/>
      <Dataset id="dsMilestoneCode">
        <ColumnInfo>
          <Column id="CMM_CD" type="STRING" size="256"/>
          <Column id="CMM_CD_NM" type="STRING" size="256"/>
        </ColumnInfo>
      </Dataset>
      <Dataset id="dsMilestoneCodeGrid">
        <ColumnInfo>
          <Column id="CMM_CD" type="STRING" size="256"/>
          <Column id="CMM_CD_NM" type="STRING" size="256"/>
        </ColumnInfo>
      </Dataset>
      <Dataset id="dsCond">
        <ColumnInfo>
          <Column id="PJT_ID" type="STRING" size="256"/>
          <Column id="PJT_NM" type="STRING" size="256"/>
          <Column id="MILESTONE_CODE" type="STRING" size="256"/>
          <Column id="MILESTONE_NAME" type="STRING" size="256"/>
        </ColumnInfo>
