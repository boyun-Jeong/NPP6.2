this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	var strMode  = "SUM";
	var strType   = "";
	var isEvent  = false;
	
	if( e.cell == obj.getBindCellIndex("body","SUM_SC_PLAN_CNT") || e.cell == obj.getBindCellIndex("body","WEEK_SC_PLAN_CNT"))
	{
		if( e.cell == obj.getBindCellIndex("body","WEEK_SC_PLAN_CNT"))
		{
			strMode = "WEEK";
		}
		
		strType = "SC";
		isEvent = this.fn_popup_chk(strMode, strType, e.row);
		if(!isEvent) return false;
	}
	else if( e.cell == obj.getBindCellIndex("body","SUM_IF_PLAN_CNT") || e.cell == obj.getBindCellIndex("body","WEEK_IF_PLAN_CNT"))
	{
		if( e.cell == obj.getBindCellIndex("body","WEEK_IF_PLAN_CNT"))
		{
			strMode = "WEEK";
		}
		
		strType = "IF";
		isEvent = this.fn_popup_chk(strMode, strType, e.row);
		if(!isEvent) return false;
	}
	else if( e.cell == obj.getBindCellIndex("body","SUM_BP_PLAN_CNT") || e.cell == obj.getBindCellIndex("body","WEEK_BP_PLAN_CNT"))
	{
		if( e.cell == obj.getBindCellIndex("body","WEEK_BP_PLAN_CNT"))
		{
			strMode = "WEEK";
		}
		
		strType = "BP";
		isEvent = this.fn_popup_chk(strMode, strType, e.row);
		if(!isEvent) return false;
	}
	else if( e.cell == obj.getBindCellIndex("body","SUM_PR_PLAN_CNT") || e.cell == obj.getBindCellIndex("body","WEEK_PR_PLAN_CNT"))
	{
		if( e.cell == obj.getBindCellIndex("body","WEEK_PR_PLAN_CNT"))
		{
			strMode = "WEEK";
		}
		
		strType = "PR";
		isEvent = this.fn_popup_chk(strMode, strType, e.row);
		if(!isEvent) return false;
	}
	else if( e.cell == obj.getBindCellIndex("body","SUM_RT_PLAN_CNT") || e.cell == obj.getBindCellIndex("body","WEEK_RT_PLAN_CNT"))
	{
		if( e.cell == obj.getBindCellIndex("body","WEEK_RT_PLAN_CNT"))
		{
			strMode = "WEEK";
		}
		
		strType = "RT";
		isEvent = this.fn_popup_chk(strMode, strType, e.row);
		if(!isEvent) return false;
	}
	
	if( isEvent )
	{
		var oTitle = {SUM  : "누적", WEEK : "주간"};

		Ex.core.popup(
			this,					// <--- 팝업 실행 스코프
			"PJ4800_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			"pj::PJ4800_P01.xfdl",	// <--- 팝업창 오픈 Url
			{
				title	: oTitle[strMode] + " - 개발진척현황 상세",
				pPjtId	: this.dsProgram.getColumn(e.row, "PJT_ID"),
				pBizId	: this.dsProgram.getColumn(e.row, "BIZ_ID"),
				pMode	: strMode,
				pBdate	: this.fv_bdate,
				pType	: strType
			},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
			"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
};

