// component init
this.fnCompInit = function()
{
	this.dsCond.setColumn(0, "PRG_ID", this.getOwnerFrame().pPrgId);

	// Grid 행상태(ROWFLAG) 이미지 처리 util 호출
	this.divContents.form.grdFuncMap.displayRowType();

	// 공통코드 조회
	this.fnGetCmmCd();

	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divContents.form.divGrdTopBtn.form.btnAdd];	// 쓰기
	Ex.util.setBtnVisible(this, 'write', arrBtn);

	arrBtn = [this.divContents.form.divGrdTopBtn.form.btnDelete];		// 삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);

	arrBtn = [this.divBtnBottom.form.btnSave];	// 저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);

	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divBtnBottom, 'right', true);

	if(this.getOwnerFrame().pMode == "NEW")
	{
		var nRow = this.dsProgram.setInsertRow();
		this.dsProgram.setColumn(nRow, "P_PJT_ID",   this.getOwnerFrame().pPjtId);
		this.dsProgram.setColumn(nRow, "P_PJT_NAME", this.getOwnerFrame().pPjtNm);
		this.dsProgram.setColumn(nRow, "PJT_ID", this.getOwnerFrame().pSubPjtId);
		this.divContents.form.cboBizId.setFocus();
		this.fnSetSubPjtCombo();	//업무 콤보 셋팅
	}
	else if(this.getOwnerFrame().pMode == "SEARCH")
	{
		this.divContents.form.btnSearchPjtPop.set_enable(false);
		this.divContents.form.cboSubProject.set_enable(false);

		this.fnSearch();
	}
}

// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd	: ['PRG_TYPE_CD']		// 조회할 상위코드
		,codeDiv	: ['PJT_MNG_CD']		// 조회할 코드의 업무구분
		,optStr		: ['SEL']				// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn		: ['Y']					// 사용여부
		,filter		: ['']					// filterStr
		,objDs		: [this.dsPrgTypeCd]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	if( Ex.isEmpty((this.dsProgram.getColumn(0, "P_PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}

	if( Ex.isEmpty((this.dsProgram.getColumn(0, "PJT_ID"))) )
	{
		var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return;
	}
	
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsProgram) && !Ex.util.isUpdated(this.dsFuncMap) )
	{
		return false;
	}

	//기간체크
	if( !Ex.isEmpty(this.dsProgram.getColumn(0, "PLAN_SDATE")) && !Ex.isEmpty(this.dsProgram.getColumn(0, "PLAN_EDATE")) )
	{
		if(this.dsProgram.getColumn(0, "PLAN_SDATE") > this.dsProgram.getColumn(0, "PLAN_EDATE"))
		{
			var oParam = {id : "alertChkPlanDtI", msg : "계획종료일은(는) 계획시작일보다 커야 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	if( !Ex.isEmpty(this.dsProgram.getColumn(0, "ACTL_SDATE")) && !Ex.isEmpty(this.dsProgram.getColumn(0, "ACTL_EDATE")) )
	{
		if(this.dsProgram.getColumn(0, "ACTL_SDATE") > this.dsProgram.getColumn(0, "ACTL_EDATE"))
		{
			var oParam = {id : "alertChkActlDtI", msg : "실제종료일은(는) 실제시작일보다 커야 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	if( !Ex.isEmpty(this.dsProgram.getColumn(0, "ACTL_SDATE")) || !Ex.isEmpty(this.dsProgram.getColumn(0, "ACTL_EDATE")) )
	{
		if( Ex.isEmpty(this.dsProgram.getColumn(0, "DEV_USER_ID")) )
		{
			var oParam = {id : "alertEssDevUserDtI", msg : "실제[시작일/종료일]을 입력하려면 개발자은(는) 필수입력 항목입니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	var findRow = this.dsFuncMap.findRowExpr("PJT_ID != '" + this.dsProgram.getColumn(0, "PJT_ID") + "'");
	if(findRow > -1)
	{
		var oParam = {id : "alertChkPjtIdI", msg : "선택된 서브프로젝트 정보와 어플리케이션 기능의 서브프로젝트 정보가 다릅니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	//if( !this.divContents.form.grdFuncMap.checkValidate(true) ) return false;
	return true;
} 

