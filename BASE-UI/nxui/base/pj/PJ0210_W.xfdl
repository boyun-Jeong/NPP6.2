              <Static id="Static00" taborder="2" text="진행상태" onclick="Div00_Static01_onclick" cssclass="sta_WF_schTitle" left="Static00_00_00:0" top="13" width="64" height="24"/>
              <Static id="Static11" taborder="6" text="프로젝트 명" onclick="Div00_Static01_onclick" cssclass="sta_WF_schTitle" left="427" top="13" width="81" height="24" countDataset="sta_WF_schTitle"/>
              <Static id="Static00_00" taborder="7" left="0" top="13" background="rgba(173,142,219,0.4)" text="20" textAlign="center" verticalAlign="middle" font="normal 12pt/normal &quot;Arial&quot;" visible="false" width="20" height="24"/>
              <Static id="Static07" taborder="3" text="년도" onclick="Div00_Static01_onclick" cssclass="sta_WF_schTitle" left="Static00_00:0" top="13" width="49" height="24" countDataset="sta_WF_schTitle"/>
              <Static id="Static03" taborder="4" text="FIX h12" cssclass="Guide_Color" visible="false" left="0" top="66" width="991" height="12"/>
              <Static id="Static09" taborder="5" text="FIX w30" cssclass="Guide_Color" visible="false" left="324" top="0" width="30" height="50"/>
              <Static id="Static00_00_00_00" taborder="9" left="397" top="13" background="rgba(173,142,219,0.4)" text="30" textAlign="center" verticalAlign="middle" font="normal 12pt/normal &quot;Arial&quot;" visible="false" width="30" height="24"/>
              <Calendar id="calYyyy" taborder="0" type="spin" dateformat="yyyy" editformat="yyyy" onchanged="divSearch_calYyyy_onchanged" left="Static07:0" top="13" width="96" height="24" cssclass="spn_WF_normal"/>
              <Combo id="cboProcStsCd" taborder="1" innerdataset="@dsProcStsCd" codecolumn="CMM_CD" datacolumn="CMM_CD_NM" onitemchanged="cmb_actCd_onitemchanged" left="Static00:0" top="13" width="138" height="24" cssclass="cbo_WF_normal"/>
              <Button id="btnSearch" taborder="11" top="13" width="75" height="24" text="조회" right="2" cssclass="btn_WF_search" onclick="fnCommOnclick"/>
              <Button id="btnSearchReset" taborder="10" top="13" width="90" height="24" text="초기화" right="btnSearch:5" cssclass="btn_WF_searchReset" onclick="fnCommOnclick"/>
              <Edit id="edtPjtNm" taborder="12" left="Static11:0" top="13" width="215" height="24" cssclass="edt_WF_normal" onkeyup="onKeyUp"/>
            </Layout>
          </Layouts>
