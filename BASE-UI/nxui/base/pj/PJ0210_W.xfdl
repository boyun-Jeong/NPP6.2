          <Column id="ERROR" type="STRING" size="255"/>
        </ColumnInfo>
        <Rows/>
      </Dataset>
      <Dataset id="dsProcStsCd"/>
      <Dataset id="dsBusiKindCd"/>
      <Dataset id="dsDevGradeCd"/>
      <Dataset id="dsCond">
        <ColumnInfo>
          <Column id="PJT_YEAR" type="STRING" size="256"/>
          <Column id="PJT_STATUS_CD" type="STRING" size="256"/>
          <Column id="PJT_ID" type="STRING" size="256"/>
