        </Div>
        <Static id="Static19" taborder="3" cssclass="Guide_Color" visible="false" left="0" right="0" height="20" bottom="64"/>
        <Static id="sta_listCnt" taborder="4" left="sta_userInfo:5" top="73" width="100" height="24" style="align:left middle;" countDataset="dsPjProject"/>
        <Static id="Static00_01" taborder="5" left="0" top="50" height="20" background="rgba(173,142,219,0.4)" text="20" textAlign="center" verticalAlign="middle" font="normal 12pt/normal &quot;Arial&quot;" visible="false" right="0"/>
        <Static id="Static00_00_00" taborder="6" left="0" top="94" height="10" background="rgba(173,142,219,0.4)" text="10" textAlign="center" verticalAlign="middle" font="normal 12pt/normal &quot;Arial&quot;" visible="false" right="0"/>
        <Div id="divGrdBotBtn" taborder="7" width="200" height="32" bottom="0" right="0">
          <Layouts>
            <Layout>
              <Button id="btnAddSubPjt" taborder="0" text="등록" width="80" height="32" bottom="0" cssclass="btn_WF_primary_save" right="0" onclick="fnCommOnclick"/>
            </Layout>
          </Layouts>
        </Div>
        <Div id="divGrdTopBtn" taborder="8" width="97" height="24" visible="true" isButtonGroup="true" top="70" right="0">
          <Layouts>
            <Layout>
              <Button id="btnExcelDown" taborder="0" text="엑셀" top="0" width="60" right="0" cssclass="btn_WF_excelDown" onclick="fnCommOnclick" height="24" visible="false"/>
