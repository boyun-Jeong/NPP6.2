                <Cell col="4" displaytype="combotext" edittype="none" style="align:left;" text="bind:PJT_TYPE_CD" combodataset="dsBusiKindCd" combocodecol="CMM_CD" combodatacol="CMM_CD_NM"/>
                <Cell col="5" displaytype="combotext" style="align:middle;" text="bind:PJT_SIZE_CD" combodataset="dsDevGradeCd" combocodecol="CMM_CD" combodatacol="CMM_CD_NM"/>
                <Cell col="6" displaytype="normal" text="expr:SPV_YN=='Y' ? '여' : SPV_YN=='N' ? &quot;부&quot; : ''"/>
                <Cell col="7" text="bind:UPMU_USER_NM"/>
                <Cell col="8" displaytype="normal" edittype="none" style="align:middle;selectfont: ;" text="bind:IN_PM_USER_NM"/>
                <Cell col="9" style="align:left;" text="bind:OUT_CPY_NAME" textAlign="left"/>
                <Cell col="10" edittype="none" text="bind:OUT_PM_USER_NM"/>
              </Band>
            </Format>
          </Formats>
        </Grid>
        <Static id="sta_userInfo" taborder="2" text="프로젝트조회" cssclass="sta_WF_gridTitle" left="0" top="70" width="101" height="24"/>
        <Div id="divSearch" taborder="0" left="0" top="0" right="0" height="50" scrollbars="none" cssclass="div_WF_divSearchArea">
