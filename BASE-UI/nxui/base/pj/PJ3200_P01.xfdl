    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ3200_P01
* 화면(명)		︰ 이슈관리 상세
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.20
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.20	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
this.dSaveChkRow;				//저장시 Validaton Check에 걸린경우 해당 row정보를 저장
// 실행활동 이력 첨부파일 setting
this.actFileParam = {
	 sSvcId		: 'actFileSvc'			// divFile callback 후처리용 서비스 ID
	,limitMinCnt: 0						// 파일 최소 갯수; defualt 0
	,limitCnt	: 3						// 파일 최대 갯수; default 5
	,limitSize	: 100					// 첨부파일 개당 용량 제한(단위 MB); default 100 MB
	,folderName	: 'pj'					// 폴더명(업무명)
	,tableName	: 'PJ_ISSUE_ACTION'		// 구분명(ex. 화면명 + "_" + 첨부파일 Div ID)
	,primaryKey1: ''					// 테이블 PK1(ex. 요청서ID 등)
	,primaryKey2: ''					// 테이블 PK2 (복합 키인 경우 사용)
	,primaryKey3: ''					// 테이블 PK3 (복합 키인 경우 사용)
	,fileDownYn	: ''					// 파일다운권한; 	 공백 시 메뉴의 조회 권한
	,fileUpYn	: ''					// 파일업로드권한; 공백 시 메뉴의 추가 권한
	,deleteYn	: ''					// 파일삭제권한; 	 공백 시 메뉴의 삭제 권한
};
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ3200_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	trace("this.getOwnerFrame().id : " + this.getOwnerFrame().id);
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ3200_P01] this.fnSearch()");
	var sTranId = "select01";																		// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ3200P/select01";																// 서비스명
    var sInDs = "dsCond=dsCond";																	// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsIssue=dsIssue dsIssueAction=dsIssueAction dsFileList=dsFileList";	// 서버에서 수신할 데이타셋
    var sArg = "";																					// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ3200_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ3200P/save01";
    var sInDs = "dsCond=dsCond dsIssue=dsIssue:U dsIssueAction=dsIssueAction:A";
	var sOutDs = "dsIssue=dsIssue dsIssueAction=dsIssueAction dsFileList=dsFileList dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
	{
		objDs.setDeleteRow(i);
	}
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ3200_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);

	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "issueFileSvc" :
			this.divFilesAct.form.fnUploadFiles(true);
			break;

		case "actFileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			
			var strRegUserId    = this.dsIssue.getColumn(0, "REG_USER_ID");
			var strRegUserNm    = this.dsIssue.getColumn(0, "REG_USER_NM");
			var strRegDept      = this.dsIssue.getColumn(0, "REG_USER_DEPT");
			this.divContents.form.divRegUser.form.setValue(strRegUserId, strRegUserNm, strRegDept);
			
			var strChgUserId    = this.dsIssue.getColumn(0, "CHG_USER_ID");
			var strChgUserNm    = this.dsIssue.getColumn(0, "CHG_USER_NM");
			var strChgDept      = this.dsIssue.getColumn(0, "CHG_USER_DEPT");
			this.divContents.form.divChgUser.form.setValue(strChgUserId, strChgUserNm, strChgDept);
			
			this.setGridFileInfo();

			this.actFileParam.primaryKey1 = this.dsIssue.getColumn(0, "ISSUE_ID")||'';
			this.divFilesAct.form.setConfig(this, this.actFileParam);
			this.divFilesAct.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));

			this.divContents.form.grdIssueAction.setCellProperty("Head", this.divContents.form.grdIssueAction.getBindCellIndex('Body', 'CHK'), "text", "0");

			this.fn_setEditMode();
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			this.divContents.form.grdIssueAction.setCellProperty("Head", this.divContents.form.grdIssueAction.getBindCellIndex('Body', 'CHK'), "text", "0");
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "ISSUE_ID")) )
			{
				this.getOwnerFrame().pIssueId	= this.dsCond.getColumn(0, "ISSUE_ID");
				this.getOwnerFrame().pMode = "SEARCH";
				this.divContents.form.divFiles.form.fnReplaceTempkeyAndSearch(this.dsCond.getColumn(0, "ISSUE_ID"));
			}
			
			var strRegUserId    = this.dsIssue.getColumn(0, "REG_USER_ID");
			var strRegUserNm    = this.dsIssue.getColumn(0, "REG_USER_NM");
			var strRegDept      = this.dsIssue.getColumn(0, "REG_USER_DEPT");
			this.divContents.form.divRegUser.form.setValue(strRegUserId, strRegUserNm, strRegDept);
			
			var strChgUserId    = this.dsIssue.getColumn(0, "CHG_USER_ID");
			var strChgUserNm    = this.dsIssue.getColumn(0, "CHG_USER_NM");
			var strChgDept      = this.dsIssue.getColumn(0, "CHG_USER_DEPT");
			this.divContents.form.divChgUser.form.setValue(strChgUserId, strChgUserNm, strChgDept);

			this.setGridFileInfo();

			this.actFileParam.primaryKey1 = this.dsIssue.getColumn(0, "ISSUE_ID")||'';
			this.divFilesAct.form.setConfig(this, this.actFileParam);
			this.divFilesAct.form.dsFommFile.assign(this.dsFileList);
			
			this.divContents.form.grdIssueAction.setCellProperty("Head", this.divContents.form.grdIssueAction.getBindCellIndex('Body', 'CHK'), "text", "0");
			break;
	}
}
// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "comFilePop" :
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsFileList.loadXML(varValue);
				
				for(var i=0; i<this.dsFileList.rowcount; i++)	this.dsFileList.setColumn(i, "CHK", "0");

				this.divFilesAct.form.dsFommFile.set_enableevent(false);
				this.divFilesAct.form.dsFommFile.set_updatecontrol(false);
				
				this.divFilesAct.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));
				for(var i=0; i<this.divFilesAct.form.dsFommFile.rowcount; i++)
				{
					var rowFlag = this.divFilesAct.form.dsFommFile.getColumn(i, "ROWFLAG");
					trace("i=" + i + " rowFlag : " + rowFlag);

					if(rowFlag == "I")
					{
						this.divFilesAct.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_INSERT);
					}
					else if(rowFlag == "D")
					{
						this.divFilesAct.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_UPDATE);
					}
				}
				
				this.setGridFileInfo(this.dsIssueAction.rowposition);				
				//trace(this.divFilesAct.form.dsFommFile.saveXML());
			}
			break;

		case "selectUserPop" :
			trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsIssueAction.setColumn(this.dsIssueAction.rowposition, "ACT_USER_ID",   this.dsPopRtn.getColumn(0, "USER_ID"));
				this.dsIssueAction.setColumn(this.dsIssueAction.rowposition, "ACT_USER_NM",   this.dsPopRtn.getColumn(0, "USER_NM"));
				this.dsIssueAction.setColumn(this.dsIssueAction.rowposition, "ACT_USER_DEPT", this.dsPopRtn.getColumn(0, "DEPT_NM"));
			}
			break;

		case "PJ0600_P01" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsIssue.setColumn(0, "MEET_ID", this.dsPopRtn.getColumn(0, "MEET_ID"));
			}
			break;

		case "PJ3210_P01" :
			trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsIssue.setColumn(0, "RISK_ID", this.dsPopRtn.getColumn(0, "RISK_ID"));
				this.dsIssue.setColumn(0, "RISK_NO", this.dsPopRtn.getColumn(i, "RISK_NO"));
				this.dsIssue.setColumn(0, "RISK_NAME", this.dsPopRtn.getColumn(i, "RISK_NAME"));
			}
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "saveConfirm" :
			for(var i=this.dsFileList.rowcount-1; i>=0; i--)
			{
				var pk2 = this.dsFileList.getColumn(i, "PK2");
				
				var fRow = this.dsIssueAction.findRowExpr("ACT_ID", pk2);
				
				if(fRow == -1)
				{
					this.divFilesAct.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
					this.dsFileList.setDeleteRow(i);
				}
				else
				{
					if(this.dsIssueAction.getColumn(fRow, "ROWFLAG") == "D")
					{
						this.divFilesAct.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
						this.dsFileList.setDeleteRow(i);
					}
				}
			}
			this.divContents.form.divFiles.form.fnUploadFiles(true);
			break;

		case "alertCboActCdStsI" :
			this.dsPjActCd.setColumn(0, "ACTION_CD", "AITM_PROC_STS_CD02");
			this.divContents.form.cboActCd.setFocus();
			break;

		case "alertEdtimeI" :
			this.divContents.form.calCplPlanTime.setFocus();
			break;

		case "alertChkCplActTimeI" :
			this.divContents.form.calCplActTime.setFocus();
			break;

		case "alertChkGrdStsI" :
			this.grdIssueAction.getBindDataset().set_rowposition(this.dSaveChkRow);			// 데이터셋 ROW 포커스 이동
			this.grdIssueAction.showEditor(true);											// 그리드 편집모드로 변경
			var cellIdx = this.grdIssueAction.getBindCellIndex("Body", "ACT_STATUS_CD");
			this.grdIssueAction.setCellPos(cellIdx, this.dSaveChkRow);						// 그리드 셀 포커스 이동
			break;
	}
}

