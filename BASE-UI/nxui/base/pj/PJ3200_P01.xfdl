this.setGridFileInfo = function(row)
{
	var startRow = 0;
	var endRow	= this.dsIssueAction.rowcount;
	
	if( !Ex.isEmpty(row) )
	{
		startRow = row;
		endRow = startRow + 1;
	}

	trace("this.setGridFileInfo() row : " + row + " startRow : " + startRow + " endRow : " + endRow);
	
	this.dsIssueAction.set_enableevent(false);
	this.dsIssueAction.set_updatecontrol(false);

	for(var i=startRow; i<endRow; i++)
	{
		this.dsFileList.set_filterstr("PK1=='" + (this.dsIssueAction.getColumn(i, "ISSUE_ID")||('TEMP_KEY_' + Ex.util.getSession('gvUserId'))) +
									  "' && PK2=='" + this.dsIssueAction.getColumn(i, "ACT_ID") + "'");
									  
		trace("i=" + i + " this.dsFileList.rowcount : " + this.dsFileList.rowcount);
		if(this.dsFileList.rowcount == 1)
		{
			this.dsIssueAction.setColumn(i, "ORG_FILE_NM", this.dsFileList.getColumn(0, "FILE_NM"));
		}
		else if(this.dsFileList.rowcount > 1)
		{
			this.dsIssueAction.setColumn(i, "ORG_FILE_NM", this.dsFileList.getColumn(0, "FILE_NM") + " 외 " + (this.dsFileList.rowcount-1) + "건");
		}
		else
		{
			this.dsIssueAction.setColumn(0, "ORG_FILE_NM", "");
		}		
	}
	
	this.dsIssueAction.set_updatecontrol(true);
	this.dsIssueAction.set_enableevent(true);

	this.dsFileList.set_filterstr("");
}
