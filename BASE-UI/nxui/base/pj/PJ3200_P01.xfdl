// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd	: ['ISU_P_STS_CD',		'ISSUE_TYPE_CD',		'IS_EFFE_LVL_CD',		'I_MES_PROC_STS'		]	// 조회할 상위코드
		,codeDiv	: ['PJT_MNG_CD',		'PJT_MNG_CD',			'PJT_MNG_CD',			'PJT_MNG_CD'			]	// 조회할 코드의 업무구분
		,optStr		: ['SEL',				'SEL',					'SEL',					'SEL'					]	// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn		: ['Y',					'Y',					'Y',					'Y'						]	// 사용여부
		,filter		: ['',					'',						'',						''						]	// filterStr
		,objDs		: [this.dsIssueStatusCd, this.dsIssueTypeCd,	this.dsIssueImpactCd,	this.dsActionStatusCd	]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsIssue) &&
		!Ex.util.isUpdated(this.dsIssueAction) &&
		!this.divContents.form.divFiles.form.isUpdateForm() &&
		!this.divFilesAct.form.isUpdateForm())
	{
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	
	var msgNo;

	//등록자 입력 체크
	var rtnVal = this.divContents.form.divRegUser.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divRegUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertRegUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcRegUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//담당자 입력 체크
	rtnVal = this.divContents.form.divChgUser.form.fnCheckValidate();

	trace("divChgUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkChgUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcChgUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	
	if(this.dsIssue.getColumn(0, "ISSUE_STIME") > this.dsIssue.getColumn(0, "CPL_PLAN_TIME"))
	{
		var oParam = {id : "alertEdtimeI", msg : "완료예정일은(는) 식별일자보다 작게 입력 불가능 합니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !this.divContents.form.grdIssueAction.checkValidate(true) ) return false;

	//진행상태가 완료인 경우 
	if(this.divContents.form.cboIssueStatusCd.value == "ISU_P_STS_CD03")
	{
		//실제완료일 필수 입력
		if( Ex.isEmpty(this.dsIssue.getColumn(0, "CPL_ACT_TIME")) )
		{
			var oParam = {id : "alertChkCplActTimeI", msg : "진행상태가 완료인 경우 실제완료일을 입력 하세요.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		//위험활동이력 완료여부 체크
		var fRow = this.dsIssueAction.findRowExpr("ACT_STATUS_CD != 'ISU_P_STS_CD03'");
		if( fRow > -1)
		{
			this.dSaveChkRow = fRow;
			var oParam = {id : "alertChkGrdStsI", msg : "진행상태가 완료인 경우 이슈해결 이력이 모두 완료되야 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}
	return true;
} 

