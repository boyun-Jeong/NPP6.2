					pPjtId		: this.dsIssue.getColumn(0, "PJT_ID"),
					pPjtNm		: this.dsIssue.getColumn(0, "PJT_NAME")
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnAdd" : 	// 저장
			var nRow = this.dsIssueAction.setInsertRow();
			this.dsIssueAction.setColumn(nRow, "ACT_ID", Ex.util.getUniqueId("ACT_ID_"));
			this.dsIssueAction.setColumn(nRow, "ISSUE_ID", this.dsIssue.getColumn(0, "ISSUE_ID"));
			break;

		case "btnDelete" : 	// 삭제
			this.fnDelete(this.dsIssueAction);
			break;

		case "btnSave" : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	obj.dropdownCalendar();
	obj.dropdownCombo();
};

this.grdOnExpandUp = function(obj:nexacro.Grid,e:nexacro.GridMouseEventInfo)
{
	if (e.cell == obj.getBindCellIndex("body","ACT_USER_NM"))
	{
		Ex.core.popup(
			this,
			'selectUserPop',
			"fomm::FOMM0000_P01.xfdl",             		// <--- 팝업창 오픈 Url
			{
				title		: Ex.core.word('사용자 선택'),
				pSelType	: 'S',
				pType		: "name"
			},											// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"width=900,height=500"                  	// <--- width/height/modeless(프레임 처리 옵션)							
		);
	}
	else if ( e.cell == obj.getBindCellIndex("body","ORG_FILE_NM"))
	{
		this.actFileParam.primaryKey1 = this.dsIssue.getColumn(0, "ISSUE_ID")||('TEMP_KEY_' + Ex.util.getSession('gvUserId'));
		this.actFileParam.primaryKey2 = this.dsIssueAction.getColumn(this.dsIssueAction.rowposition, "ACT_ID")||'';
		trace("this.actFileParam.primaryKey1 : " + this.actFileParam.primaryKey1 + "\nthis.actFileParam.primaryKey2 : " + this.actFileParam.primaryKey2);

		Ex.core.popup(
			this,
			'comFilePop',
			"comm::comFilePop.xfdl",             		// <--- 팝업창 오픈 Url
			{
				title			: Ex.core.word('위험활동 이력 - 첨부파일'),
				pTargetFileDiv	: this.divFilesAct,
				pDsObj			: this.dsFileList,
				pIsSearch		: false,
				pBtnConfVisible	: true,
				pFileCfg		: this.actFileParam,
				pFilterStr		: "PK1=='" + this.actFileParam.primaryKey1 + "' && PK2=='" + this.actFileParam.primaryKey2 + "'"
			},											// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize,bCloseButton=false"				// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	obj.setUpdateRow(e);
};

this.onBeforeClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	if(!this.isCloseBtnClick) if(this.saveFlg == "Y") this.getOwnerFrame()._formObj.fnSearch();
};]]></Script>
