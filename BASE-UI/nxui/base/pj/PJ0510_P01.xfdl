this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsPjActionItem) &&
		!Ex.util.isUpdated(this.dsPjActAction) &&
		!this.divContents.form.divFiles.form.isUpdateForm() &&
		!this.divFilesAct.form.isUpdateForm())
	{
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	
	var msgNo;

	//등록자 입력 체크
	var rtnVal = this.divContents.form.divRegUser.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divRegUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertRegUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcRegUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//요청자 입력 체크
	rtnVal = this.divContents.form.divReqUser.form.fnCheckValidate();

	trace("divReqUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkReqUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcReqUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//실행자 입력 체크
	rtnVal = this.divContents.form.divActUser.form.fnCheckValidate();

	trace("divActUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkActUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcActUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !this.divContents.form.grdMain.checkValidate(true) ) return false;
	return true;
} 

this.fn_setEditMode = function()
{
	if( !Ex.isEmpty(this.getOwnerFrame().pEditMode) && this.getOwnerFrame().pEditMode == "read")
