			},											// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"width=900,height=500"                  	// <--- width/height/modeless(프레임 처리 옵션)							
		);
	}
	else if ( e.cell == obj.getBindCellIndex("body","ORG_FILE_NM"))
	{
		this.actFileParam.primaryKey1 = this.dsPjActionItem.getColumn(0, "ACT_ITEM_ID")||('TEMP_KEY_' + Ex.util.getSession('gvUserId'));
		this.actFileParam.primaryKey2 = this.dsPjActAction.getColumn(this.dsPjActAction.rowposition, "ACT_ID")||'';
		trace("this.actFileParam.primaryKey1 : " + this.actFileParam.primaryKey1 + "\nthis.actFileParam.primaryKey2 : " + this.actFileParam.primaryKey2);

		Ex.core.popup(
			this,
			'comFilePop',
			"comm::comFilePop.xfdl",             		// <--- 팝업창 오픈 Url
			{
				title			: Ex.core.word('실행활동 이력 - 첨부파일'),
				pTargetFileDiv	: this.divFilesAct,
				pDsObj			: this.dsFileList,
				pIsSearch		: false,
				pBtnConfVisible	: true,
				pFileCfg		: this.actFileParam,
				pFilterStr		: "PK1=='" + this.actFileParam.primaryKey1 + "' && PK2=='" + this.actFileParam.primaryKey2 + "'"
			},											// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize,bCloseButton=false"				// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(obj.id == "dsPjActAction")
	{
		if(e.columnid == "ACT_STATUS_CD")
		{
			if(e.newvalue == "AITM_PROC_STS_CD02" && this.divContents.form.cboActCd.value != "AITM_PROC_STS_CD02" )
			{
				var oParam = {id : "alertCboActCdStsI", msg : "활동이력이 진행중이면 실행상태가 진행중으로 변경됩니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return false;
			}
		}
	}
	obj.setUpdateRow(e);
};

this.onBeforeClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	if(!this.isCloseBtnClick) if(this.saveFlg == "Y") this.getOwnerFrame()._formObj.fnSearch();
};]]></Script>
