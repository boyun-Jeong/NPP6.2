// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnMeetSearchPop" :
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0600_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0600_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title		: "회의 검색 팝업",
					pSelType	: 'S',
					pPjtId		: this.dsCond.getColumn(0, "PJT_ID")
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"				// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnAdd" : 	// 저장
			var nRow = this.dsPjActAction.setInsertRow();
			this.dsPjActAction.setColumn(nRow, "ACT_ID", Ex.util.getUniqueId("ACT_ID_"));
			this.dsPjActAction.setColumn(nRow, "ACT_ITEM_ID", this.dsPjActionItem.getColumn(0, "ACT_ITEM_ID"));
			break;

		case "btnDelete" : 	// 삭제
			this.fnDelete(this.dsPjActAction);
			break;

		case "btnSave" : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

