    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ0510_P01
* 화면(명)		︰ Action Item 상세
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.06
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.06	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
// 실행활동 이력 첨부파일 setting
this.actFileParam = {
	 sSvcId		: 'actFileSvc'			// divFile callback 후처리용 서비스 ID
	,limitMinCnt: 0					// 파일 최소 갯수; defualt 0
	,limitCnt	: 3					// 파일 최대 갯수; default 5
	,limitSize	: 100				// 첨부파일 개당 용량 제한(단위 MB); default 100 MB
	,folderName	: 'pj'				// 폴더명(업무명)
	,tableName	: 'PJ_ACT_ACTION'	// 구분명(ex. 화면명 + "_" + 첨부파일 Div ID)
	,primaryKey1: ''				// 테이블 PK1(ex. 요청서ID 등)
	,primaryKey2: ''				// 테이블 PK2 (복합 키인 경우 사용)
	,primaryKey3: ''				// 테이블 PK3 (복합 키인 경우 사용)
	,fileDownYn	: ''				// 파일다운권한; 	 공백 시 메뉴의 조회 권한
	,fileUpYn	: ''				// 파일업로드권한; 공백 시 메뉴의 추가 권한
	,deleteYn	: ''				// 파일삭제권한; 	 공백 시 메뉴의 삭제 권한
};
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ0510_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	trace("this.getOwnerFrame().id : " + this.getOwnerFrame().id);
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ0510_P01] this.fnSearch()");
	var sTranId = "select01";																		// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ0510P/select01";																// 서비스명
    var sInDs = "dsCond=dsCond";																	// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjActionItem=dsPjActionItem dsPjActAction=dsPjActAction dsFileList=dsFileList";	// 서버에서 수신할 데이타셋
    var sArg = "";																					// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ0510_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ0510P/save01";
    var sInDs = "dsCond=dsCond dsPjActionItem=dsPjActionItem:U dsPjActAction=dsPjActAction:A";
	var sOutDs = "dsPjActionItem=dsPjActionItem dsPjActAction=dsPjActAction dsFileList=dsFileList dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
	{
		objDs.setDeleteRow(i);
	}
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ0510_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);

	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "actItemFileSvc" :
			this.divFilesAct.form.fnUploadFiles(true);
			break;

		case "actFileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			
			var strRegUserId    = this.dsPjActionItem.getColumn(0, "REG_USER_ID");
			var strRegUserNm    = this.dsPjActionItem.getColumn(0, "REG_USER_NM");
			var strRegDept      = this.dsPjActionItem.getColumn(0, "REG_USER_DEPT");
			this.divContents.form.divRegUser.form.setValue(strRegUserId, strRegUserNm, strRegDept);
			
			var strReqUserId    = this.dsPjActionItem.getColumn(0, "REQ_USER_ID");
			var strReqUserNm    = this.dsPjActionItem.getColumn(0, "REQ_USER_NM");
			var strReqDept      = this.dsPjActionItem.getColumn(0, "REQ_USER_DEPT");
			this.divContents.form.divReqUser.form.setValue(strReqUserId, strReqUserNm, strReqDept);
			
			var strActUserId    = this.dsPjActionItem.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsPjActionItem.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsPjActionItem.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			this.setGridFileInfo();

			this.actFileParam.primaryKey1 = this.dsPjActionItem.getColumn(0, "ACT_ITEM_ID")||'';
			this.divFilesAct.form.setConfig(this, this.actFileParam);
			this.divFilesAct.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));
			
			this.fn_setEditMode();
			
			this.divContents.form.grdMain.setCellProperty("Head", this.divContents.form.grdMain.getBindCellIndex('Body', 'CHK'), "text", "0");
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "ACT_ITEM_ID")) )
			{
				this.getOwnerFrame().pActItemCd	= this.dsCond.getColumn(0, "ACT_ITEM_ID");
				this.getOwnerFrame().pNew = "N";
				this.divContents.form.divFiles.form.fnReplaceTempkeyAndSearch(this.dsCond.getColumn(0, "ACT_ITEM_ID"));
			}
			
			var strRegUserId    = this.dsPjActionItem.getColumn(0, "REG_USER_ID");
			var strRegUserNm    = this.dsPjActionItem.getColumn(0, "REG_USER_NM");
			var strRegDept      = this.dsPjActionItem.getColumn(0, "REG_USER_DEPT");
			this.divContents.form.divRegUser.form.setValue(strRegUserId, strRegUserNm, strRegDept);
			
			var strReqUserId    = this.dsPjActionItem.getColumn(0, "REQ_USER_ID");
			var strReqUserNm    = this.dsPjActionItem.getColumn(0, "REQ_USER_NM");
			var strReqDept      = this.dsPjActionItem.getColumn(0, "REQ_USER_DEPT");
			this.divContents.form.divReqUser.form.setValue(strReqUserId, strReqUserNm, strReqDept);
			
			var strActUserId    = this.dsPjActionItem.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsPjActionItem.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsPjActionItem.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			this.setGridFileInfo();

			this.actFileParam.primaryKey1 = this.dsPjActionItem.getColumn(0, "ACT_ITEM_ID")||'';
			this.divFilesAct.form.setConfig(this, this.actFileParam);
			this.divFilesAct.form.dsFommFile.assign(this.dsFileList);
			
			this.divContents.form.grdMain.setCellProperty("Head", this.divContents.form.grdMain.getBindCellIndex('Body', 'CHK'), "text", "0");
