	}
}
// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID, varValue)
{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "comFilePop" :
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsFileList.loadXML(varValue);
				
				for(var i=0; i<this.dsFileList.rowcount; i++)	this.dsFileList.setColumn(i, "CHK", "0");

				this.divFilesAct.form.dsFommFile.set_enableevent(false);
				this.divFilesAct.form.dsFommFile.set_updatecontrol(false);
				
				this.divFilesAct.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));
				for(var i=0; i<this.divFilesAct.form.dsFommFile.rowcount; i++)
				{
					var rowFlag = this.divFilesAct.form.dsFommFile.getColumn(i, "ROWFLAG");
					trace("i=" + i + " rowFlag : " + rowFlag);

					if(rowFlag == "I")
					{
						this.divFilesAct.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_INSERT);
					}
					else if(rowFlag == "D")
					{
						this.divFilesAct.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_UPDATE);
					}
				}
				
				this.setGridFileInfo(this.dsPjActAction.rowposition);				
				//trace(this.divFilesAct.form.dsFommFile.saveXML());
			}
			break;

		case "selectUserPop" :
			trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsPjActAction.setColumn(this.dsPjActAction.rowposition, "ACT_USER_ID",   this.dsPopRtn.getColumn(0, "USER_ID"));
				this.dsPjActAction.setColumn(this.dsPjActAction.rowposition, "ACT_USER_NM",   this.dsPopRtn.getColumn(0, "USER_NM"));
				this.dsPjActAction.setColumn(this.dsPjActAction.rowposition, "ACT_USER_DEPT", this.dsPopRtn.getColumn(0, "DEPT_NM"));
			}
			break;

		case "PJ0600_P01" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsPjActionItem.setColumn(0, "MEET_ID", this.dsPopRtn.getColumn(0, "MEET_ID"));
			}
			break;
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID, varValue)
{
	// TO DO
	if(!varValue) return;
	
	switch(mID)
	{
		case "saveConfirm" :
			for(var i=this.dsFileList.rowcount-1; i>=0; i--)
			{
				var pk2 = this.dsFileList.getColumn(i, "PK2");
				
				var fRow = this.dsPjActAction.findRow("ACT_ID", pk2);
				
				if(fRow == -1)
				{
					this.divFilesAct.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
					this.dsFileList.setDeleteRow(i);
				}
				else
				{
					if(this.dsPjActAction.getColumn(fRow, "ROWFLAG") == "D")
					{
						this.divFilesAct.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
						this.dsFileList.setDeleteRow(i);
					}
				}
			}
			this.divContents.form.divFiles.form.fnUploadFiles(true);
			break;

		case "alertCboActCdStsI" :
			this.dsPjActCd.setColumn(0, "ACTION_CD", "AITM_PROC_STS_CD02");
			this.divContents.form.cboActCd.setFocus();
			break;
	}
}

