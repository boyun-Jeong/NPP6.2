this.setGridFileInfo = function(row)
{
	var startRow = 0;
	var endRow	= this.dsPjActAction.rowcount;
	
	if( !Ex.isEmpty(row) )
	{
		startRow = row;
		endRow = startRow + 1;
	}

	trace("this.setGridFileInfo() row : " + row + " startRow : " + startRow + " endRow : " + endRow);
	
	this.dsPjActAction.set_enableevent(false);
	this.dsPjActAction.set_updatecontrol(false);

	for(var i=startRow; i<endRow; i++)
	{
		this.dsFileList.set_filterstr("PK1=='" + (this.dsPjActAction.getColumn(i, "ACT_ITEM_ID")||('TEMP_KEY_' + Ex.util.getSession('gvUserId'))) +
									  "' && PK2=='" + this.dsPjActAction.getColumn(i, "ACT_ID") + "'");
									  
		trace("i=" + i + " this.dsFileList.rowcount : " + this.dsFileList.rowcount);
		if(this.dsFileList.rowcount == 1)
		{
			this.dsPjActAction.setColumn(i, "ORG_FILE_NM", this.dsFileList.getColumn(0, "FILE_NM"));
		}
		else if(this.dsFileList.rowcount > 1)
		{
			this.dsPjActAction.setColumn(i, "ORG_FILE_NM", this.dsFileList.getColumn(0, "FILE_NM") + " 외 " + (this.dsFileList.rowcount-1) + "건");
		}
		else
		{
			this.dsPjActAction.setColumn(0, "ORG_FILE_NM", "");
		}		
	}
	
	this.dsPjActAction.set_updatecontrol(true);
	this.dsPjActAction.set_enableevent(true);

	this.dsFileList.set_filterstr("");
}

