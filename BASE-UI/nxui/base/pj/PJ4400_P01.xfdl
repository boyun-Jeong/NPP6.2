this.searchSubPjtInfoCallBack = function()
{
	//trace("this.searchSubPjtInfoCallBack() dsCond BTJB_ID ::: " + this.dsCond.getColumn(0, "BTJB_ID") + " SUB_PJT_ID : " + this.dsBatchJob.getColumn(0, "PJT_ID"));
	var aRow = this.divContents.form.cboSubProject.getInnerDataset().insertRow(0);
	this.divContents.form.cboSubProject.getInnerDataset().setColumn(0, "PJT_NAME", Ex.core.word("- 선택 -"));

	this.divContents.form.cboSubProject.set_displaynulltext("");

	var fRow = this.divContents.form.cboSubProject.getInnerDataset().findRow("PJT_ID", this.dsBatchJob.getColumn(0, "PJT_ID"));
	//trace("fRow:::" + fRow);
