	this.dsBatchJob.setColumn(0, "PJT_ID", this.divContents.form.cboSubProject.value);

	//trace("AF this.searchProjectBizCallBack() DS_COND BIZ_ID : " + this.dsBatchJob.getColumn(0, "BIZ_ID"));
