// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchPjtPop" :		// 검색
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "프로젝트 검색"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnAdd" : 	// 저장
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ3820_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ3820_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "요구사항 검색",
					pSelType		: "M",
					pPjtId			: this.dsBatchJob.getColumn(0, "P_PJT_ID"),
					pPjtNm			: this.dsBatchJob.getColumn(0, "P_PJT_NAME"),
					pSubPjtId		: this.dsBatchJob.getColumn(0, "PJT_ID"),
					pBizId			: this.dsBatchJob.getColumn(0, "BIZ_ID"),
					pPjtEnable		: false,
					pSubPjtEnable	: false,
					pCheckDs		: this.dsReqMap,
					pCheckCol		: "REQ_ID"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnDelete" : 	// 삭제
			this.fnDelete(this.dsReqMap);
			break;

		case "btnSave" : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

this.cboOnItemChanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
