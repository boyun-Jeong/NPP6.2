    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ0810_P01
* 화면(명)		︰ 서브 프로젝트 등록/상세조회
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.09.20
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.09.20	 WEMB			최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
this.fv_today	= "";
this.fv_oriMethodId = "";
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ0810_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ0810_P01] this.fnSearch()");
	this.dsCond.setColumn(0, "PARENT_ID", this.getOwnerFrame().pParentId);
	this.dsCond.setColumn(0, "PJT_ID", this.getOwnerFrame().pPjtId);
	
	var sTranId = "select01";									// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ0810P/select01";							// 서비스명
    var sInDs = "dsCond=dsCond";								// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjProject=dsPjProject dsBizItem=dsBizItem";	// 서버에서 수신할 데이타셋
    var sArg = "";												// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	this.dsCond.setColumn(0, "ORI_METHOD_ID", this.fv_oriMethodId);
	
	//trace("[PJ0810_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ0810P/save01";
    var sInDs = "dsCond=dsCond dsPjProject=dsPjProject:U dsBizItem=dsBizItem:U";
	var sOutDs = "dsPjProject=dsPjProject dsBizItem=dsBizItem dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 저장 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
		objDs.setDeleteRow(i);
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}
/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			
			var strInPmUserId    = this.dsPjProject.getColumn(0, "IN_PM_USER_ID");
			var strInPmUserNm    = this.dsPjProject.getColumn(0, "IN_PM_USER_NM");
			var strInPmDept      = this.dsPjProject.getColumn(0, "IN_PM_DEPT");
			this.divContents.form.divInPmUser.form.setValue(strInPmUserId, strInPmUserNm, strInPmDept);

			var strOutPmUserId   = this.dsPjProject.getColumn(0, "OUT_PM_USER_ID");
			var strOutPmUserNm   = this.dsPjProject.getColumn(0, "OUT_PM_USER_NM");
			this.divContents.form.divOutPmUser.form.setValue(strOutPmUserId, strOutPmUserNm);
		
			var strOutCpyId     = this.dsPjProject.getColumn(0, "OUT_CPY_ID");
			var strOutCpyNm     = this.dsPjProject.getColumn(0, "OUT_CPY_NAME");
			this.divContents.form.divOutCpy.form.setValue(strOutCpyId, strOutCpyNm);
			
			this.fv_oriMethodId = this.dsPjProject.getColumn(0 , "METHOD_ID");

			this.divContents.form.edtPjtName.setFocus();
			break;

		case "select02" :
			this.dsPjMethod.insertRow(0);
			this.dsPjMethod.setColumn(0, "METHOD_ID", "");
			this.dsPjMethod.setColumn(0, "METHOD_NAME", Ex.core.word("- 선택 -"));
			this.divContents.form.cboMethod.set_index(0);
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			this.divContents.form.grdMain.setCellProperty("Head", this.divContents.form.grdMain.getBindCellIndex('Body', 'CHK'), "text", "0");
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")) )
			{
				this.getOwnerFrame().pPjtId	= this.dsCond.getColumn(0, "PJT_ID");
				this.getOwnerFrame().pIsNew = "N";
			}

			var strInPmUserId    = this.dsPjProject.getColumn(0, "IN_PM_USER_ID");
			var strInPmUserNm    = this.dsPjProject.getColumn(0, "IN_PM_USER_NM");
			var strInPmDept      = this.dsPjProject.getColumn(0, "IN_PM_DEPT");
			this.divContents.form.divInPmUser.form.setValue(strInPmUserId, strInPmUserNm, strInPmDept);

			var strOutPmUserId   = this.dsPjProject.getColumn(0, "OUT_PM_USER_ID");
			var strOutPmUserNm   = this.dsPjProject.getColumn(0, "OUT_PM_USER_NM");
			this.divContents.form.divOutPmUser.form.setValue(strOutPmUserId, strOutPmUserNm);
		
			var strOutCpyId     = this.dsPjProject.getColumn(0, "OUT_CPY_ID");
			var strOutCpyNm     = this.dsPjProject.getColumn(0, "OUT_CPY_NAME");
			this.divContents.form.divOutCpy.form.setValue(strOutCpyId, strOutCpyNm);

			this.fv_oriMethodId = this.dsPjProject.getColumn(0 , "METHOD_ID");
			break;
	}
}
