// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearch" :		// 검색
			this.fnSearch();
			break;
			
		case "btnAdd" : 	// 추가
			var nRow = this.dsBizItem.setInsertRow();
			this.dsBizItem.setColumn(nRow, "PJT_ID",		this.dsPjProject.getColumn(0, "PJT_ID"));
			this.dsBizItem.setColumn(nRow, "REG_USER_ID",   Ex.util.getSession('gvUserId'));
			this.dsBizItem.setColumn(nRow, "REG_USER_NM",   Ex.util.getSession('gvUserNm'));
			this.dsBizItem.setColumn(nRow, "REG_USER_DEPT", Ex.util.getSession('gvDeptNm'));
			break;

		case "btnDelete" :	// 삭제
			this.fnDelete(this.dsBizItem);
			break;

		case "btnSave" : 	// 저장
			// validation
			var rtn = this.fnCheckValidate();
			trace("this.fnCheckValidate() ::: " + rtn);
			if( rtn )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

this.cboCanItemChange = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	if(obj.id == "cboMethod")
	{
		var mothodID = this.dsPjProject.getColumn(0, "METHOD_ID");
		
		//기존값이 null이 아닐경우
		if( !Ex.isEmpty( mothodID) )
		{
			if(e.postvalue !=  e.prevalue)
			{
				var mothodCnt = this.dsPjProject.getColumn(0, "DELIVERY_CNT");
				if( mothodCnt > 0 )
				{
					var oParam = {id : "alertMethodChkI", msg : "방법론이 변경되면 기존 산출물 목록은 초기화된 후 재적용 됩니다.", arrparam : [], msgtype : "I"};
					Ex.core.alert(this, oParam);
					return false;
				}
			}
		}
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	obj.setUpdateRow(e);
};

this.onBeforeClose = function(obj:nexacro.Form,e:nexacro.CloseEventInfo)
{
	if(!this.isCloseBtnClick) if(this.saveFlg == "Y") this.getOwnerFrame()._formObj.fnSearch();
