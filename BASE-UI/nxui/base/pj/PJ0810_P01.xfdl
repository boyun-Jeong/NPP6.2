// component init
this.fnCompInit = function()
{
	// Grid 행상태(ROWFLAG) 이미지 처리 util 호출
	this.divContents.form.grdMain.displayRowType();

	this.dsCond.setColumn(0, "PARENT_ID", this.getOwnerFrame().pParentId);
	this.dsCond.setColumn(0, "PJT_ID", this.getOwnerFrame().pPjtId);
	this.divContents.form.edtParentNm.set_value(this.getOwnerFrame().pParentNm);

	// 공통코드 조회
	this.fnGetCmmCd();
	
	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divContents.form.divGrdTopBtn.form.btnAdd];	// 쓰기
	Ex.util.setBtnVisible(this, 'write', arrBtn);
	
	arrBtn = [this.divContents.form.divGrdTopBtn.form.btnDelete];	// 삭제
	Ex.util.setBtnVisible(this, 'delete', arrBtn);
	
	arrBtn = [this.divBtnBottom.form.btnSave];						// 저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);
	
	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divContents.form.divGrdTopBtn, 'right', true);
	Ex.util.setBtnAlign(this.divBtnBottom, 'right', true);

	this.fv_today = Ex.util.today();

	//trace("[PJ0810_P01] this.fnCompInit() this.getOwnerFrame().pIsNew : " + this.getOwnerFrame().pIsNew);

	this.fn_methodCodeSearch();	//방법론Combo 
	
	if(this.getOwnerFrame().pIsNew == "Y")
	{
		var nRow = this.dsPjProject.setInsertRow();
		this.dsPjProject.setColumn(nRow, "PARENT_ID", this.getOwnerFrame().pParentId);
		this.dsPjProject.setColumn(nRow, "PJT_TYPE_CD", this.getOwnerFrame().pTypeCd);
		this.dsPjProject.setColumn(nRow, "PJT_SIZE_CD", this.getOwnerFrame().pSizeCd); 
		this.divContents.form.edtPjtName.setFocus();
	}
	else if( !Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")) )
	{
		this.fnSearch();
 	}
	
}

// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		 upCmmCd : ['YN_SHORT_CD', 'PROC_STS_CD', 'BUSI_KIND_CD', 'DEV_GRADE_CD']			// 조회할 상위코드
		,codeDiv: ['CMM_CD', 'PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD']						// 조회할 코드의 업무구분
		,optStr	: ['SEL', 'SEL', 'SEL', 'SEL']												// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn	: ['Y', 'Y', 'Y', 'Y']														// 사용여부
		,filter	: ['', '', '', '']															// filterStr
		,objDs	: [this.dsUseYn, this.dsProcStsCd, this.dsBusiKindCd, this.dsDevGradeCd]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);
}

this.fnCheckValidate = function()
{
	trace("this.fnCheckValidate()");
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsPjProject) && !Ex.util.isUpdated(this.dsBizItem))	return false;

	var nWeight = this.divContents.form.edtWeight.value;

	//현 작성중인 서브프로젝트를 제외한 가중치 합.
	var nSumWeight= "";
	if(this.getOwnerFrame().pIsNew == "Y")	nSumWeight = this.getOwnerFrame().nMainWeight;
	else									nSumWeight = this.dsPjProject.getColumn(0, "SUM_WEIGHT");

	if( Ex.isEmpty(nSumWeight) )	nSumWeight = 0;

	var nTotalWeight = new Number(nWeight) + new Number(nSumWeight);
	if( nTotalWeight > 100 )
	{
		var oParam = {id : "alertChkWeightI", msg : "메인 프로젝트 가중치 합계가 100 넘을 수 없습니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	var msgNo;
	
	//IT 담당자 입력 체크
	var rtnVal = this.divContents.form.divInPmUser.form.fnCheckValidate();
	trace("IT 담당자 rtnVal : " + rtnVal);
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkInPmUsrI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcInPmUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//수행 담당자 입력 체크
	rtnVal = this.divContents.form.divOutPmUser.form.fnCheckValidate();
	trace("수행 담당자 rtnVal : " + rtnVal);
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkOutPmUsrI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcOutPmUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//수행 담당자 업체명 입력 체크
	rtnVal = this.divContents.form.divOutCpy.form.fnCheckValidate();
	trace("수행 업체 rtnVal : " + rtnVal);
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertChkOutCpyI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcOutPmUser.text) + " - " + Ex.core.word("업체")], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	//기간체크
	if( !Ex.isEmpty(this.dsPjProject.getColumn(0, "PJT_STIME")) && !Ex.isEmpty(this.dsPjProject.getColumn(0, "PJT_ETIME")) )
	{
		if(this.dsPjProject.getColumn(0, "PJT_STIME") > this.dsPjProject.getColumn(0, "PJT_ETIME"))
		{
			var oParam = {id : "alertEdtimeI", msg : "종료일자는 시작일자보다 이후 날짜로 입력해야 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;
	if( !this.divContents.form.grdMain.checkValidate() ) return false;
	
	this.dsPjProject.setColumn(0, "SUB_PJT_YN", 'Y');
	return true;
} 

