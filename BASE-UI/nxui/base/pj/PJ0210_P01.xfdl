    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ0210_P01
* 화면(명)		︰ 프로젝트 등록/상세조회
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.09.12
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.09.12	 WEMB			최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.saveFlg = "N";				//화면 저장 여부
this.isCloseBtnClick = false;	//닫기 버튼을 눌러 닫는 경우 true
this.fv_today	= "";
this.pjtPreDate	= "";

/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo) 
{
	this.gfnFormOnLoad(this, this.fnInit);
	//trace("[PJ0210_P01] this.form_onload() this.getOwnerFrame()._formObj : " + this.getOwnerFrame()._formObj);
}

this.fnInit = function()
{
	this.fnCompInit();		// 컴포넌트 초기화	
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 조회
this.fnSearch = function()
{
	//trace("[PJ0210_P01] this.fnSearch()");
	var sTranId = "select01";											// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ0210P/select01";									// 서비스명
    var sInDs = "dsCond=dsCond";										// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsProject=dsProject dsProjectItem=dsProjectItem";		// 서버에서 수신할 데이타셋
    var sArg = "";														// 서버 @ParamVariable 인자와 맵핑됨
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

this.fnSave = function()
{
	//trace("[PJ0210_P01] this.fnSave()");
	var sTranId = "save01";
    var sService = "PJ0210P/save01";
    var sInDs = "dsCond=dsCond dsProject=dsProject:U dsProjectItem=dsProjectItem:U";
	var sOutDs = "dsProject=dsProject dsProjectItem=dsProjectItem dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
