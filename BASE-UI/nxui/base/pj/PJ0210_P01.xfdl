//개월수 계산 ( 시작일-종료일)
this.fn_monthDiff = function()
{ 
	var sDate = Ex.util.strToDate(this.divContents.form.calPjtStime.value);
	var eDate = Ex.util.strToDate(this.divContents.form.calPjtEtime.value);

	if( Ex.isEmpty(sDate) || Ex.isEmpty(eDate) ) return false;

	if( sDate > eDate )
	{
		var oParam = {id : "alertPjtDateChkI", msg : "종료일자는 착수일자보다 커야 합니다.", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	// 날자 유형 계산.
	var years  = eDate.getFullYear() - sDate.getFullYear();
	var months = eDate.getMonth()    - sDate.getMonth();
	var days   = eDate.getDate()     - sDate.getDate()+1;
	var days2  = "";

	//당월 마지막 일자 구하기
	var newDt = new Date();
	newDt.setFullYear(eDate.getFullYear(), eDate.getMonth(), eDate.getDate());
	newDt.setMonth(newDt.getMonth()+ 1 );
	newDt.setDate(0);

	//전월 마지막일자 구하기  구하기
	var newDt2 = new Date();
	newDt2.setFullYear(eDate.getFullYear(), eDate.getMonth(), eDate.getDate());
	newDt2.setMonth(newDt2.getMonth() );
	newDt2.setDate(0);

	var mmRate;
	
	//시작 일(DD)보다 종료일이(DD) 클 경우(전월 마지막일자 - 착수 일 + 종료 일+ 1일);
	if( sDate.getDate() > eDate.getDate() )
	{
		days2 = newDt2.getDate() - sDate.getDate() + eDate.getDate() + 1;
		mmRate = (days2 / newDt2.getDate() );	// 초과일 / 전월 마지막 일자
	}
	else if( eDate.getDate() >= sDate.getDate() )
	{
		//종료일자(DD)가 시작일자(DD)보다 클 경우( 종료일 - 시자일 + 1일 )
		days2 = (eDate.getDate() - sDate.getDate() )+1 ;
		mmRate = (days2 / newDt.getDate() );	// 초과일 / 당월 마지막 일자
	}
	var strMonthDiff = (years * 12) + months + (days > 0 ? 0 : -1) ;
	strMonthDiff = nexacro.round( (strMonthDiff + mmRate), 2 );
	
	this.divContents.form.edtMonthDiff.set_value(strMonthDiff);
}

