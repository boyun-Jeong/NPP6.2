// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchPjtPop" :			
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ0100_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ0100_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title : "프로젝트 검색"
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				//"width=920,height=650"	// <--- width/height/modeless(프레임 처리 옵션)							
				"autosize"	// <--- width/height/modeless(프레임 처리 옵션)
			);
			break;

		case "btnSearchReset" :		// 검색조건 초기화
			this.fnSearchReset();
			break;

		case "btnSearch" :			// 검색
			this.fnSearch();
			break;

		case "btnExcelTemplate" :	// 엑셀템플릿
			break;

		case "btnDefect" :			//결함등록
			var nChkCnt = this.dsScnrTest.getCaseCount("CHK=='1'");
			if( nChkCnt == 0 )
			{
				alert( "선택된 자료가 없습니다." ) ;
				return false;
			}
			else if( nChkCnt > 1 )
			{
				alert( "결함등록 1건만 진행 가능합니다." ) ;
				return false;	
			}
			else if( nChkCnt == 1 )
			{
				var nFindRow = this.dsScnrTest.findRow("CHK" , "1");
				this.ds_selectScnr_test.clearData();
				var nRow = this.ds_selectScnr_test.addRow();
				this.ds_selectScnr_test.copyRow(nRow, this.ds_scnr_test, nFindRow);
				
				Ex.core.popup(
					this,					// <--- 팝업 실행 스코프
					"PJ5600_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
					"pj::PJ5600_P01.xfdl",	// <--- 팝업창 오픈 Url
					{
						title		: "시나리오 결함 등록",
						pPjtId		: this.dsCond.getColumn(0, "PJT_ID"),
						pPjtNm		: this.dsCond.getColumn(0, "PJT_NM"),
						pSubPjtId	: this.dsCond.getColumn(0, "SUB_PJT_ID"),
						pStestId	: '',
						pMode		: 'NEW2'
						//pSelectDs	: this.ds_selectScnr_test
					},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
					"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
				);
			}
			break;

		case "btnExcelDown" :		// 엑셀다운
			var oInfo = {
   				Grid	: [this.grdMainList],
   				Sheet	: [this.getOwnerFrame().info.menuNm],
   				FileNm	: [this.getOwnerFrame().info.menuNm],
   				ExportType : "XLSX",
   				ExportId : "excel01"
   			};
			this.gfnExportExcel(this, oInfo);
			break;

		case "btnDelete" :			// 삭제
			this.fnDelete(this.dsScnrTest);
			break;

		case "btnConfAddRep" :		//일괄등록
			break;

		case "btnAddRep" : 			// 등록
			if(Ex.isEmpty(this.dsCond.getColumn(0, "PJT_ID")))
			{
				var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if(Ex.isEmpty(this.dsCond.getColumn(0, "SUB_PJT_ID")))
			{
				var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}

			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ5400_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ5400_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title		: "시나리오 테스트 등록",
					pPjtId		: this.dsCond.getColumn(0, "PJT_ID"),
					pPjtNm		: this.dsCond.getColumn(0, "PJT_NM"),
					pSubPjtId	: this.dsCond.getColumn(0, "SUB_PJT_ID"),
					pStestId	: '',
					pMode		: 'NEW'
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
			break;

		case "btnSave" :			//저장
			if(this.dsScnrTest.getCaseCount("ROWFLAG=='D'") > 0)
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;
	}
}

this.onKeyUp = function(obj:nexacro.Edit,e:nexacro.KeyEventInfo)
{
	if(e.keycode == 13) this.fnSearch();
};

this.cboOnItemChanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	if(obj.id == "cboSubProject")
	{
		trace("this.cboOnItemChanged() cboSubProject e.postvalue : " + e.postvalue);
		this.fnSetPjtBizCombo(e.postvalue);
	}
};

this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(e.row < 0) return;

	// PJT_NAME 프로젝트/서브프로젝트명을 누를 때 마다 POPUP화면 호출
	if (e.cell == obj.getBindCellIndex("body", "STEP_NO") || e.cell == obj.getBindCellIndex("body", "STEP_NAME"))
	{
		var strPjtId    = this.dsScnrTest.getColumn(obj.getDatasetRow(e.row), "P_PJT_ID");
		var strPjtNm	= this.dsScnrTest.getColumn(obj.getDatasetRow(e.row), "P_PJT_NAME");
		var strSubPjtId	= this.dsScnrTest.getColumn(obj.getDatasetRow(e.row), "PJT_ID");
		var strSubPjtNm	= this.dsScnrTest.getColumn(obj.getDatasetRow(e.row), "PJT_NAME");
		var strStestId	= this.dsScnrTest.getColumn(obj.getDatasetRow(e.row), "STEST_ID");

		trace("strStestId : " + strStestId);

		Ex.core.popup(
			this,					// <--- 팝업 실행 스코프
			"PJ5400_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
			"pj::PJ5400_P01.xfdl",	// <--- 팝업창 오픈 Url
			{
				title		: "시나리오 테스트 상세",
				pPjtId		: strPjtId,
				pPjtNm		: strPjtNm,
				pSubPjtId	: strSubPjtId,
				pStestId	: strStestId,
				pMode		: 'SEARCH'
			},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
			"autosize"				// <--- width/height/modeless(프레임 처리 옵션)
		);
	}
};

this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	obj.setUpdateRow(e);
