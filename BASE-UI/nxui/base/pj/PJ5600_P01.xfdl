// component init
this.fnCompInit = function()
{
	this.dsCond.setColumn(0, "STDFT_ID", this.getOwnerFrame().pStdftId);

	this.divContents.form.edtStdftNo.set_displaynulltext(Ex.core.word("자동채번"));

	// 공통코드 조회
	this.fnGetCmmCd();
	
	// 버튼&권한 mapping && visible 처리
	var arrBtn = [this.divBtnBottom.form.btnSave];	// 저장
	Ex.util.setBtnVisible(this, 'save', arrBtn);
	
	// 버튼 영역 div align 처리
	Ex.util.setBtnAlign(this.divBtnBottom, 'right', true);

	// 첨부파일 setting
	var oParam = {
		 sSvcId		: 'fileSvc'									// divFile callback 후처리용 서비스 ID
		,limitMinCnt: 0											// 파일 최소 갯수; defualt 0
		,limitCnt	: 3											// 파일 최대 갯수; default 5
		,limitSize	: 100										// 첨부파일 개당 용량 제한(단위 MB); default 100 MB
		,folderName	: 'pj'										// 폴더명(업무명)
		,tableName	: 'PJ_ST_DEFECT'							// 구분명(ex. 화면명 + "_" + 첨부파일 Div ID)
		,primaryKey1: this.dsCond.getColumn(0, "STDFT_ID")||''	// 테이블 PK1(ex. 요청서ID 등)
		,primaryKey2: ''										// 테이블 PK2 (복합 키인 경우 사용)
		,primaryKey3: ''										// 테이블 PK3 (복합 키인 경우 사용)
		,fileDownYn	: ''										// 파일다운권한; 	 공백 시 메뉴의 조회 권한
		,fileUpYn	: ''										// 파일업로드권한; 공백 시 메뉴의 추가 권한
		,deleteYn	: ''										// 파일삭제권한; 	 공백 시 메뉴의 삭제 권한
	};
	this.divContents.form.divFiles.form.setConfig(this, oParam);

	this.fv_today = Ex.util.today();

	//trace("[PJ5600_P01] this.fnCompInit() this.getOwnerFrame().pIsNew : " + this.getOwnerFrame().pIsNew);

	if(this.getOwnerFrame().pMode == "NEW")
	{
		var nRow = this.dsStDefect.setInsertRow();
		this.dsStDefect.setColumn(nRow, "P_PJT_ID",  this.getOwnerFrame().pPjtId);
		this.dsStDefect.setColumn(nRow, "P_PJT_NAME", this.getOwnerFrame().pPjtNm);
		this.dsStDefect.setColumn(nRow, "PJT_ID", this.getOwnerFrame().pSubPjtId);
		this.divContents.form.divDftUser.form.setValue(Ex.util.getSession('gvUserId'), Ex.util.getSession('gvUserNm'), Ex.util.getSession('gvDeptNm'));
		this.dsStDefect.setColumn(nRow, "STDFT_RCV_CD", "STEST_REV_CD01");
		this.dsStDefect.setColumn(nRow, "DFT_REG_DATE", Ex.util.today());
		this.divContents.form.cboBizId.setFocus();
		this.fnSetSubPjtCombo();	//업무 콤보 셋팅
		
		this.cboOnItemChanged(this.divContents.form.cboStdftRcvCd, null);
		this.cboOnItemChanged(this.divContents.form.cboStdftConfCd, null);
	}
	else if(this.getOwnerFrame().pMode == "SEARCH")
	{
		this.divContents.form.btnSearchPjtPop.set_enable(false);
		this.divContents.form.cboSubProject.set_enable(false);
		
		this.fnSearch();
 	}
}

// 공통코드 조회
this.fnGetCmmCd = function()
{
	var upcmmCd	= ['STEST_DFT_CD', 'STEST_CLS_CD', 'STEST_SVT_CD', 'STEST_TYPE_CD', 'STEST_REV_CD', 'STEST_DPRC_CD', 'STEST_CTYPE_CD'];
	var codeDiv	= ['PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD', 'PJT_MNG_CD'];
	var optStr	= ['SEL', 'SEL', 'SEL', 'SEL', 'SEL', 'SEL', 'SEL'];
	var useYn	= ['Y', 'Y', 'Y', 'Y', 'Y', 'Y', 'Y'];
	var filter	= ['', '', '', '', '', '', ''];
	var objDs	= [this.dsStdftTypeCd, this.dsStdftClassCd, this.dsStdftSeverityCd, this.dsStestTypeCd, this.dsStdftRcvCd, this.dsStdftActCd, this.dsStdftConfCd];
	
	var oParam = {
		 upCmmCd	: upcmmCd	// 조회할 상위코드
		,codeDiv	: codeDiv	// 조회할 코드의 업무구분
		,optStr		: optStr	// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn		: useYn		// 사용여부
		,filter		: filter	// filterStr
		,objDs		: objDs		// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);

	//테스트 차수
	var aRow = this.dsStestVersion.addRow();
	this.dsStestVersion.setColumn(aRow, "CODE", undefined);
	this.dsStestVersion.setColumn(aRow, "NAME", Ex.core.word("- 선택 -"));

	for(var i=1; i<=10; i++)
	{
		aRow = this.dsStestVersion.addRow();
		this.dsStestVersion.setColumn(aRow, "CODE", i);
		this.dsStestVersion.setColumn(aRow, "NAME", i + Ex.core.word("차"));
	}
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsStDefect) && !this.divContents.form.divFiles.form.isUpdateForm())
	{
		return false;
	}

	if( this.dsStDefect.getColumn(0, "UTDFT_RCV_CD") == "DFT_REV_STS_CD02" &&
		Ex.isEmpty(this.dsStDefect.getColumn(0, "UTDFT_RCV_DESC")) )
	{
		var oParam = {id : "alertChkRcvCd", msg : "접수상태가 미수용인경우 접수의견은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}

	if( !Ex.util.checkValidate(this.divContents, "input01") ) return false;

	/*
	var msgNo;

	//성명 입력 체크
	var rtnVal = this.divContents.form.divDftUser.form.fnCheckValidate();
	
	if(rtnVal == "NO_INPUT")	msgNo = 11001;	//"은(는) 필수 입력 항목입니다.";
	else if(rtnVal == "NO_ID")	msgNo = 20001;	//"은(는) 입력 후 검색까지 완료해야 합니다.";

	trace("divDevUser rtnVal : " + rtnVal);

	if(rtnVal == "NO_INPUT" || rtnVal == "NO_ID")
	{
		var oParam = {id : "alertDftUserI", msg : msgNo, arrparam : [Ex.core.word(this.divContents.form.stcDftUser.text)], msgtype : "I"};
		Ex.core.alert(this, oParam);
		return false;
	}
	*/
	
	var strRcvCd = this.dsStDefect.getColumn(0, "STDFT_RCV_CD");
	var strActCd = this.dsStDefect.getColumn(0, "STDFT_ACT_CD");
	var strConfCd = this.dsStDefect.getColumn(0, "STDFT_CONF_CD");

	//접수 상태 - 수용
	if(strRcvCd == "STEST_REV_CD02")
	{
		if(strActCd != "STEST_DPRC_CD01" && strActCd != "STEST_DPRC_CD02")
		{
			var oParam = {id : "alertStdftActCdI", msg : "접수상태가 수용인 경우 결함처리 상태는 조치중, 조치완료만 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "ACT_PDATE")) )
		{
			var oParam = {id : "alertActPDateI", msg : "접수상태가 수용인 경우 조치계획일자은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	//접수 상태 - 미수용
	if(strRcvCd == "STEST_REV_CD03")
	{
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "STDFT_RCV_DESC")) )
		{
			var oParam = {id : "alertStdftRcvDescI", msg : "접수상태가 미수용인 경우 접수의견은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		if(strActCd != "STEST_DPRC_CD03" && strActCd != "STEST_DPRC_CD04")
		{
			var oParam = {id : "alertStdftActCdI", msg : "접수상태가 미수용인 경우 결함처리 상태는 결함아님, 수용불가만 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	//결함처리 상태 - 조치중, 조치완료
	if(strActCd == "STEST_DPRC_CD01" || strActCd == "STEST_DPRC_CD02")
	{
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "ACT_USER_ID")) )
		{
			var oParam = {id : "alertDivActUserI", msg : "결함처리 상태가 조치중, 조치완료인 경우 조치자은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}
	
	//결함처리 상태 - 조치중
	if(strActCd == "STEST_DPRC_CD01")
	{
		if( !Ex.isEmpty(this.dsStDefect.getColumn(0, "ACT_ADATE")) )
		{
			var oParam = {id : "alertActADateI", msg : "결함처리 상태가 조치중인 경우 조치완료일자은(는) 입력 불가능 합니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}
	
	//결함처리 상태 - 조치완료
	if(strActCd == "STEST_DPRC_CD02")
	{
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "ACT_ADATE")) )
		{
			var oParam = {id : "alertActADateI", msg : "결함처리 상태가 조치완료인 경우 조치완료일자은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}

	//확인상태
	if( !Ex.isEmpty(strConfCd) )
	{
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "CONF_USER_ID")) )
		{
			var oParam = {id : "alertDivConfUserI", msg : "확인 상태를 입력 하려면 최종확인자은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		if( Ex.isEmpty(this.dsStDefect.getColumn(0, "CONF_DATE")) )
		{
			var oParam = {id : "alertConfDateI", msg : "확인 상태를 입력 하려면 최종확인일자은(는) 필수입력 항목입니다", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
	}
	return true;
} 

