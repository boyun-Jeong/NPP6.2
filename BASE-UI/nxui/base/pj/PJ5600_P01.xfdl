// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case "btnSearchStest" :	//시나리오 테스트 돋보기 버튼
			if( Ex.isEmpty((this.dsStDefect.getColumn(0, "P_PJT_ID"))) )
			{
				var oParam = {id : "alertNoSelPjtI", msg : "프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}

			if( Ex.isEmpty((this.dsStDefect.getColumn(0, "PJT_ID"))) )
			{
				var oParam = {id : "alertNoSelSubPjtI", msg : "서브프로젝트를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			if( Ex.isEmpty((this.dsStDefect.getColumn(0, "BIZ_ID"))) )
			{
				var oParam = {id : "alertNoSelBizIdI", msg : "업무를 선택하세요.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
			
			var strPjtId    	= this.dsStDefect.getColumn(0, "P_PJT_ID");
			var strPjtNm		= this.dsStDefect.getColumn(0, "P_PJT_NAME");
			var strSubPjtId		= this.dsStDefect.getColumn(0, "PJT_ID");
			var strSubPjtNm		= this.dsStDefect.getColumn(0, "PJT_NAME");
			var strBizId		= this.dsStDefect.getColumn(0, "BIZ_ID");
			var strRefNo		= this.dsStDefect.getColumn(0, "STEST_NO");
			var strRefNm		= this.dsStDefect.getColumn(0, "STEST_NAME");
			var strStestTypeCd	= this.dsStDefect.getColumn(0, "STEST_TYPE_CD");
			var strStestVersion	= this.dsStDefect.getColumn(0, "STEST_VERSION");
			var strStepNo		= this.dsStDefect.getColumn(0, "STEP_NO");
			var strStepNm		= this.dsStDefect.getColumn(0, "STEP_NAME");
			
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"PJ5420_P01",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ5420_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title			: Ex.core.word("시나리오 테스트 검색"),
					pSelType		: "S",
					pPjtId			: strPjtId,
					pPjtNm			: strPjtNm,
					pSubPjtId		: strSubPjtId,
					pBizId			: strBizId,
					pRefNo			: strRefNo,
					pRefNm			: strRefNm,
					pStestTypeCd	: strStestTypeCd,
					pStestVersion	: strStestVersion,
					pStepNo			: strStepNo,
					pStepNm			: strStepNm,
					pPjtEnable		: false,
					pSubPjtEnable	: false,
					pBizEnable		: false
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
			break;

		case "btnSave" : 	// 저장
			// validation
			if( this.fnCheckValidate() )
			{
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;

		case "btnClose" : 	// 닫기
			this.isCloseBtnClick = true;
			Ex.core.pclose(this, this.saveFlg);
			break;
	}
}

this.cboCanItemChange = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	if(obj.id == "cboStdftRcvCd")	//접수 상태
	{
		var strActCd = this.dsStDefect.getColumn(0, "STDFT_ACT_CD");
		var strConfCd = this.dsStDefect.getColumn(0, "STDFT_CONF_CD");
		
		if( !Ex.isEmpty(strConfCd) )
		{
			var oParam = {id : "alertStdftRcvCdI", msg : "확인상태가 입력된 경우 접수 상태는 변경 불가능 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		//결함처리 상태 - 조치중, 조치완료
		if((e.postvalue != "STEST_REV_CD02") && (strActCd == "STEST_DPRC_CD01" || strActCd == "STEST_DPRC_CD02"))
		{
			var oParam = {id : "alertStdftRcvCdI", msg : "결함처리 상태가 조치중, 조치완료인 경우 접수상태는 수용만 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		//결함처리 상태 - 결함아님, 수용불가
		if((e.postvalue != "STEST_REV_CD03") && (strActCd == "STEST_DPRC_CD03" || strActCd == "STEST_DPRC_CD04"))
		{
			var oParam = {id : "alertStdftRcvCdI", msg : "결함처리 상태가 결함아님, 수용불가인 경우 접수상태는 미수용만 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		return true;
	}
	else if(obj.id == "cboStdftActCd")	//결함처리 상태
	{
		var strRcvCd = this.dsStDefect.getColumn(0, "STDFT_RCV_CD");
		var strConfCd = this.dsStDefect.getColumn(0, "STDFT_CONF_CD");

		if( !Ex.isEmpty(strConfCd) )
		{
			var oParam = {id : "alertStdftActCdI", msg : "확인상태가 입력된 경우 결함처리 상태는 변경 불가능 합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}

		//접수 상태 - 수용
		if((strRcvCd != "STEST_REV_CD02") && (e.postvalue == "STEST_DPRC_CD01" || e.postvalue == "STEST_DPRC_CD02"))
		{
			var oParam = {id : "alertStdftActCdI", msg : "접수상태가 수용인경우만 결함처리 상태는 조치중, 조치완료 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		
		//접수 상태 - 미수용
		if((strRcvCd != "STEST_REV_CD03") && (e.postvalue == "STEST_DPRC_CD03" || e.postvalue == "STEST_DPRC_CD04"))
		{
			var oParam = {id : "alertStdftActCdI", msg : "접수상태가 미수용인경우만 결함처리 상태는 결함아님, 수용불가 선택 가능합니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return false;
		}
		return true;
	}
	else if(obj.id == "cboStdftConfCd")
	{
		if( !Ex.isEmpty(e.postvalue) )
		{
			var strRcvCd = this.dsStDefect.getColumn(0, "STDFT_RCV_CD");
			var strActCd = this.dsStDefect.getColumn(0, "STDFT_ACT_CD");
			
			//등록
			if((strRcvCd == "STEST_REV_CD01"))
			{
				var oParam = {id : "alertStdftConfCdI", msg : "접수상태가 등록인 경우 확인상태를 입력 할 수 없습니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return false;
			}
			
			//조치중
			if((strActCd == "STEST_DPRC_CD01"))
			{
				var oParam = {id : "alertStdftConfCdI", msg : "결함처리 상태가 조치중인 경우 확인상태를 입력 할 수 없습니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return false;
			}
		}
		return true;
	}
};

this.cboOnItemChanged = function(obj:nexacro.Combo,e:nexacro.ItemChangeEventInfo)
{
	trace("this.cboOnItemChanged() obj.id : " + obj.id + " obj.value : " + obj.value);
	if(obj.id == "cboStdftRcvCd")		//접수 상태
	{
		var bMode = true;
	
		//접수 상태 - 수용
		if(obj.value == "STEST_REV_CD02")	bMode = false;
		
		Ex.util.setCompPropAll(this, this.divContents.form.divActUser.form, !bMode);
		this.divContents.form.calActPdate.set_readonly(bMode);
		this.divContents.form.calActAdate.set_readonly(bMode);
		this.divContents.form.txaActDesc.set_readonly(bMode);

		if(bMode)
		{
			this.divContents.form.divActUser.form.setValue(null, null, null);
			this.dsStDefect.setColumn(0, "ACT_USER_ID",   undefined);
			this.dsStDefect.setColumn(0, "ACT_USER_NM",   undefined);
			this.dsStDefect.setColumn(0, "ACT_USER_DEPT", undefined);
			
			this.dsStDefect.setColumn(0, "ACT_PDATE", undefined);
			this.dsStDefect.setColumn(0, "ACT_ADATE", undefined);
			this.dsStDefect.setColumn(0, "ACT_DESC", undefined);
		}
		//접수상태 변경시 접수자가 없으면 로그인자로 기본 세팅(등록이나 선택이 아닐경우)
		if( (obj.value != "STEST_REV_CD01" && !Ex.isEmpty(obj.value))  )
		{
			if( Ex.isEmpty(this.dsStDefect.getColumn(0, "STDFT_RCV_USER_ID")) )
			{
				this.dsStDefect.setColumn(0, "STDFT_RCV_USER_ID",   Ex.util.getSession('gvUserId') );
				this.dsStDefect.setColumn(0, "STDFT_RCV_USER_NM",   Ex.util.getSession('gvUserNm') );
				this.dsStDefect.setColumn(0, "STDFT_RCV_USER_DEPT", Ex.util.getSession('gvDeptNm') );
				this.divContents.form.divRcvUser.form.setValue(Ex.util.getSession('gvUserId'), Ex.util.getSession('gvUserNm'), Ex.util.getSession('gvDeptNm'));
			}
		}
	}
	else if(obj.id == "cboStdftConfCd")	//확인 상태
	{
		var bMode = true;
	
		//확인 상태 - 확인완료
		if(obj.index > 0)	bMode = false;
		
		Ex.util.setCompPropAll(this, this.divContents.form.divConfUser.form, !bMode);
		this.divContents.form.calConfDate.set_readonly(bMode);
		this.divContents.form.txaRemark.set_readonly(bMode);
		
		if(bMode)
		{
			this.dsStDefect.setColumn(0, "CONF_USER_ID",   undefined);
			this.dsStDefect.setColumn(0, "CONF_USER_NM",   undefined);
