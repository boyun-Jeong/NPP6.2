{
	if( Ex.isEmpty(varValue) ) return;

	switch(pID)
	{
		case "PJ5420_P01" :
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsPopRtn.loadXML(varValue);
				this.dsStDefect.setColumn(0, "STEST_ID",       this.dsPopRtn.getColumn(0,"STEST_ID"));
				this.dsStDefect.setColumn(0, "STEST_NO",       this.dsPopRtn.getColumn(0,"STEST_NO"));
				this.dsStDefect.setColumn(0, "STEST_NAME",     this.dsPopRtn.getColumn(0,"STEST_NAME"));
				this.dsStDefect.setColumn(0, "STEST_TYPE_CD",  this.dsPopRtn.getColumn(0,"STEST_TYPE_CD"));
				this.dsStDefect.setColumn(0, "STEST_VERSION",  this.dsPopRtn.getColumn(0,"STEST_VERSION"));
				this.dsStDefect.setColumn(0, "CASE_DESC",      this.dsPopRtn.getColumn(0,"CASE_DESC"));
				this.dsStDefect.setColumn(0, "STEP_SEQ",       this.dsPopRtn.getColumn(0,"STEP_SEQ"));
				this.dsStDefect.setColumn(0, "STEP_NO",        this.dsPopRtn.getColumn(0,"STEP_NO"));
				this.dsStDefect.setColumn(0, "STEP_NAME",      this.dsPopRtn.getColumn(0,"STEP_NAME"));
			}
			break;
	}
}
