this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ5600_P01] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);

	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "fileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			
			var strDftUserId    = this.dsStDefect.getColumn(0, "DFT_USER_ID");
			var strDftUserNm    = this.dsStDefect.getColumn(0, "DFT_USER_NM");
			var strDftDept      = this.dsStDefect.getColumn(0, "DFT_USER_DEPT");
			this.divContents.form.divDftUser.form.setValue(strDftUserId, strDftUserNm, strDftDept);
			
			var strRcvUserId    = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_ID");
			var strRcvUserNm    = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_NM");
			var strRcvDept      = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_DEPT");
			this.divContents.form.divRcvUser.form.setValue(strRcvUserId, strRcvUserNm, strRcvDept);

			var strActUserId    = this.dsStDefect.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsStDefect.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsStDefect.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			var strConfUserId    = this.dsStDefect.getColumn(0, "CONF_USER_ID");
			var strConfUserNm    = this.dsStDefect.getColumn(0, "CONF_USER_NM");
			var strConfDept      = this.dsStDefect.getColumn(0, "CONF_USER_DEPT");
			this.divContents.form.divConfUser.form.setValue(strConfUserId, strConfUserNm, strConfDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅

			if( Ex.isEmpty(this.dsStDefect.getColumn(0, "STDFT_CONF_CD")) )
			{
				//활성화 처리
				this.cboOnItemChanged(this.divContents.form.cboStdftRcvCd, null);
				this.cboOnItemChanged(this.divContents.form.cboStdftConfCd, null);
			}
			else
			{
				this.fn_setReadonlyAll(true);
			}
			break;

		case "save01" :
			this.saveFlg = "Y";
			Ex.core.toast(this, "10004", null, {toastType:'S'});
			
			if( !Ex.isEmpty(this.dsCond.getColumn(0, "STDFT_ID")) )
			{
				this.getOwnerFrame().pStdftId	= this.dsCond.getColumn(0, "STDFT_ID");
				this.getOwnerFrame().pMode = "SEARCH";
				this.divContents.form.divFiles.form.fnReplaceTempkeyAndSearch(this.dsCond.getColumn(0, "STDFT_ID"));
			}
			
			var strDftUserId    = this.dsStDefect.getColumn(0, "DFT_USER_ID");
			var strDftUserNm    = this.dsStDefect.getColumn(0, "DFT_USER_NM");
			var strDftDept      = this.dsStDefect.getColumn(0, "DFT_USER_DEPT");
			this.divContents.form.divDftUser.form.setValue(strDftUserId, strDftUserNm, strDftDept);
			
			var strRcvUserId    = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_ID");
			var strRcvUserNm    = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_NM");
			var strRcvDept      = this.dsStDefect.getColumn(0, "STDFT_RCV_USER_DEPT");
			this.divContents.form.divRcvUser.form.setValue(strRcvUserId, strRcvUserNm, strRcvDept);

			var strActUserId    = this.dsStDefect.getColumn(0, "ACT_USER_ID");
			var strActUserNm    = this.dsStDefect.getColumn(0, "ACT_USER_NM");
			var strActDept      = this.dsStDefect.getColumn(0, "ACT_USER_DEPT");
			this.divContents.form.divActUser.form.setValue(strActUserId, strActUserNm, strActDept);

			var strConfUserId    = this.dsStDefect.getColumn(0, "CONF_USER_ID");
			var strConfUserNm    = this.dsStDefect.getColumn(0, "CONF_USER_NM");
			var strConfDept      = this.dsStDefect.getColumn(0, "CONF_USER_DEPT");
			this.divContents.form.divConfUser.form.setValue(strConfUserId, strConfUserNm, strConfDept);
			
			this.fnSetSubPjtCombo();	//업무 콤보 셋팅

			if( Ex.isEmpty(this.dsStDefect.getColumn(0, "STDFT_CONF_CD")) )
			{
				//활성화 처리
				this.cboOnItemChanged(this.divContents.form.cboStdftRcvCd, null);
				this.cboOnItemChanged(this.divContents.form.cboStdftConfCd, null);
			}
			else
			{
				this.fn_setReadonlyAll(true);
			}
			break;
	}
}
