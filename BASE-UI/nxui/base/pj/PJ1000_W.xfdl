 * Component Event
 ***********************************************************************************/
// 공통 onclick : component
this.fnCommOnclick = function(obj:objects, e:nexacro.ClickEventInfo)
{
	switch(obj.name)
	{
		case 'btnSearch' :	// 조회
			this.fnSearch();
			break;
			
		case 'btnSearchReset' :	// 조회조건 초기화
			this.fnSearchReset();
			break;
			
		case 'btnAdd' : 	// 권한 그리드 추가
			if(obj.parent.parent.id == "divGrdLeftBtn")
			{
				var aRow = this.dsPjMethod.setInsertRow(this.dsPjMethod.rowposition+1);
				this.dsPjMethod.setColumn(aRow, "TEMP_METHOD_ID", Ex.util.getUniqueId("METHOD_ID_"));
				this.dsPjMethod.setColumn(aRow, "USE_YN", "Y");
				
				this.grdMainList.showEditor(true);																// 그리드 편집모드로 변경
				this.grdDetailList.setCellPos(this.grdMainList.getBindCellIndex("Body", "METHOD_NAME"), aRow);
			}
			else if(obj.parent.parent.id == "divGrdRightBtn")
			{
				if( Ex.isEmpty(this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_CLASS_CD")) )
				{
					var oParam = {id : "alertNoMethodIdI", msg : "방법론-[구분]이 선택되지 않았습니다.", arrparam : [], msgtype : "I"};
					Ex.core.alert(this, oParam);
					return;
				}

				var aRow = this.dsPjMethodDlvr.setInsertRow(this.dsPjMethodDlvr.rowposition+1);
				this.dsPjMethodDlvr.setColumn(aRow, "DELIVERY_ID", Ex.util.getUniqueId("DELIVERY_ID_"));
				
				if( Ex.isEmpty(this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_ID")) )
				{
					this.dsPjMethodDlvr.setColumn(aRow, "TEMP_METHOD_ID", this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "TEMP_METHOD_ID"));
				}
				else
				{
					this.dsPjMethodDlvr.setColumn(aRow, "METHOD_ID", this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_ID"));
				}
				
				this.grdDetailList.showEditor(true);															// 그리드 편집모드로 변경
				this.grdDetailList.setCellPos(this.grdDetailList.getBindCellIndex("Body", "STAGE_CD"), aRow);
			}
			break;
		case 'btnDelete' :	// 권한 그리드 삭제
			if(obj.parent.parent.id == "divGrdLeftBtn")
			{
				this.fnDelete(this.dsPjMethod);
			}
			else if(obj.parent.parent.id == "divGrdRightBtn")
			{
				this.fnDelete(this.dsPjMethodDlvr);
			}
			break;

		case 'btnSave' : 	// 저장
			// validation
			if( this.fnCheckValidate() ) {	
				var param = {
						id : "saveConfirm"
					  , msg : "10003"
					  , arrparam : ['']
					  , msgtype : "I"};
				Ex.core.confirm(this, param);
			}
			break;
	}
}

this.grdOnCellClick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if(obj.id == "grdDetailList")
	{
		if (e.row < 0) return;

		if (e.cell == obj.getBindCellIndex("body", "FILE_IMG"))
		{
			this.fileParam.primaryKey1 = this.dsPjMethodDlvr.getColumn(this.dsPjMethodDlvr.rowposition, "METHOD_ID")||('TEMP_KEY_' + Ex.util.getSession('gvUserId'));
			this.fileParam.primaryKey2 = this.dsPjMethodDlvr.getColumn(this.dsPjMethodDlvr.rowposition, "DELIVERY_ID")||'';
			trace("this.fileParam.primaryKey1 : " + this.fileParam.primaryKey1 + "\nthis.fileParam.primaryKey2 : " + this.fileParam.primaryKey2);

			Ex.core.popup(
				this,
				'comFilePop',
				"comm::comFilePop.xfdl",             		// <--- 팝업창 오픈 Url
				{
					title			: Ex.core.word('산출물 참조 템플릿 - 첨부파일'),
					pTargetFileDiv	: this.divFiles,
					pDsObj			: this.dsFileList,
					pIsSearch		: false,
					pBtnConfVisible	: true,
					pFileCfg		: this.fileParam,
					pFilterStr		: "PK1=='" + this.fileParam.primaryKey1 + "' && PK2=='" + this.fileParam.primaryKey2 + "'"
				},											// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize,bCloseButton=false"				// <--- width/height/modeless(프레임 처리 옵션)
			);
		}
	}
	obj.dropdownCombo();
	obj.dropdownCalendar();
};

// 공통 oncloumnchanged event : dataset
this.dsOnColumnChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSColChangeEventInfo)
{
	if(obj.id == "dsPjMethod")
	{
		if(e.columnid == "METHOD_NAME")
		{
			this.setGrdDetailTitle(e.row);
		}
		obj.setUpdateRow(e);
	}
	else if(obj.id == "dsPjMethodDlvr")	obj.setUpdateRow(e, ["ORG_FILE_NM"]);
};

this.dsCanRowposChange = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	if(obj.id == "dsPjMethod")
	{
		if( Ex.util.isUpdated(this.dsPjMethodDlvr) )
		{
			if( !this.rowChangeFlag ) {
				this.nSelRow = e.newrow;		// 이동하려는 행 저장
				var param = {
							id : "rowposChangeConfirm"
						  , msg : "10001"
						  , arrparam : []
						  , msgtype : "I"};
				Ex.core.confirm(this, param);
				return false;
			}
		}
	}
};

this.dsOnRowposChanged = function(obj:nexacro.NormalDataset,e:nexacro.DSRowPosChangeEventInfo)
{
	trace("this.dsOnRowposChanged() obj.id : " + obj.id + " e.oldrow : " + e.oldrow + " e.newrow : " + e.newrow);
	
	if(obj.id == "dsPjMethod")
	{
		this.dsPjMethodDlvr.clearData();

		var methodId = this.dsPjMethod.getColumn(e.newrow, 'METHOD_ID');

		this.setGrdDetailTitle(e.newrow);

		this.dsCond.setColumn(this.dsCond.rowposition, 'METHOD_ID', methodId);
		this.fnSearchD();

		this.rowChangeFlag = false;		// 행변경 flag 초기화
		this.nSelRow = -1;				// 이동하려는 행 초기화
	}
};]]></Script>
