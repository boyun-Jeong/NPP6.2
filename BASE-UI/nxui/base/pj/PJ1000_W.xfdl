    <Script type="xscript5.1"><![CDATA[/***********************************************************************************
* 화면(ID)		︰ PJ1000_W
* 화면(명)		︰ 방법론/산출물정의
* 메뉴(경로)	︰ 프로젝트관리 > 표준관리 > 방법론/산출물정의
* 화면 설명	︰ 
* 작성자		︰ WEMB
* 작성일		︰ 2023.10.16
* 수정이력		︰
*------------------------------------------------------------------
* 수정일        작성자		이력
*------------------------------------------------------------------
* 2023.10.16	 WEMB		최초작성
*------------------------------------------------------------------
***********************************************************************************/

/***********************************************************************************
 * Script Include 
 ***********************************************************************************/

/***********************************************************************************
 * Form Variable
 ***********************************************************************************/
this.nSelRow = -1;				// 이동하려는 행
this.rowChangeFlag = false;		// true: 행변경 가능
this.dSaveChkRow;				//저장시 Validaton Check에 걸린경우 해당 row정보를 저장
this.fileParam = {
	 sSvcId		: 'fileSvc'			// divFile callback 후처리용 서비스 ID
	,limitMinCnt: 0					// 파일 최소 갯수; defualt 0
	,limitCnt	: 3					// 파일 최대 갯수; default 5
	,limitSize	: 100				// 첨부파일 개당 용량 제한(단위 MB); default 100 MB
	,folderName	: 'pj'				// 폴더명(업무명)
	,tableName	: 'PJ_METHOD_DLVR'	// 구분명(ex. 화면명 + "_" + 첨부파일 Div ID)
	,primaryKey1: ''				// 테이블 PK1(ex. 요청서ID 등)
	,primaryKey2: ''				// 테이블 PK2 (복합 키인 경우 사용)
	,primaryKey3: ''				// 테이블 PK3 (복합 키인 경우 사용)
	,fileDownYn	: ''				// 파일다운권한; 	 공백 시 메뉴의 조회 권한
	,fileUpYn	: ''				// 파일업로드권한; 공백 시 메뉴의 추가 권한
	,deleteYn	: ''				// 파일삭제권한; 	 공백 시 메뉴의 삭제 권한
};
/***********************************************************************************
 * Form Function
 ***********************************************************************************/
this.form_onload = function(obj:Form, e:nexacro.LoadEventInfo)
{
	// 업무화면 laod 전 공통 처리를 위한 함수. 
	// 업무단 소스는 fnInit을 onload로 취급하고 fnInit에 기술할 것.
	this.gfnFormOnLoad(this, this.fnInit);
}

this.fnInit = function()
{	
	this.fnCompInit();		// 컴포넌트 초기화
}

/***********************************************************************************
 * Common Function
 ***********************************************************************************/
// 검색조건 초기화
this.fnSearchReset = function()
{
	this.dsCond.clearData();
	this.dsCond.addRow();
}

// 변경 행 확인
this.fnSearch = function()
{
	if( Ex.util.isUpdated(this.dsPjMethod) ||
		Ex.util.isUpdated(this.dsPjMethodDlvr) )
	{
		var param = {
						id : "searchConfirm"
					  , msg : "10001"
					  , arrparam : []
					  , msgtype : "I"};
		Ex.core.confirm(this, param);
		return;
	}
	this.fnSearchM();
}

// 조회
this.fnSearchM = function()
{
	// transaction
	var sTranId = "select01";				// transaction 서비스 실행 ID / 콜백에서 수신할 서비스ID
    var sService = "PJ1000W/select01";		// 서비스명
    var sInDs = "";							// 서버로 전송할 데이타셋 @ParamDataSet 인자와 맵핑됨
    var sOutDs = "dsPjMethod=dsPjMethod";	// 서버에서 수신할 데이타셋
    var sArg = "";                   		// 서버 @ParamVariable 인자와 맵핑됨
    
    /**
	 * 기본문법
	 * this : transaction 실행스코프 (필수)
	 */
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}
 
// 선택 권한별 사용자 목록 조회
this.fnSearchD = function()
{
	var sTranId = "select02";
    var sService = "PJ1000W/select02";
    var sInDs = "dsCond=dsCond";
    var sOutDs = "dsPjMethodDlvr=dsPjMethodDlvr dsFileList=dsFileList";
    var sArg = "";
    
	Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 저장 func.
this.fnSave = function()
{
	this.dsPjMethod.set_enableevent(false);
	
	var sTranId = "save01";
    var sService = "PJ1000W/save01";
    var sInDs = "dsPjMethod=dsPjMethod:U dsPjMethodDlvr=dsPjMethodDlvr:U dsCond=dsCond";
    var sOutDs = "dsPjMethod=dsPjMethod dsPjMethodDlvr=dsPjMethodDlvr dsFileList=dsFileList dsCond=dsCond";
    var sArg = "";
    Ex.core.tran(this, sTranId, sService, sInDs, sOutDs, sArg);
}

// 공통 삭제 func. ROWFLAG가 'I'면 행 삭제, 'U'/''면 'D'로 변경, 'D'면 원복
this.fnDelete = function(objDs:Dataset)
{
	objDs.set_enableevent(false);
	objDs.set_filterstr("CHK=='1'");
	
	for(var i = objDs.rowcount-1; i >= 0; i--)
	{
		objDs.setDeleteRow(i);
	}
		
	objDs.set_filterstr("");
	objDs.set_enableevent(true);
}

/***********************************************************************************
 * CallBack Event 
 ***********************************************************************************/
this.fnUploadCallback = function(sSvcId, nErrorCode, sErrorMsg)
{
	//trace("[PJ1000_W] this.fnUploadCallback() sSvcId : " + sSvcId + " nErrorCode : " + nErrorCode + " sErrorMsg : " + sErrorMsg);

	// TO DO
	// 개발자가 지정한 sSvcId(setConfig 옵션에 넣은 파일첨부ID), nErrorCode(에러코드), sErrorMsg(에러메시지)
	if (nErrorCode < 0) 
	{
		Ex.core.error(this, {id:"ERROR", msg:sSvcId + "\n" + Ex.core.msg(sErrorMsg)});
		return;
	}
	
	switch(sSvcId)
	{
		case "fileSvc" :
			this.fnSave();
			break;
	}
};

// Transaction 디폴트 콜백함수
this.fnCallback = function (sSvcId, nErrorCode, sErrorMsg)
{
	// 에러메세지 처리
	if(sSvcId == "save01")	this.dsPjMethod.set_enableevent(true);
	
	if (nErrorCode < 0) 
	{
		var param = {id : "ERROR", msg : sSvcId+"::"+nErrorCode+"::"+sErrorMsg};
		Ex.core.error(this, param);
		return;
	}
			
	// 각 Transaction별 CallBack 처리
	switch (sSvcId)
	{
		case "select01" :
			Ex.core.toast(this, "10002");
			this.grdMainList.setCellProperty("Head", this.grdMainList.getBindCellIndex('Body', 'CHK'), "text", "0");
			break;

		case "select02" :
			this.grdDetailList.setCellProperty("Head", this.grdDetailList.getBindCellIndex('Body', 'CHK'), "text", "0");
			this.fileParam.primaryKey1 = this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_ID") || '';
			this.divFiles.form.setConfig(this, this.fileParam);
			this.divFiles.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));
			break;

		case "save01" :
			Ex.core.toast(this, "10004");
			this.grdMainList.setCellProperty("Head", this.grdMainList.getBindCellIndex('Body', 'CHK'), "text", "0");
			this.grdDetailList.setCellProperty("Head", this.grdDetailList.getBindCellIndex('Body', 'CHK'), "text", "0");

			var fRow = this.dsPjMethod.findRow("METHOD_ID", this.dsCond.getColumn(0, "METHOD_ID"));
			if(this.dsPjMethod.rowcount > 0)	this.dsPjMethod.set_rowposition(fRow > -1 ? fRow : 0);
			break;	
	}
}
// 팝업 디폴트 콜백함수
this.fnPopupAfter = function(pID,varValue)
{
	// TO DO
	// 개발자가 지정한 pID(팝업창ID) , varValue(팝업창 반환값)
	switch(pID)
	{
		case "comFilePop" :
			//trace("varValue:\n" + varValue);
			if(varValue.indexOf("Dataset id=") > -1)
			{
				this.dsFileList.loadXML(varValue);
				
				for(var i=0; i<this.dsFileList.rowcount; i++)	this.dsFileList.setColumn(i, "CHK", "0");

				this.divFiles.form.dsFommFile.set_enableevent(false);
				this.divFiles.form.dsFommFile.set_updatecontrol(false);
				
				this.divFiles.form.dsFommFile.loadXML(this.dsFileList.saveXML("dsFommFile", "ALL"));
				for(var i=0; i<this.divFiles.form.dsFommFile.rowcount; i++)
				{
					var rowFlag = this.divFiles.form.dsFommFile.getColumn(i, "ROWFLAG");
					trace("i=" + i + " rowFlag : " + rowFlag);

					if(rowFlag == "I")
					{
						this.divFiles.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_INSERT);
					}
					else if(rowFlag == "D")
					{
						this.divFiles.form.dsFommFile.setRowType(i, Dataset.ROWTYPE_UPDATE);
