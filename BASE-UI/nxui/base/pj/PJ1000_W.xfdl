				else
				{
					this.dsPjMethodDlvr.setColumn(this.dsPjMethodDlvr.rowposition, "ORG_FILE_NM", undefined);
				}
				//trace(this.divFilesAct.form.dsFommFile.saveXML());
			}
			break;
	}
}
// alert/confirm/error 메시지창 디폴트 콜백함수
this.fnMsgAfter = function(mID,varValue)
{
	// TO DO
	// 개발자가 지정한 mID(메시지창ID) , varValue(메시지창 반환값)
	// alert/error : 무조건 true 반환
	// confirm     : true/false 선택적 반환
	if(!varValue)
		return;
	
	switch(mID)
	{
		case "searchConfirm" :
			this.fnSearchM();
			break;
			
		case "saveConfirm" :
			this.rowChangeFlag = true;						// 행변경 flag
			for(var i=this.dsFileList.rowcount-1; i>=0; i--)
			{
				var pk2 = this.dsFileList.getColumn(i, "PK2");
				var fRow = this.dsPjMethodDlvr.findRowExpr("DELIVERY_ID", pk2);
				
				if(fRow == -1)
				{
					this.divFiles.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
					this.dsFileList.setDeleteRow(i);
				}
				else
				{
					if(this.dsPjMethodDlvr.getColumn(fRow, "ROWFLAG") == "D")
					{
						this.divFiles.form.FileUpTransfer.removeFile(this.dsFileList.getColumn(i, "FILE_NM"));
						this.dsFileList.setDeleteRow(i);
					}
				}
			}
			this.divFiles.form.fnUploadFiles(true);
			break;
		
		case "rowposChangeConfirm" :						// 행변경 confirm
			this.rowChangeFlag  = true;
			this.dsPjMethod.set_rowposition(this.nSelRow);
			break;

		case "alertChkStageCdI" :
			this.grdDetailList.getBindDataset().set_rowposition(this.dSaveChkRow);				// 데이터셋 ROW 포커스 이동
			this.grdDetailList.showEditor(true);												// 그리드 편집모드로 변경
			var cellIdx = this.grdDetailList.getBindCellIndex("Body", "STAGE_CD");
			this.grdDetailList.setCellPos(cellIdx, this.dSaveChkRow);							// 그리드 셀 포커스 이동
			break;

		case "alertChkActDateI" :
			this.grdMainList.getBindDataset().set_rowposition(this.dSaveChkRow);				// 데이터셋 ROW 포커스 이동
			this.grdMainList.showEditor(true);													// 그리드 편집모드로 변경
			var cellIdx = this.grdMainList.getBindCellIndex("Body", "ACT_STIME");
			this.grdMainList.setCellPos(cellIdx, this.dSaveChkRow);								// 그리드 셀 포커스 이동
			break;
	}
}

