// 공통코드 조회
this.fnGetCmmCd = function()
{
	var oParam = {
		upCmmCd : ['USE_CD',		'RPT_MAKE_CHRG_CD',		'CLASS_STAGE_CD',		'MNG_STAGE_CD',		'MNG_STG_CD_USR']	// 조회할 상위코드
		,codeDiv: ['CMM_CD',		'PJT_MNG_CD',			'PJT_MNG_CD',			'PJT_MNG_CD',		'PJT_MNG_CD']		// 조회할 코드의 업무구분
		,optStr	: ['SEL',			'SEL',					'SEL',					'SEL',				'SEL']				// ALL: '- 전체 -'; SEL: '- 선택 -'
		,useYn	: ['Y',				'Y',					'Y',					'Y',				'Y']				// 사용여부
		,filter	: ['',				'',						'',						'',					'']					// filterStr
		,objDs	: [this.dsUseYn,	this.dsChargeCd,		this.dsMethodClassCd,	this.dsStageCd,		this.dsStageCd2]	// copy dataset
	}
	Ex.util.getCmmCode(oParam);	// 공통코드 getter util; (oParam);

	this.dsGradeEsncl.insertRow(0);
	this.dsGradeEsncl.setColumn(0, "NAME", Ex.core.word("- 선택 -"));
	//this.dsStageCdAll.appendData(this.dsStageCd);
	//this.dsStageCdAll.appendData(this.dsStageCd2);
}

this.fnCheckValidate = function()
{
	// 저장할 dataset에 변경된 data 있는 지 확인
	if( !Ex.util.isUpdated(this.dsPjMethod) &&
		!Ex.util.isUpdated(this.dsPjMethodDlvr) &&
		!this.divFiles.form.isUpdateForm())
	{
		return false;
	}

	// Grid Validation
	if( !this.grdMainList.checkValidate() )	return false;

	for(var i=0; i<this.dsPjMethod.rowcount ; i++)
	{
		var strActStime = this.dsPjMethod.getColumn(i, "ACT_STIME");
		var strActEtime = this.dsPjMethod.getColumn(i, "ACT_ETIME");
		
		if( !Ex.isEmpty(strActStime) && !Ex.isEmpty(strActEtime) )
		{
			if (strActStime > strActEtime)
			{
				this.dSaveChkRow = i;
				var oParam = {id : "alertChkActDateI", msg : "적용시작일은 적용종료일보다\n같거나 작아야 합니다.", arrparam : [], msgtype : "I"};
				Ex.core.alert(this, oParam);
				return;
			}
		}
	}

	if( !this.grdDetailList.checkValidate() )	return false;
	
	var chkDsObj;
	
	if(this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_CLASS_CD") == "CLASS_STAGE_CD01")
	{
		chkDsObj = this.dsStageCd;
	}
	else if(this.dsPjMethod.getColumn(this.dsPjMethod.rowposition, "METHOD_CLASS_CD") == "CLASS_STAGE_CD02")
	{
		chkDsObj = this.dsStageCd2;
	}
	
	for(var i=0; i<this.dsPjMethodDlvr.rowcount; i++)
	{
		var fRow = chkDsObj.findRow("CMM_CD", this.dsPjMethodDlvr.getColumn(i, "STAGE_CD"));
		
		trace("this.fnCheckValidate() i=" + i + " fRow : " + fRow);

		if(fRow == -1)
		{
			this.dSaveChkRow = i;
			var oParam = {id : "alertChkStageCdI", msg : "산출물리스트-[단계]가 올바르게 지정되지 않았습니다.", arrparam : [], msgtype : "I"};
			Ex.core.alert(this, oParam);
			return;
		}
	}
	return true;
}

this.setGrdDetailTitle = function(rowIdx)
{
	var methodId		= this.dsPjMethod.getColumn(rowIdx, 'METHOD_ID');
	var methodNm		= this.dsPjMethod.getColumn(rowIdx, 'METHOD_NAME')||'';
	var methodClassCd	= this.dsPjMethod.getColumn(rowIdx, "METHOD_CLASS_CD");

	// Grid title 처리
	var leftTitle = Ex.isEmpty(methodNm) ? '' : '[' + methodNm + '] ';
	this.stcRightGrdTitle.set_text(leftTitle + Ex.core.word('산출물 리스트') );

	// cnt 위치 조정
	var objSize = nexacro.getTextSize(this.stcRightGrdTitle.text, "bold 14px 'NanumSquareR'");
	var nWidth = parseInt(objSize.nx) + 25;
	this.stcRightGrdTitle.set_width(nWidth);

	this.stcRightCnt.set_left(this.stcRightGrdTitle.getOffsetLeft() + nWidth + 5);
}

