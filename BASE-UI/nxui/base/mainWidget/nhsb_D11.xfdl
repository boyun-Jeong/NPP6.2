      <BindItem id="item2" compid="divRight.form.divCnt01.form.stcCnt" propid="text" datasetid="dsYearlyCnt" columnid="TOT"/>
      <BindItem id="item3" compid="divRight.form.divCnt02.form.stcCnt" propid="text" datasetid="dsYearlyCnt" columnid="FINISH"/>
      <BindItem id="item4" compid="divRight.form.divCnt03.form.stcCnt" propid="text" datasetid="dsSatisCnt" columnid="COUNT_ALL"/>
      <BindItem id="item5" compid="divRight.form.divCnt04.form.stcCnt" propid="text" datasetid="dsSatisCnt" columnid="COUNT_NOT"/>
