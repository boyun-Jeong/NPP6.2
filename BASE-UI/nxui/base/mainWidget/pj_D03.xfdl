this.grdData_oncellclick = function(obj:nexacro.Grid,e:nexacro.GridClickEventInfo)
{
	if( e.cell == obj.getBindCellIndex('body', 'TITLE') ) {
		var ds = obj.getBindDataset();
		var gubun		= ds.getColumn(e.row, "GUBUN");
		var PJT_ID 		= ds.getColumn(e.row, "PJT_ID");
		var PJT_NAME	= ds.getColumn(e.row, "PJT_NAME");
		var SUB_ID		= ds.getColumn(e.row, "SUB_ID");
	
		if(gubun == "위험") 
		{
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"riskPop",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ3100_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title	: "위험관리 상세",
					pPjtId	: PJT_ID,
					pPjtNm  : PJT_NAME,
					PMode	: 'SEARCH',
					pRiskId	: SUB_ID
				},						// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"				// <--- width/height/modeless(프레임 처리 옵션)
			);
			return;
		} 
		else if(gubun == "이슈") 
		{
			Ex.core.popup(
				this,					// <--- 팝업 실행 스코프
				"issuePop",			// <--- 팝업창 아이디(반드시 영문으로 입력 처리)
				"pj::PJ3200_P01.xfdl",	// <--- 팝업창 오픈 Url
				{
					title		: "이슈관리 상세",
					pPjtId		: PJT_ID,
					pPjtNm		: PJT_NAME,
					pIssueId	: SUB_ID,
					pMode		: 'SEARCH'
				},							// <--- title및 파라미터(p1,p2는 화면에서 사용할 파라미터 등)
				"autosize"					// <--- width/height/modeless(프레임 처리 옵션)							
			);
		}
	}
