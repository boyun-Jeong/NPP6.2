						for (String name : selectObj.get(i).keySet()) {

							if (name.indexOf("_MapMeta_") > -1) { // 조회된 데이터가 없는 경우는 MapMeta가 붙어서 넘어온다.
								String dsNm = name.replaceAll("_MapMeta_", "");
								// log.debug("gubun : [" + gubun + "] dsNm ::::: " + dsNm);
								DataSet ds = new DataSet(dsNm);
